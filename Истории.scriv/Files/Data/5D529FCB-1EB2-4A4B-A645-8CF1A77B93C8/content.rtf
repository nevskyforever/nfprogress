\par\pard\plain \sa160\sl259\slmult1\ql\ltrch\loch {\f2\fs24\b0\i0 \loch\af2\hich\af2\dbch\af2\uc1\u1058\'D2\u1077\'E5\u1087\'EF\u1077\'E5\u1088\'F0\u1100\'FC \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1080\'E8 \u1085\'ED\u1077\'E5 \u1091\'F3\u1074\'E2\u1083\'EB\u1077\'E5\u1082\'EA\u1072\'E0\u1102\'FE\u1090\'F2 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1090\'F2\u1086\'EE\u1095\'F7\u1085\'ED\u1086\'EE \u1090\'F2\u1072\'E0\u1082\'EA \u1078\'E6\u1077\'E5, \u1082\'EA\u1072\'E0\u1082\'EA \u1088\'F0\u1072\'E0\u1085\'ED\u1100\'FC\u1096\'F8\u1077\'E5, \u1084\'EC\u1086\'EE\u1081\'E9 \u1074\'E2\u1082\'EA\u1091\'F3\u1089\'F1 \u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB \u1073\'E1\u1086\'EE\u1083\'EB\u1077\'E5\u1077\'E5 \u1080\'E8\u1079\'E7\u1099\'FB\u1089\'F1\u1082\'EA\u1072\'E0\u1085\'ED\u1085\'ED\u1099\'FB\u1084\'EC \u1080\'E8 \u1090\'F2\u1088\'F0\u1077\'E5\u1073\'E1\u1086\'EE\u1074\'E2\u1072\'E0\u1090\'F2\u1077\'E5\u1083\'EB\u1100\'FC\u1085\'ED\u1099\'FB\u1084\'EC \u1082\'EA \u1083\'EB\u1080\'E8\u1090\'F2\u1077\'E5\u1088\'F0\u1072\'E0\u1090\'F2\u1091\'F3\u1088\'F0\u1077\'E5, \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1091\'F3\u1102\'FE \u1103\'FF \u1095\'F7\u1080\'E8\u1090\'F2\u1072\'E0\u1102\'FE. \u1053\'CD\u1086\'EE \u1101\'FD\u1090\'F2\u1086\'EE \u1085\'ED\u1077\'E5 \u1079\'E7\u1085\'ED\u1072\'E0\u1095\'F7\u1080\'E8\u1090\'F2, \u1095\'F7\u1090\'F2\u1086\'EE \u1086\'EE\u1085\'ED\u1072\'E0 \u1090\'F2\u1072\'E0\u1082\'EA\u1072\'E0\u1103\'FF \u1078\'E6\u1077\'E5 \u1080\'E8\u1079\'E7\u1099\'FB\u1089\'F1\u1082\'EA\u1072\'E0\u1085\'ED\u1085\'ED\u1072\'E0\u1103\'FF, \u1082\'EA\u1072\'E0\u1082\'EA \u1084\'EC\u1086\'EE\u1081\'E9 \u1074\'E2\u1082\'EA\u1091\'F3\u1089\'F1.}
\par\plain {\f2\fs24\b0\i0 \loch\af2\hich\af2\dbch\af2\uc1\u1055\'CF\u1077\'E5\u1088\'F0\u1074\'E2\u1086\'EE\u1077\'E5 \u1084\'EC\u1077\'E5\u1089\'F1\u1090\'F2\u1086\'EE: \line \u1053\'CD\u1072\'E0\u1090\'F2\u1072\'E0\u1096\'F8\u1072\'E0 \u1050\'CA\u1072\'E0\u1084\'EC\u1087\'EF\u1091\'F3\u1096\'F8 \endash  \u171\'AB3096 \u1076\'E4\u1085\'ED\u1077\'E5\u1081\'E9\u187\'BB.}
\par\plain {\f2\fs24\b0\i0 \loch\af2\hich\af2\dbch\af2\uc1\u1069\'DD\u1090\'F2\u1072\'E0 \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1072\'E0 \u1073\'E1\u1099\'FB\u1083\'EB\u1072\'E0 \u1086\'EE\u1089\'F1\u1085\'ED\u1086\'EE\u1074\'E2\u1072\'E0\u1085\'ED\u1072\'E0 \u1085\'ED\u1072\'E0 \u1088\'F0\u1077\'E5\u1072\'E0\u1083\'EB\u1100\'FC\u1085\'ED\u1099\'FB\u1093\'F5 \u1089\'F1\u1086\'EE\u1073\'E1\u1099\'FB\u1090\'F2\u1080\'E8\u1103\'FF\u1093\'F5 \u1074\'E2 \u1078\'E6\u1080\'E8\u1079\'E7\u1085\'ED\u1080\'E8 \u1072\'E0\u1074\'E2\u1090\'F2\u1086\'EE\u1088\'F0\u1072\'E0. \u1042\'C2 \u1102\'FE\u1085\'ED\u1086\'EE\u1084\'EC \u1074\'E2\u1086\'EE\u1079\'E7\u1088\'F0\u1072\'E0\u1089\'F1\u1090\'F2\u1077\'E5 \u1077\'E5\u1105\'B8 \u1087\'EF\u1086\'EE\u1093\'F5\u1080\'E8\u1090\'F2\u1080\'E8\u1083\'EB \u1084\'EC\u1091\'F3\u1078\'E6\u1095\'F7\u1080\'E8\u1085\'ED\u1072\'E0 \u1080\'E8 \u1077\'E5\u1081\'E9 \u1087\'EF\u1088\'F0\u1080\'E8\u1096\'F8\u1083\'EB\u1086\'EE\u1089\'F1\u1100\'FC \u1087\'EF\u1088\'F0\u1086\'EE\u1074\'E2\u1077\'E5\u1089\'F1\u1090\'F2\u1080\'E8 \u1087\'EF\u1086\'EE\u1095\'F7\u1090\'F2\u1080\'E8 8,5 \u1083\'EB\u1077\'E5\u1090\'F2 \u1074\'E2 \u1079\'E7\u1072\'E0\u1090\'F2\u1086\'EE\u1095\'F7\u1077\'E5\u1085\'ED\u1080\'E8\u1080\'E8 \u1080\'E8 \u1084\'EC\u1072\'E0\u1083\'EB\u1077\'E5\u1085\'ED\u1100\'FC\u1082\'EA\u1086\'EE\u1081\'E9 \u1082\'EA\u1086\'EE\u1084\'EC\u1085\'ED\u1072\'E0\u1090\'F2\u1082\'EA\u1077\'E5, \u1075\'E3\u1076\'E4\u1077\'E5 \u1074\'E2\u1083\'EB\u1072\'E0\u1089\'F1\u1090\'F2\u1077\'E5\u1085\'ED \u1073\'E1\u1099\'FB\u1083\'EB \u1083\'EB\u1080\'E8\u1096\'F8\u1100\'FC \u1086\'EE\u1076\'E4\u1080\'E8\u1085\'ED \u1095\'F7\u1077\'E5\u1083\'EB\u1086\'EE\u1074\'E2\u1077\'E5\u1082\'EA. \u1054\'CE\u1085\'ED\u1072\'E0 \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1076\'E4\u1072\'E0\u1083\'EB\u1072\'E0, \u1089\'F1\u1080\'E8\u1076\'E4\u1077\'E5\u1083\'EB\u1072\'E0 \u1074\'E2 \u1090\'F2\u1077\'E5\u1084\'EC\u1085\'ED\u1086\'EE\u1090\'F2\u1077\'E5 \u1095\'F7\u1072\'E0\u1089\'F1\u1072\'E0\u1084\'EC\u1080\'E8, \u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1076\'E4\u1072\'E0\u1083\'EB\u1072\'E0 \u1086\'EE\u1090\'F2 \u1089\'F1\u1082\'EA\u1091\'F3\u1082\'EA\u1080\'E8 \u1080\'E8 \u1074\'E2\u1089\'F1\u1087\'EF\u1086\'EE\u1084\'EC\u1080\'E8\u1085\'ED\u1072\'E0\u1083\'EB\u1072\'E0 \u1089\'F1\u1074\'E2\u1086\'EE\u1102\'FE \u1073\'E1\u1077\'E5\u1079\'E7\u1079\'E7\u1072\'E0\u1073\'E1\u1086\'EE\u1090\'F2\u1085\'ED\u1091\'F3\u1102\'FE \u1078\'E6\u1080\'E8\u1079\'E7\u1085\'ED\u1100\'FC. \u1061\'D5\u1086\'EE\u1090\'F2\u1100\'FC \u1087\'EF\u1086\'EE \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1077\'E5 \u1073\'E1\u1099\'FB\u1083\'EB \u1089\'F1\u1085\'ED\u1103\'FF\u1090\'F2 \u1086\'EE\u1076\'E4\u1085\'ED\u1086\'EE\u1080\'E8\u1084\'EC\u1077\'E5\u1085\'ED\u1085\'ED\u1099\'FB\u1081\'E9 \u1092\'F4\u1080\'E8\u1083\'EB\u1100\'FC\u1084\'EC, \u1086\'EE\u1085\'ED \u1085\'ED\u1077\'E5 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1076\'E4\u1072\'E0\u1089\'F1\u1090\'F2 \u1074\'E2\u1089\'F1\u1077\'E5\u1093\'F5 \u1090\'F2\u1077\'E5\u1093\'F5 \u1089\'F1\u1083\'EB\u1086\'EE\u1074\'E2 \u1080\'E8 \u1101\'FD\u1084\'EC\u1086\'EE\u1094\'F6\u1080\'E8\u1081\'E9, \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1099\'FB\u1077\'E5 \u1080\'E8\u1089\'F1\u1087\'EF\u1099\'FB\u1090\'F2\u1072\'E0\u1083\'EB\u1072\'E0 \u1076\'E4\u1077\'E5\u1074\'E2\u1091\'F3\u1096\'F8\u1082\'EA\u1072\'E0, \u1086\'EE\u1085\'ED \u1085\'ED\u1077\'E5 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1076\'E4\u1072\'E0\u1089\'F1\u1090\'F2 \u1074\'E2\u1089\'F1\u1077\'E5\u1093\'F5 \u1077\'E5\u1105\'B8 \u1074\'E2\u1086\'EE\u1089\'F1\u1087\'EF\u1086\'EE\u1084\'EC\u1080\'E8\u1085\'ED\u1072\'E0\u1085\'ED\u1080\'E8\u1081\'E9 \u1080\'E8 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1078\'E6\u1080\'E8\u1074\'E2\u1072\'E0\u1085\'ED\u1080\'E8\u1081\'E9.\line \u1052\'CC\u1085\'ED\u1077\'E5 \u1073\'E1\u1099\'FB\u1083\'EB\u1086\'EE \u1085\'ED\u1077\'E5\u1074\'E2\u1077\'E5\u1088\'F0\u1086\'EE\u1103\'FF\u1090\'F2\u1085\'ED\u1086\'EE \u1080\'E8\u1085\'ED\u1090\'F2\u1077\'E5\u1088\'F0\u1077\'E5\u1089\'F1\u1085\'ED\u1086\'EE \u1091\'F3\u1079\'E7\u1085\'ED\u1072\'E0\u1090\'F2\u1100\'FC \u1074\'E2\u1089\'F1\u1077\'E5, \u1095\'F7\u1090\'F2\u1086\'EE \u1089\'F1 \u1085\'ED\u1077\'E5\u1081\'E9 \u1087\'EF\u1088\'F0\u1086\'EE\u1080\'E8\u1089\'F1\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1083\'EB\u1086\'EE, \u1085\'ED\u1077\'E5\u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1103\'FF \u1085\'ED\u1072\'E0 \u1090\'F2\u1086\'EE, \u1095\'F7\u1090\'F2\u1086\'EE \u1103\'FF \u1087\'EF\u1086\'EE\u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1077\'E5\u1083\'EB \u1092\'F4\u1080\'E8\u1083\'EB\u1100\'FC\u1084\'EC \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1076\'E4 \u1090\'F2\u1077\'E5\u1084\'EC, \u1082\'EA\u1072\'E0\u1082\'EA \u1087\'EF\u1088\'F0\u1086\'EE\u1095\'F7\u1077\'E5\u1089\'F1\u1090\'F2\u1100\'FC \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1091\'F3. \u1071\'DF \u1087\'EF\u1086\'EE\u1088\'F0\u1072\'E0\u1078\'E6\u1072\'E0\u1083\'EB\u1089\'F1\u1103\'FF \u1090\'F2\u1086\'EE\u1084\'EC\u1091\'F3, \u1082\'EA\u1072\'E0\u1082\'EA \u1086\'EE\u1085\'ED\u1072\'E0 \u1074\'E2\u1099\'FB\u1076\'E4\u1077\'E5\u1088\'F0\u1078\'E6\u1080\'E8\u1074\'E2\u1072\'E0\u1083\'EB\u1072\'E0 \u1101\'FD\u1090\'F2\u1086\'EE \u1074\'E2\u1089\'F1\u1077\'E5, \u1082\'EA\u1072\'E0\u1082\'EA \u1086\'EE\u1085\'ED\u1072\'E0 \u1085\'ED\u1077\'E5 \u1089\'F1\u1086\'EE\u1096\'F8\u1083\'EB\u1072\'E0 \u1089\'F1 \u1091\'F3\u1084\'EC\u1072\'E0 \u1086\'EE\u1090\'F2 \u1087\'EF\u1086\'EE\u1085\'ED\u1080\'E8\u1084\'EC\u1072\'E0\u1085\'ED\u1080\'E8\u1103\'FF \u1090\'F2\u1086\'EE\u1075\'E3\u1086\'EE, \u1089\'F1 }{\f3\fs24\b0\i1 \loch\af3\hich\af3\dbch\af3\uc1\u1082\'EA\u1072\'E0\u1082\'EA\u1080\'E8\u1084\'EC }{\f2\fs24\b0\i0 \loch\af2\hich\af2\dbch\af2\uc1\u1095\'F7\u1077\'E5\u1083\'EB\u1086\'EE\u1074\'E2\u1077\'E5\u1082\'EA\u1086\'EE\u1084\'EC \u1077\'E5\u1081\'E9 \u1087\'EF\u1088\'F0\u1080\'E8\u1076\'E4\u1077\'E5\u1090\'F2\u1089\'F1\u1103\'FF \u1087\'EF\u1088\'F0\u1086\'EE\u1074\'E2\u1077\'E5\u1089\'F1\u1090\'F2\u1080\'E8 \u1078\'E6\u1080\'E8\u1079\'E7\u1085\'ED\u1100\'FC.\line \u1069\'DD\u1090\'F2\u1072\'E0 \u1080\'E8\u1089\'F1\u1090\'F2\u1086\'EE\u1088\'F0\u1080\'E8\u1103\'FF \u1079\'E7\u1072\'E0\u1089\'F1\u1090\'F2\u1072\'E0\u1074\'E2\u1083\'EB\u1103\'FF\u1077\'E5\u1090\'F2 \u1094\'F6\u1077\'E5\u1085\'ED\u1080\'E8\u1090\'F2\u1100\'FC \u1089\'F1\u1086\'EE\u1073\'E1\u1089\'F1\u1090\'F2\u1074\'E2\u1077\'E5\u1085\'ED\u1085\'ED\u1091\'F3\u1102\'FE \u1078\'E6\u1080\'E8\u1079\'E7\u1085\'ED\u1100\'FC \u1080\'E8 \u1074\'E2\u1089\'F1\u1077\'E5, \u1095\'F7\u1090\'F2\u1086\'EE \u1074\'E2 \u1085\'ED\u1077\'E5\u1081\'E9 \u1087\'EF\u1088\'F0\u1086\'EE\u1080\'E8\u1089\'F1\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1090\'F2, \u1076\'E4\u1072\'E0\u1078\'E6\u1077\'E5 \u1087\'EF\u1083\'EB\u1086\'EE\u1093\'F5\u1086\'EE\u1077\'E5. \u1040\'C0 \u1077\'E5\u1097\'F9\u1105\'B8 \u1103\'FF \u1087\'EF\u1086\'EE\u1085\'ED\u1103\'FF\u1083\'EB, \u1085\'ED\u1072\'E0\u1089\'F1\u1082\'EA\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1073\'E1\u1099\'FB\u1074\'E2\u1072\'E0\u1077\'E5\u1090\'F2 \u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1085\'ED\u1085\'ED\u1099\'FB\u1084\'EC \u1086\'EE\u1073\'E1\u1097\'F9\u1077\'E5\u1089\'F1\u1090\'F2\u1074\'E2\u1086\'EE, \u1074\'E2\u1077\'E5\u1076\'E4\u1100\'FC \u1077\'E5\u1075\'E3\u1086\'EE \u1088\'F0\u1077\'E5\u1072\'E0\u1082\'EA\u1094\'F6\u1080\'E8\u1103\'FF \u1085\'ED\u1072\'E0 \u171\'AB\u1086\'EE\u1090\'F2\u1082\'EA\u1088\'F0\u1086\'EE\u1074\'E2\u1077\'E5\u1085\'ED\u1080\'E8\u1077\'E5\u187\'BB? \u1073\'E1\u1099\'FB\u1083\'EB\u1072\'E0 \u1089\'F1\u1086\'EE\u1074\'E2\u1089\'F1\u1077\'E5\u1084\'EC \u1085\'ED\u1077\'E5\u1090\'F2\u1080\'E8\u1087\'EF\u1080\'E8\u1095\'F7\u1085\'ED\u1086\'EE\u1081\'E9 \u1076\'E4\u1083\'EB\u1103\'FF \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1080\'E8, \u1074\'E2\u1086\'EE\u1079\'E7\u1084\'EC\u1086\'EE\u1078\'E6\u1085\'ED\u1086\'EE, \u1076\'E4\u1083\'EB\u1103\'FF \u1074\'E2\u1089\'F1\u1077\'E5\u1093\'F5 \u1086\'EE\u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB\u1100\'FC\u1085\'ED\u1099\'FB\u1093\'F5.}
\par\plain {\f2\fs24\b0\i0 \loch\af2\hich\af2\dbch\af2\uc1\u1042\'C2\u1090\'F2\u1086\'EE\u1088\'F0\u1086\'EE\u1077\'E5 \u1084\'EC\u1077\'E5\u1089\'F1\u1090\'F2\u1086\'EE:\line \u1057\'D1\u1100\'FC\u1102\'FE\u1079\'E7\u1077\'E5\u1085\'ED \u1050\'CA\u1086\'EE\u1083\'EB\u1083\'EB\u1080\'E8\u1085\'ED\u1079\'E7 \endash  \u171\'AB\u1043\'C3\u1086\'EE\u1083\'EB\u1086\'EE\u1076\'E4\u1085\'ED\u1099\'FB\u1077\'E5 \u1048\'C8\u1075\'E3\u1088\'F0\u1099\'FB\u187\'BB.}
\par\plain {\f2\fs24\b0\i0 \loch\af2\hich\af2\dbch\af2\uc1\u1042\'C2 \u1094\'F6\u1077\'E5\u1085\'ED\u1090\'F2\u1088\'F0\u1077\'E5 \u1089\'F1\u1102\'FE\u1078\'E6\u1077\'E5\u1090\'F2\u1072\'E0 \u1076\'E4\u1077\'E5\u1074\'E2\u1091\'F3\u1096\'F8\u1082\'EA\u1072\'E0 \u1087\'EF\u1086\'EE \u1080\'E8\u1084\'EC\u1077\'E5\u1085\'ED\u1080\'E8 \u1050\'CA\u1080\'E8\u1090\'F2\u1085\'ED\u1080\'E8\u1089\'F1\u1089\'F1, \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1072\'E0\u1103\'FF \u1087\'EF\u1086\'EE\u1078\'E6\u1077\'E5\u1088\'F0\u1090\'F2\u1074\'E2\u1086\'EE\u1074\'E2\u1072\'E0\u1083\'EB\u1072\'E0 \u1089\'F1\u1086\'EE\u1073\'E1\u1086\'EE\u1081\'E9 \u1088\'F0\u1072\'E0\u1076\'E4\u1080\'E8 \u1084\'EC\u1072\'E0\u1083\'EB\u1077\'E5\u1085\'ED\u1100\'FC\u1082\'EA\u1086\'EE\u1081\'E9 \u1089\'F1\u1077\'E5\u1089\'F1\u1090\'F2\u1088\'F0\u1099\'FB \u1085\'ED\u1072\'E0 \u171\'AB\u1078\'E6\u1072\'E0\u1090\'F2\u1074\'E2\u1077\'E5\u187\'BB. \u171\'AB\u1046\'C6\u1072\'E0\u1090\'F2\u1074\'E2\u1091\'F3\u187\'BB \u1082\'EA\u1072\'E0\u1078\'E6\u1076\'E4\u1099\'FB\u1081\'E9 \u1075\'E3\u1086\'EE\u1076\'E4 \u1091\'F3\u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1080\'E8\u1074\'E2\u1072\'E0\u1077\'E5\u1090\'F2 \u1073\'E1\u1077\'E5\u1079\'E7\u1078\'E6\u1072\'E0\u1083\'EB\u1086\'EE\u1089\'F1\u1090\'F2\u1085\'ED\u1086\'EE\u1077\'E5 \u1087\'EF\u1088\'F0\u1072\'E0\u1074\'E2\u1080\'E8\u1090\'F2\u1077\'E5\u1083\'EB\u1100\'FC\u1089\'F1\u1090\'F2\u1074\'E2\u1086\'EE \u1055\'CF\u1072\'E0\u1085\'ED\u1077\'E5\u1084\'EC\u1072\'E0, \u1082\'EA\u1072\'E0\u1078\'E6\u1076\'E4\u1099\'FB\u1081\'E9 \u1088\'F0\u1072\'E0\u1079\'E7 \u1076\'E4\u1086\'EE\u1082\'EA\u1072\'E0\u1079\'E7\u1099\'FB\u1074\'E2\u1072\'E0\u1103\'FF, \u1095\'F7\u1090\'F2\u1086\'EE \u1073\'E1\u1091\'F3\u1085\'ED\u1090\'F2\u1086\'EE\u1074\'E2\u1072\'E0\u1090\'F2\u1100\'FC \u1087\'EF\u1088\'F0\u1086\'EE\u1090\'F2\u1080\'E8\u1074\'E2 \u1074\'E2\u1083\'EB\u1072\'E0\u1089\'F1\u1090\'F2\u1080\'E8 \endash  \u1089\'F1\u1077\'E5\u1073\'E1\u1077\'E5 \u1076\'E4\u1086\'EE\u1088\'F0\u1086\'EE\u1078\'E6\u1077\'E5. \u1062\'D6\u1077\'E5\u1083\'EB\u1100\'FC \u171\'AB\u1048\'C8\u1075\'E3\u1088\'F0\u187\'BB \endash  \u1072\'E0\u1073\'E1\u1089\'F1\u1086\'EE\u1083\'EB\u1102\'FE\u1090\'F2\u1085\'ED\u1086\'EE \u1083\'EB\u1102\'FE\u1073\'E1\u1099\'FB\u1084\'EC \u1089\'F1\u1087\'EF\u1086\'EE\u1089\'F1\u1086\'EE\u1073\'E1\u1086\'EE\u1084\'EC \u1074\'E2\u1099\'FB\u1078\'E6\u1080\'E8\u1090\'F2\u1100\'FC \u1086\'EE\u1076\'E4\u1085\'ED\u1086\'EE\u1084\'EC\u1091\'F3 \u1091\'F3\u1095\'F7\u1072\'E0\u1089\'F1\u1090\'F2\u1085\'ED\u1080\'E8\u1082\'EA\u1091\'F3 \u1080\'E8\u1079\'E7 24-\u1077\'E5\u1093\'F5, \u1087\'EF\u1086\'EE \u1076\'E4\u1077\'E5\u1074\'E2\u1091\'F3\u1096\'F8\u1082\'EA\u1077\'E5 \u1080\'E8 \u1087\'EF\u1072\'E0\u1088\'F0\u1085\'ED\u1102\'FE \u1086\'EE\u1090\'F2 \u1082\'EA\u1072\'E0\u1078\'E6\u1076\'E4\u1086\'EE\u1075\'E3\u1086\'EE \u1076\'E4\u1080\'E8\u1089\'F1\u1090\'F2\u1088\'F0\u1080\'E8\u1082\'EA\u1090\'F2\u1072\'E0 (\u1088\'F0\u1077\'E5\u1075\'E3\u1080\'E8\u1086\'EE\u1085\'ED\u1072\'E0).\line \u1043\'C3\u1086\'EE\u1083\'EB\u1086\'EE\u1076\'E4\u1085\'ED\u1099\'FB\u1077\'E5 \u1080\'E8\u1075\'E3\u1088\'F0\u1099\'FB \u1101\'FD\u1090\'F2\u1086\'EE \u1090\'F2\u1086\'EE, \u1095\'F7\u1090\'F2\u1086\'EE \u1086\'EE\u1090\'F2 \u1085\'ED\u1072\'E0\u1087\'EF\u1088\'F0\u1103\'FF\u1078\'E6\u1077\'E5\u1085\'ED\u1080\'E8\u1103\'FF \u1079\'E7\u1072\'E0\u1089\'F1\u1090\'F2\u1072\'E0\u1074\'E2\u1083\'EB\u1103\'FF\u1077\'E5\u1090\'F2 \u1090\'F2\u1077\'E5\u1073\'E1\u1103\'FF \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1083\'EB\u1080\'E8\u1089\'F1\u1090\'F2\u1099\'FB\u1074\'E2\u1072\'E0\u1090\'F2\u1100\'FC \u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1085\'ED\u1080\'E8\u1094\'F6\u1091\'F3 \u1079\'E7\u1072\'E0 \u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1085\'ED\u1080\'E8\u1094\'F6\u1077\'E5\u1081\'E9, \u1085\'ED\u1077\'E5 \u1079\'E7\u1072\'E0\u1084\'EC\u1077\'E5\u1095\'F7\u1072\'E0\u1103\'FF \u1074\'E2\u1088\'F0\u1077\'E5\u1084\'EC\u1077\'E5\u1085\'ED\u1080\'E8, \u1076\'E4\u1072\'E0\u1078\'E6\u1077\'E5 \u1077\'E5\u1089\'F1\u1083\'EB\u1080\'E8 \u1090\'F2\u1099\'FB \u1079\'E7\u1085\'ED\u1072\'E0\u1077\'E5\u1096\'F8\u1100\'FC \u1082\'EA\u1072\'E0\u1082\'EA \u1074\'E2\u1089\'F1\u1077\'E5 \u1079\'E7\u1072\'E0\u1082\'EA\u1086\'EE\u1085\'ED\u1095\'F7\u1080\'E8\u1083\'EB\u1086\'EE\u1089\'F1\u1100\'FC. \u1050\'CA\u1072\'E0\u1082\'EA \u1080\'E8 \u1089\'F1 \u1087\'EF\u1088\'F0\u1086\'EE\u1096\'F8\u1083\'EB\u1086\'EE\u1081\'E9 \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1086\'EE\u1081\'E9, \u1103\'FF \u1089\'F1\u1085\'ED\u1072\'E0\u1095\'F7\u1072\'E0\u1083\'EB\u1072\'E0 \u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1077\'E5\u1083\'EB \u1074\'E2\u1089\'F1\u1077\'E5 \u1092\'F4\u1080\'E8\u1083\'EB\u1100\'FC\u1084\'EC\u1099\'FB \u1080\'E8 \u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1087\'EF\u1086\'EE\u1090\'F2\u1086\'EE\u1084\'EC \u1091\'F3\u1078\'E6\u1077\'E5 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1096\'F8\u1077\'E5\u1083\'EB \u1082\'EA \u1073\'E1\u1091\'F3\u1084\'EC\u1072\'E0\u1078\'E6\u1085\'ED\u1086\'EE\u1084\'EC \u1074\'E2\u1072\'E0\u1088\'F0\u1080\'E8\u1072\'E0\u1085\'ED\u1090\'F2\u1091\'F3 \u1074\'E2\u1089\'F1\u1077\'E5\u1081\'E9 \u1090\'F2\u1088\'F0\u1080\'E8\u1083\'EB\u1086\'EE\u1075\'E3\u1080\'E8\u1080\'E8, \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1099\'FB\u1081\'E9 \u1076\'E4\u1086\'EE\u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB\u1089\'F1\u1103\'FF \u1084\'EC\u1085\'ED\u1077\'E5 \u1073\'E1\u1086\'EE\u1083\'EB\u1100\'FC\u1096\'F8\u1080\'E8\u1084\'EC \u1090\'F2\u1088\'F0\u1091\'F3\u1076\'E4\u1086\'EE\u1084\'EC \u1080\'E8 \u1103\'FF \u1086\'EE\u1073\'E1 \u1101\'FD\u1090\'F2\u1086\'EE\u1084\'EC \u1085\'ED\u1077\'E5 \u1087\'EF\u1086\'EE\u1078\'E6\u1072\'E0\u1083\'EB\u1077\'E5\u1083\'EB. \u1050\'CA\u1072\'E0\u1078\'E6\u1076\'E4\u1072\'E0\u1103\'FF \u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1085\'ED\u1080\'E8\u1094\'F6\u1072\'E0 \u1073\'E1\u1099\'FB\u1083\'EB\u1072\'E0 \u1087\'EF\u1086\'EE\u1083\'EB\u1085\'ED\u1072\'E0 \u1085\'ED\u1072\'E0\u1087\'EF\u1088\'F0\u1103\'FF\u1078\'E6\u1077\'E5\u1085\'ED\u1080\'E8\u1103\'FF, \u1072\'E0 \u1082\'EA\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0 \u1074\'E2\u1089\'F1\u1077\'E5 \u1089\'F1\u1090\'F2\u1072\'E0\u1085\'ED\u1086\'EE\u1074\'E2\u1080\'E8\u1083\'EB\u1086\'EE\u1089\'F1\u1100\'FC \u1090\'F2\u1080\'E8\u1096\'F8\u1077\'E5, \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1085\'ED\u1077\'E5 \u1087\'EF\u1086\'EE\u1082\'EA\u1080\'E8\u1076\'E4\u1072\'E0\u1083\'EB\u1072\'E0 \u1084\'EC\u1099\'FB\u1089\'F1\u1083\'EB\u1100\'FC \u1086\'EE \u1090\'F2\u1086\'EE\u1084\'EC, \u1095\'F7\u1090\'F2\u1086\'EE \u1087\'EF\u1088\'F0\u1103\'FF\u1084\'EC\u1086\'EE \u1085\'ED\u1072\'E0 \u1089\'F1\u1083\'EB\u1077\'E5\u1076\'E4\u1091\'F3\u1102\'FE\u1097\'F9\u1077\'E5\u1081\'E9 \u1089\'F1\u1090\'F2\u1088\'F0\u1086\'EE\u1082\'EA\u1077\'E5 \u1095\'F7\u1090\'F2\u1086\'EE-\u1090\'F2\u1086\'EE \u1087\'EF\u1088\'F0\u1086\'EE\u1080\'E8\u1079\'E7\u1086\'EE\u1081\'E9\u1076\'E4\u1077\'E5\u1090\'F2.\line \u1052\'CC\u1085\'ED\u1086\'EE\u1075\'E3\u1080\'E8\u1077\'E5 \u1075\'E3\u1086\'EE\u1074\'E2\u1086\'EE\u1088\'F0\u1103\'FF\u1090\'F2, \u1095\'F7\u1090\'F2\u1086\'EE \u1101\'FD\u1090\'F2\u1086\'EE \u1085\'ED\u1091\'F3\u1076\'E4\u1103\'FF\u1090\'F2\u1080\'E8\u1085\'ED\u1072\'E0, \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1072\'E0\u1103\'FF \u1087\'EF\u1086\'EE\u1083\'EB\u1085\'ED\u1072\'E0 \u1087\'EF\u1086\'EE\u1076\'E4\u1088\'F0\u1086\'EE\u1089\'F1\u1090\'F2\u1082\'EA\u1086\'EE\u1074\'E2\u1099\'FB\u1093\'F5 \u1089\'F1\u1086\'EE\u1087\'EF\u1083\'EB\u1077\'E5\u1081\'E9, \u1085\'ED\u1086\'EE \u1103\'FF \u1089\'F1\u1095\'F7\u1080\'E8\u1090\'F2\u1072\'E0\u1102\'FE, \u1095\'F7\u1090\'F2\u1086\'EE \u1101\'FD\u1090\'F2\u1086\'EE \u1085\'ED\u1077\'E5 \u1090\'F2\u1072\'E0\u1082\'EA. \u1042\'C2 \u1101\'FD\u1090\'F2\u1086\'EE\u1081\'E9 \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1077\'E5 \u1077\'E5\u1089\'F1\u1090\'F2\u1100\'FC \u1072\'E0\u1076\'E4 \u1095\'F7\u1077\'E5\u1084\'EC \u1087\'EF\u1086\'EE\u1076\'E4\u1091\'F3\u1084\'EC\u1072\'E0\u1090\'F2\u1100\'FC \u1080\'E8 \u1077\'E5\u1105\'B8 \u1087\'EF\u1080\'E8\u1089\'F1\u1072\'E0\u1083\'EB\u1072\'E0 \u1076\'E4\u1072\'E0\u1083\'EB\u1077\'E5\u1082\'EA\u1086\'EE \u1085\'ED\u1077\'E5 \u1084\'EC\u1072\'E0\u1083\'EB\u1077\'E5\u1085\'ED\u1100\'FC\u1082\'EA\u1072\'E0\u1103\'FF \u1076\'E4\u1077\'E5\u1074\'E2\u1086\'EE\u1095\'F7\u1082\'EA\u1072\'E0. \u1085\'ED\u1077\'E5\u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1099\'FB\u1077\'E5 \u1074\'E2\u1077\'E5\u1097\'F9\u1080\'E8 \u1073\'E1\u1099\'FB\u1083\'EB\u1080\'E8 \u1085\'ED\u1072\'E0\u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u171\'AB\u1078\'E6\u1080\'E8\u1079\'E7\u1085\'ED\u1077\'E5\u1085\'ED\u1085\'ED\u1099\'FB\u1084\'EC\u1080\'E8\u187\'BB, \u1095\'F7\u1090\'F2\u1086\'EE \u1076\'E4\u1091\'F3\u1096\'F8\u1072\'E0 \u1088\'F0\u1072\'E0\u1076\'E4\u1086\'EE\u1074\'E2\u1072\'E0\u1083\'EB\u1072\'E0\u1089\'F1\u1100\'FC. \u1053\'CD\u1072\'E0\u1087\'EF\u1088\'F0\u1080\'E8\u1084\'EC\u1077\'E5\u1088\'F0: \u171\'AB\u1048\'C8 \u1087\'EF\u1091\'F3\u1089\'F1\u1090\'F2\u1100\'FC \u1091\'F3\u1076\'E4\u1072\'E0\u1095\'F7\u1072\'E0 \u1074\'E2\u1089\'F1\u1077\'E5\u1075\'E3\u1076\'E4\u1072\'E0 \u1073\'E1\u1091\'F3\u1076\'E4\u1077\'E5\u1090\'F2 \u1085\'ED\u1072\'E0 \u1074\'E2\u1072\'E0\u1096\'F8\u1077\'E5\u1081\'E9 \u1089\'F1\u1090\'F2\u1086\'EE\u1088\'F0\u1086\'EE\u1085\'ED\u1077\'E5!\u187\'BB \endash  \u1090\'F2\u1072\'E0 \u1089\'F1\u1072\'E0\u1084\'EC\u1072\'E0\u1103\'FF \u1092\'F4\u1088\'F0\u1072\'E0\u1079\'E7\u1072\'E0, \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1091\'F3\u1102\'FE \u1103\'FF \u1090\'F2\u1077\'E5\u1087\'EF\u1077\'E5\u1088\'F0\u1100\'FC \u1080\'E8\u1089\'F1\u1087\'EF\u1086\'EE\u1083\'EB\u1100\'FC\u1079\'E7\u1091\'F3\u1102\'FE \u1074\'E2 \u1082\'EA\u1072\'E0\u1095\'F7\u1077\'E5\u1089\'F1\u1090\'F2\u1074\'E2\u1077\'E5 \u1080\'E8\u1088\'F0\u1086\'EE\u1085\'ED\u1080\'E8\u1080\'E8 \u1086\'EE \u1089\'F1\u1074\'E2\u1086\'EE\u1077\'E5\u1084\'EC \u1085\'ED\u1077\'E5\u1074\'E2\u1077\'E5\u1079\'E7\u1077\'E5\u1085\'ED\u1080\'E8\u1080\'E8.\line \u1042\'C2\u1072\'E0\u1084\'EC \u1089\'F1\u1090\'F2\u1086\'EE\u1080\'E8\u1090\'F2 \u1087\'EF\u1088\'F0\u1086\'EE\u1095\'F7\u1080\'E8\u1090\'F2\u1072\'E0\u1090\'F2\u1100\'FC \u1101\'FD\u1090\'F2\u1091\'F3 \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1091\'F3, \u1076\'E4\u1072\'E0\u1078\'E6\u1077\'E5 \u1077\'E5\u1089\'F1\u1083\'EB\u1080\'E8 \u1074\'E2\u1099\'FB \u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1077\'E5\u1083\'EB\u1080\'E8 \u1082\'EA\u1080\'E8\u1085\'ED\u1086\'EE\u1083\'EB\u1077\'E5\u1085\'ED\u1090\'F2\u1099\'FB, \u1087\'EF\u1086\'EE\u1090\'F2\u1086\'EE\u1084\'EC\u1091\'F3 \u1095\'F7\u1090\'F2\u1086\'EE \u1086\'EE\u1085\'ED\u1072\'E0 \u1085\'ED\u1072\'E0\u1087\'EF\u1088\'F0\u1103\'FF\u1075\'E3\u1072\'E0\u1077\'E5\u1090\'F2 \u1085\'ED\u1077\'E5 \u1084\'EC\u1077\'E5\u1085\'ED\u1100\'FC\u1096\'F8\u1077\'E5, \u1095\'F7\u1077\'E5\u1084\'EC \u1092\'F4\u1080\'E8\u1083\'EB\u1100\'FC\u1084\'EC\u1099\'FB, \u1072\'E0 \u1090\'F2\u1072\'E0\u1082\'EA \u1078\'E6\u1077\'E5 \u1088\'F0\u1072\'E0\u1089\'F1\u1082\'EA\u1088\'F0\u1099\'FB\u1074\'E2\u1072\'E0\u1077\'E5\u1090\'F2 \u1080\'E8\u1089\'F1\u1090\'F2\u1080\'E8\u1085\'ED\u1085\'ED\u1099\'FB\u1077\'E5 \u1095\'F7\u1091\'F3\u1074\'E2\u1089\'F1\u1090\'F2\u1074\'E2\u1072\'E0 \u1080\'E8 \u1101\'FD\u1084\'EC\u1086\'EE\u1094\'F6\u1080\'E8\u1080\'E8 \u1075\'E3\u1083\'EB\u1072\'E0\u1074\'E2\u1085\'ED\u1086\'EE\u1081\'E9 \u1075\'E3\u1077\'E5\u1088\'F0\u1086\'EE\u1080\'E8\u1085\'ED\u1080\'E8, \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1099\'FB\u1077\'E5, \u1072\'E0 \u1084\'EC\u1086\'EE\u1081\'E9 \u1074\'E2\u1079\'E7\u1075\'E3\u1083\'EB\u1103\'FF\u1076\'E4 \u1085\'ED\u1077\'E5 \u1073\'E1\u1099\'FB\u1083\'EB\u1080\'E8 \u1086\'EE\u1090\'F2\u1088\'F0\u1072\'E0\u1078\'E6\u1077\'E5\u1085\'ED\u1099\'FB \u1074\'E2 \u1082\'EA\u1080\'E8\u1085\'ED\u1086\'EE.}
\par\plain {\f2\fs24\b0\i0 \loch\af2\hich\af2\dbch\af2\uc1\u1058\'D2\u1088\'F0\u1077\'E5\u1090\'F2\u1100\'FC\u1077\'E5 \u1084\'EC\u1077\'E5\u1089\'F1\u1090\'F2\u1086\'EE:\line \u1040\'C0\u1083\'EB\u1072\'E0\u1085\'ED \u1043\'C3\u1083\'EB\u1080\'E8\u1085\'ED\u1085\'ED \endash  \u171\'AB\u1054\'CE\u1073\'E1\u1083\'EB\u1072\'E0\u1089\'F1\u1090\'F2\u1080\'E8 \u1090\'F2\u1100\'FC\u1084\'EC\u1099\'FB\u187\'BB.}
\par\plain {\f2\fs24\b0\i0 \loch\af2\hich\af2\dbch\af2\uc1\u1054\'CE\u1076\'E4\u1085\'ED\u1072\'E0 \u1090\'F2\u1072\'E0\u1073\'E1\u1083\'EB\u1077\'E5\u1090\'F2\u1082\'EA\u1072\'E0 \u1084\'EC\u1086\'EE\u1078\'E6\u1077\'E5\u1090\'F2 \u1080\'E8\u1079\'E7\u1084\'EC\u1077\'E5\u1085\'ED\u1080\'E8\u1090\'F2\u1100\'FC \u1074\'E2\u1072\'E0\u1096\'F8\u1091\'F3 \u1078\'E6\u1080\'E8\u1079\'E7\u1085\'ED\u1100\'FC \u1089\'F1 \u1090\'F2\u1086\'EE\u1095\'F7\u1085\'ED\u1086\'EE\u1089\'F1\u1090\'F2\u1100\'FC\u1102\'FE \u1076\'E4\u1086\'EE \u1085\'ED\u1072\'E0\u1086\'EE\u1073\'E1\u1086\'EE\u1088\'F0\u1086\'EE\u1090\'F2. \u1042\'C2\u1099\'FB \u1087\'EF\u1086\'EE\u1074\'E2\u1077\'E5\u1088\'F0\u1080\'E8\u1090\'F2\u1077\'E5 \u1074\'E2 \u1101\'FD\u1090\'F2\u1086\'EE, \u1077\'E5\u1089\'F1\u1083\'EB\u1080\'E8 \u1087\'EF\u1088\'F0\u1086\'EE\u1095\'F7\u1080\'E8\u1090\'F2\u1072\'E0\u1077\'E5\u1090\'F2\u1077\'E5 \u171\'AB\u1054\'CE\u1073\'E1\u1083\'EB\u1072\'E0\u1089\'F1\u1090\'F2\u1080\'E8 \u1090\'F2\u1100\'FC\u1084\'EC\u1099\'FB\u187\'BB. \u1044\'C4\u1072\'E0, \u1101\'FD\u1090\'F2\u1086\'EE \u1086\'EE\u1095\'F7\u1077\'E5\u1088\'F0\u1077\'E5\u1076\'E4\u1085\'ED\u1072\'E0\u1103\'FF \u1101\'FD\u1082\'EA\u1088\'F0\u1072\'E0\u1085\'ED\u1080\'E8\u1079\'E7\u1080\'E8\u1088\'F0\u1086\'EE\u1074\'E2\u1072\'E0\u1085\'ED\u1085\'ED\u1072\'E0\u1103\'FF \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1072\'E0, \u1085\'ED\u1086\'EE \u1101\'FD\u1082\'EA\u1088\'F0\u1072\'E0\u1085\'ED\u1080\'E8\u1079\'E7\u1072\'E0\u1094\'F6\u1080\'E8\u1103\'FF \u1086\'EE\u1082\'EA\u1072\'E0\u1079\'E7\u1072\'E0\u1083\'EB\u1072\'E0\u1089\'F1\u1100\'FC \u1085\'ED\u1077\'E5 \u1085\'ED\u1072\'E0\u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1093\'F5\u1086\'EE\u1088\'F0\u1086\'EE\u1096\'F8\u1072\'E0, \u1082\'EA\u1072\'E0\u1082\'EA \u1093\'F5\u1086\'EE\u1090\'F2\u1077\'E5\u1083\'EB\u1086\'EE\u1089\'F1\u1100\'FC \u1073\'E1\u1099\'FB, \u1090\'F2\u1072\'E0\u1084\'EC \u1074\'E2\u1089\'F1\u1077\'E5 \u1089\'F1\u1082\'EA\u1086\'EE\u1088\'F0\u1077\'E5\u1077\'E5 \u1088\'F0\u1072\'E0\u1076\'E4\u1080\'E8 \u171\'AB\u1074\'E2\u1072\'E0\u1091\'F3\u187\'BB \u1080\'E8 \u1082\'EA\u1072\'E0\u1089\'F1\u1089\'F1\u1099\'FB, \u1095\'F7\u1077\'E5\u1084\'EC \u1076\'E4\u1083\'EB\u1103\'FF \u1101\'FD\u1082\'EA\u1088\'F0\u1072\'E0\u1085\'ED\u1080\'E8\u1079\'E7\u1072\'E0\u1094\'F6\u1080\'E8\u1080\'E8.\line \u1069\'DD\u1090\'F2\u1072\'E0 \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1072\'E0 \u1086\'EE \u1095\'F7\u1077\'E5\u1083\'EB\u1086\'EE\u1074\'E2\u1077\'E5\u1082\'EA\u1077\'E5, \u1091\'F3 \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1086\'EE\u1075\'E3\u1086\'EE \u1085\'ED\u1077\'E5 \u1074\'E2\u1089\'F1\u1077\'E5 \u1075\'E3\u1083\'EB\u1072\'E0\u1076\'E4\u1082\'EA\u1086\'EE \u1074\'E2 \u1078\'E6\u1080\'E8\u1079\'E7\u1085\'ED\u1080\'E8. \u1054\'CE\u1085\'ED \u1089\'F1\u1090\'F2\u1072\'E0\u1088\'F0\u1072\'E0\u1083\'EB\u1089\'F1\u1103\'FF \u1095\'F7\u1077\'E5\u1089\'F1\u1090\'F2\u1085\'ED\u1086\'EE \u1074\'E2\u1099\'FB\u1087\'EF\u1086\'EE\u1083\'EB\u1085\'ED\u1103\'FF\u1090\'F2\u1100\'FC \u1089\'F1\u1074\'E2\u1086\'EE\u1102\'FE \u1084\'EC\u1072\'E0\u1083\'EB\u1086\'EE\u1087\'EF\u1077\'E5\u1088\'F0\u1089\'F1\u1087\'EF\u1077\'E5\u1082\'EA\u1090\'F2\u1080\'E8\u1074\'E2\u1085\'ED\u1091\'F3\u1102\'FE \u1088\'F0\u1072\'E0\u1073\'E1\u1086\'EE\u1090\'F2\u1091\'F3, \u1095\'F7\u1072\'E0\u1089\'F1\u1090\'F2\u1086\'EE \u1074\'E2\u1089\'F1\u1087\'EF\u1086\'EE\u1084\'EC\u1080\'E8\u1085\'ED\u1072\'E0\u1083\'EB \u1089\'F1\u1074\'E2\u1086\'EE\u1080\'E8 \u1083\'EB\u1080\'E8\u1093\'F5\u1080\'E8\u1077\'E5 \u1075\'E3\u1086\'EE\u1076\'E4\u1099\'FB \u1080\'E8 \u1085\'ED\u1077\'E5 \u1086\'EE\u1089\'F1\u1086\'EE\u1073\'E1\u1086\'EE \u1093\'F5\u1086\'EE\u1090\'F2\u1077\'E5\u1083\'EB \u1095\'F7\u1090\'F2\u1086\'EE-\u1090\'F2\u1086\'EE \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF\u1090\'F2\u1100\'FC \u1074\'E2 \u1089\'F1\u1074\'E2\u1086\'EE\u1077\'E5\u1081\'E9 \u1078\'E6\u1080\'E8\u1079\'E7\u1085\'ED\u1080\'E8. \u1054\'CE\u1076\'E4\u1085\'ED\u1072\'E0\u1078\'E6\u1076\'E4\u1099\'FB \u1074\'E2\u1089\'F1\u1090\'F2\u1088\'F0\u1077\'E5\u1090\'F2\u1080\'E8\u1074\'E2 \u1089\'F1\u1074\'E2\u1086\'EE\u1077\'E5\u1075\'E3\u1086\'EE \u1079\'E7\u1085\'ED\u1072\'E0\u1082\'EA\u1086\'EE\u1084\'EC\u1086\'EE\u1075\'E3\u1086\'EE, \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1099\'FB\u1081\'E9 \u1087\'EF\u1088\'F0\u1077\'E5\u1076\'E4\u1083\'EB\u1086\'EE\u1078\'E6\u1080\'E8\u1083\'EB \u1077\'E5\u1084\'EC\u1091\'F3 \u1086\'EE\u1076\'E4\u1080\'E8\u1085\'ED \u1101\'FD\u1082\'EA\u1089\'F1\u1087\'EF\u1077\'E5\u1088\'F0\u1080\'E8\u1084\'EC\u1077\'E5\u1085\'ED\u1090\'F2\u1072\'E0\u1083\'EB\u1100\'FC\u1085\'ED\u1099\'FB\u1081\'E9 \u1087\'EF\u1088\'F0\u1077\'E5\u1087\'EF\u1072\'E0\u1088\'F0\u1072\'E0\u1090\'F2 \endash  \u1086\'EE\u1085\'ED \u171\'AB\u1087\'EF\u1088\'F0\u1086\'EE\u1079\'E7\u1088\'F0\u1077\'E5\u1083\'EB\u187\'BB. \u1045\'C5\u1075\'E3\u1086\'EE \u1084\'EC\u1099\'FB\u1096\'F8\u1083\'EB\u1077\'E5\u1085\'ED\u1080\'E8\u1077\'E5 \u1089\'F1\u1084\'EC\u1077\'E5\u1085\'ED\u1080\'E8\u1083\'EB\u1086\'EE \u1089\'F1\u1074\'E2\u1086\'EE\u1081\'E9 \u1086\'EE\u1073\'E1\u1099\'FB\u1095\'F7\u1085\'ED\u1099\'FB\u1081\'E9 \u1088\'F0\u1072\'E0\u1089\'F1\u1087\'EF\u1086\'EE\u1088\'F0\u1103\'FF\u1076\'E4\u1086\'EE\u1082\'EA, \u1080\'E8\u1079\'E7 \u1075\'E3\u1083\'EB\u1091\'F3\u1073\'E1\u1080\'E8\u1085\'ED \u1087\'EF\u1072\'E0\u1084\'EC\u1103\'FF\u1090\'F2\u1080\'E8 \u1074\'E2\u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3 \u1087\'EF\u1086\'EE\u1103\'FF\u1074\'E2\u1080\'E8\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1082\'EA\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0-\u1090\'F2\u1086\'EE \u1087\'EF\u1086\'EE\u1083\'EB\u1091\'F3\u1095\'F7\u1077\'E5\u1085\'ED\u1085\'ED\u1099\'FB\u1077\'E5 \u1079\'E7\u1085\'ED\u1072\'E0\u1085\'ED\u1080\'E8\u1103\'FF \u1080\'E8 \u1086\'EE\u1085\'ED \u1085\'ED\u1072\'E0\u1095\'F7\u1072\'E0\u1083\'EB \u1080\'E8\u1089\'F1\u1087\'EF\u1086\'EE\u1083\'EB\u1100\'FC\u1079\'E7\u1086\'EE\u1074\'E2\u1072\'E0\u1090\'F2\u1100\'FC \u1080\'E8\u1093\'F5 \u1085\'ED\u1072\'E0 \u1087\'EF\u1086\'EE\u1083\'EB\u1085\'ED\u1091\'F3\u1102\'FE. \u1053\'CD\u1086\'EE \u1087\'EF\u1086\'EE\u1089\'F1\u1090\'F2\u1072\'E0\u1074\'E2\u1097\'F9\u1080\'E8\u1082\'EA \u1079\'E7\u1072\'E0\u1075\'E3\u1072\'E0\u1076\'E4\u1086\'EE\u1095\'F7\u1085\'ED\u1099\'FB\u1084\'EC \u1086\'EE\u1073\'E1\u1088\'F0\u1072\'E0\u1079\'E7\u1086\'EE\u1084\'EC \u1087\'EF\u1086\'EE\u1075\'E3\u1080\'E8\u1073\'E1, \u1075\'E3\u1077\'E5\u1088\'F0\u1086\'EE\u1081\'E9 \u1085\'ED\u1072\'E0\u1096\'F8\u1077\'E5\u1083\'EB \u171\'AB\u1079\'E7\u1072\'E0\u1085\'ED\u1072\'E0\u1095\'F7\u1082\'EA\u1091\'F3\u187\'BB, \u1086\'EE\u1085\'ED \u1090\'F2\u1072\'E0\u1082\'EA \u1078\'E6\u1077\'E5 \u1086\'EE\u1073\'E1\u1085\'ED\u1072\'E0\u1088\'F0\u1091\'F3\u1078\'E6\u1080\'E8\u1083\'EB \u1073\'E1\u1091\'F3\u1082\'EA\u1074\'E2\u1072\'E0\u1083\'EB\u1100\'FC\u1085\'ED\u1086\'EE \u1091\'F3\u1073\'E1\u1080\'E8\u1081\'E9\u1089\'F1\u1090\'F2\u1074\'E2\u1077\'E5\u1085\'ED\u1085\'ED\u1099\'FB\u1077\'E5 \u1087\'EF\u1086\'EE\u1073\'E1\u1086\'EE\u1095\'F7\u1085\'ED\u1099\'FB\u1077\'E5 \u1101\'FD\u1092\'F4\u1092\'F4\u1077\'E5\u1082\'EA\u1090\'F2\u1099\'FB, \u1086\'EE\u1090\'F2 \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1099\'FB\u1093\'F5 \u1085\'ED\u1077\'E5\u1082\'EA\u1091\'F3\u1076\'E4\u1072\'E0 \u1076\'E4\u1077\'E5\u1090\'F2\u1100\'FC\u1089\'F1\u1103\'FF. \u1044\'C4\u1072\'E0 \u1077\'E5\u1097\'F9\u1105\'B8 \u1080\'E8 \u1085\'ED\u1072\'E0 \u1075\'E3\u1086\'EE\u1088\'F0\u1080\'E8\u1079\'E7\u1086\'EE\u1085\'ED\u1090\'F2\u1077\'E5 \u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1087\'EF\u1077\'E5\u1088\'F0\u1089\'F1\u1087\'EF\u1077\'E5\u1082\'EA\u1090\'F2\u1080\'E8\u1074\'E2\u8230\'85\line \u1050\'CA\u1085\'ED\u1080\'E8\u1075\'E3\u1072\'E0 \u1085\'ED\u1072\'E0\u1087\'EF\u1086\'EE\u1083\'EB\u1085\'ED\u1077\'E5\u1085\'ED\u1072\'E0 \u1074\'E2\u1086\'EE\u1089\'F1\u1087\'EF\u1086\'EE\u1084\'EC\u1080\'E8\u1085\'ED\u1072\'E0\u1085\'ED\u1080\'E8\u1103\'FF\u1084\'EC\u1080\'E8, \u1088\'F0\u1072\'E0\u1079\'E7\u1084\'EC\u1099\'FB\u1096\'F8\u1083\'EB\u1077\'E5\u1085\'ED\u1080\'E8\u1103\'FF\u1084\'EC\u1080\'E8 \u1080\'E8 \u1085\'ED\u1077\'E5\u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1099\'FB\u1084\'EC\u1080\'E8 \u1080\'E8\u1085\'ED\u1090\'F2\u1077\'E5\u1088\'F0\u1077\'E5\u1089\'F1\u1085\'ED\u1099\'FB\u1084\'EC\u1080\'E8 \u1076\'E4\u1077\'E5\u1090\'F2\u1072\'E0\u1083\'EB\u1103\'FF\u1084\'EC\u1080\'E8, \u1082\'EA \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1099\'FB\u1084\'EC \u1087\'EF\u1086\'EE\u1085\'ED\u1072\'E0\u1095\'F7\u1072\'E0\u1083\'EB\u1091\'F3 \u1090\'F2\u1088\'F0\u1091\'F3\u1076\'E4\u1085\'ED\u1086\'EE \u1087\'EF\u1088\'F0\u1080\'E8\u1074\'E2\u1099\'FB\u1082\'EA\u1085\'ED\u1091\'F3\u1090\'F2\u1100\'FC, \u1085\'ED\u1086\'EE \u1101\'FD\u1090\'F2\u1086\'EE \u1090\'F2\u1086\'EE\u1095\'F7\u1085\'ED\u1086\'EE \u1090\'F2\u1086\'EE\u1075\'E3\u1086\'EE \u1089\'F1\u1090\'F2\u1086\'EE\u1080\'E8\u1090\'F2, \u1087\'EF\u1086\'EE\u1089\'F1\u1082\'EA\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1091\'F3 \u1076\'E4\u1086\'EE \u1089\'F1\u1072\'E0\u1084\'EC\u1086\'EE\u1075\'E3\u1086\'EE \u1082\'EA\u1086\'EE\u1085\'ED\u1094\'F6\u1072\'E0 \u1103\'FF \u1085\'ED\u1077\'E5 \u1084\'EC\u1086\'EE\u1075\'E3 \u1087\'EF\u1086\'EE\u1085\'ED\u1103\'FF\u1090\'F2\u1100\'FC, \u1095\'F7\u1090\'F2\u1086\'EE \u1087\'EF\u1088\'F0\u1086\'EE\u1080\'E8\u1079\'E7\u1086\'EE\u1081\'E9\u1076\'E4\u1077\'E5\u1090\'F2 \u1076\'E4\u1072\'E0\u1083\'EB\u1100\'FC\u1096\'F8\u1077\'E5, \u1103\'FF \u1085\'ED\u1077\'E5 \u1087\'EF\u1086\'EE\u1084\'EC\u1085\'ED\u1080\'E8\u1083\'EB \u1082\'EA\u1086\'EE\u1085\'ED\u1094\'F6\u1086\'EE\u1074\'E2\u1082\'EA\u1091\'F3 \u1101\'FD\u1082\'EA\u1088\'F0\u1072\'E0\u1085\'ED\u1080\'E8\u1079\'E7\u1072\'E0\u1094\'F6\u1080\'E8\u1080\'E8, \u1085\'ED\u1086\'EE \u1086\'EE\u1076\'E4\u1085\'ED\u1086\'EE\u1079\'E7\u1085\'ED\u1072\'E0\u1095\'F7\u1085\'ED\u1086\'EE \u1084\'EC\u1086\'EE\u1075\'E3\u1091\'F3 \u1089\'F1\u1082\'EA\u1072\'E0\u1079\'E7\u1072\'E0\u1090\'F2\u1100\'FC \endash  \u1095\'F7\u1080\'E8\u1090\'F2\u1072\'E0\u1081\'E9\u1090\'F2\u1077\'E5 \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1091\'F3. \u1055\'CF\u1086\'EE\u1095\'F7\u1077\'E5\u1084\'EC\u1091\'F3? \u1055\'CF\u1086\'EE\u1090\'F2\u1086\'EE\u1084\'EC\u1091\'F3 \u1095\'F7\u1090\'F2\u1086\'EE \u1086\'EE\u1085\'ED\u1072\'E0 \u1085\'ED\u1077\'E5\u1087\'EF\u1088\'F0\u1077\'E5\u1076\'E4\u1089\'F1\u1082\'EA\u1072\'E0\u1079\'E7\u1091\'F3\u1077\'E5\u1084\'EC\u1072\'E0, \u1087\'EF\u1086\'EE\u1090\'F2\u1086\'EE\u1084\'EC\u1091\'F3 \u1095\'F7\u1090\'F2\u1086\'EE \u1086\'EE\u1085\'ED\u1072\'E0 \u1079\'E7\u1072\'E0\u1074\'E2\u1086\'EE\u1088\'F0\u1072\'E0\u1078\'E6\u1080\'E8\u1074\'E2\u1072\'E0\u1077\'E5\u1090\'F2 \u1086\'EE\u1073\'E1\u1088\'F0\u1072\'E0\u1079\'E7\u1086\'EE\u1074\'E2\u1072\'E0\u1085\'ED\u1085\'ED\u1086\'EE\u1089\'F1\u1090\'F2\u1100\'FC\u1102\'FE \u1072\'E0\u1074\'E2\u1090\'F2\u1086\'EE\u1088\'F0\u1072\'E0 \u1080\'E8 \u1075\'E3\u1077\'E5\u1088\'F0\u1086\'EE\u1103\'FF, \u1087\'EF\u1086\'EE\u1090\'F2\u1086\'EE\u1084\'EC\u1091\'F3 \u1095\'F7\u1090\'F2\u1086\'EE \u1101\'FD\u1090\'F2\u1072\'E0 \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1072\'E0 \u1079\'E7\u1072\'E0\u1089\'F1\u1090\'F2\u1072\'E0\u1074\'E2\u1083\'EB\u1103\'FF\u1077\'E5\u1090\'F2 \u1076\'E4\u1091\'F3\u1084\'EC\u1072\'E0\u1090\'F2\u1100\'FC \u1086\'EE \u1090\'F2\u1086\'EE\u1084\'EC, \u1095\'F7\u1090\'F2\u1086\'EE \u1082\'EA\u1072\'E0\u1078\'E6\u1076\'E4\u1099\'FB\u1081\'E9 \u1080\'E8\u1079\'E7 \u1085\'ED\u1072\'E0\u1089\'F1 \u1084\'EC\u1086\'EE\u1078\'E6\u1077\'E5\u1090\'F2 \u1076\'E4\u1086\'EE\u1089\'F1\u1090\'F2\u1080\'E8\u1095\'F7\u1100\'FC \u1075\'E3\u1086\'EE\u1088\'F0\u1072\'E0\u1079\'E7\u1076\'E4\u1086\'EE \u1073\'E1\u1086\'EE\u1083\'EB\u1100\'FC\u1096\'F8\u1077\'E5\u1075\'E3\u1086\'EE, \u1095\'F7\u1077\'E5\u1084\'EC \u1086\'EE\u1085\'ED \u1086\'EE\u1090\'F2 \u1089\'F1\u1077\'E5\u1073\'E1\u1103\'FF \u1078\'E6\u1076\'E4\u1077\'E5\u1090\'F2. \u1040\'C0 \u1077\'E5\u1097\'F9\u1105\'B8, \u1087\'EF\u1086\'EE\u1089\'F1\u1083\'EB\u1077\'E5 \u1087\'EF\u1088\'F0\u1086\'EE\u1095\'F7\u1090\'F2\u1077\'E5\u1085\'ED\u1080\'E8\u1103\'FF \u1103\'FF \u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB \u1095\'F7\u1072\'E0\u1097\'F9\u1077\'E5 \u1085\'ED\u1072\'E0\u1074\'E2\u1086\'EE\u1076\'E4\u1080\'E8\u1090\'F2\u1100\'FC \u1087\'EF\u1086\'EE\u1088\'F0\u1103\'FF\u1076\'E4\u1086\'EE\u1082\'EA \u1074\'E2 \u1089\'F1\u1074\'E2\u1086\'EE\u1077\'E5\u1081\'E9 \u1082\'EA\u1086\'EE\u1084\'EC\u1085\'ED\u1072\'E0\u1090\'F2\u1077\'E5, \u1087\'EF\u1086\'EE\u1090\'F2\u1086\'EE\u1084\'EC \u1087\'EF\u1086\'EE\u1081\'E9\u1084\'EC\u1077\'E5\u1090\'F2\u1077\'E5 \u1087\'EF\u1086\'EE\u1095\'F7\u1077\'E5\u1084\'EC\u1091\'F3.}
\par\plain {\f2\fs24\b0\i0 \loch\af2\hich\af2\dbch\af2\uc1\u1044\'C4\u1072\'E0, \u1088\'F0\u1077\'E5\u1073\'E1\u1103\'FF\u1090\'F2, \u1074\'E2\u1089\'F1\u1077\'E5 \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1080\'E8 \u1073\'E1\u1099\'FB\u1083\'EB\u1080\'E8 \u1101\'FD\u1082\'EA\u1088\'F0\u1072\'E0\u1085\'ED\u1080\'E8\u1079\'E7\u1080\'E8\u1088\'F0\u1086\'EE\u1074\'E2\u1072\'E0\u1085\'ED\u1099\'FB, \u1085\'ED\u1086\'EE \u1101\'FD\u1090\'F2\u1086\'EE \u1079\'E7\u1072\'E0\u1095\'F7\u1072\'E0\u1089\'F1\u1090\'F2\u1091\'F3\u1102\'FE \u1076\'E4\u1086\'EE\u1082\'EA\u1072\'E0\u1079\'E7\u1099\'FB\u1074\'E2\u1072\'E0\u1077\'E5\u1090\'F2 \u1080\'E8\u1093\'F5 \u1082\'EA\u1072\'E0\u1095\'F7\u1077\'E5\u1089\'F1\u1090\'F2\u1074\'E2\u1086\'EE, \u1085\'ED\u1086\'EE \u1086\'EE\u1085\'ED\u1086\'EE \u1076\'E4\u1083\'EB\u1103\'FF \u1082\'EA\u1072\'E0\u1078\'E6\u1076\'E4\u1086\'EE\u1075\'E3\u1086\'EE \u1089\'F1\u1074\'E2\u1086\'EE\u1077\'E5. \u1063\'D7\u1080\'E8\u1090\'F2\u1072\'E0\u1081\'E9\u1090\'F2\u1077\'E5 \u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1090\'F2\u1086\'EE, \u1095\'F7\u1090\'F2\u1086\'EE \u1074\'E2\u1072\'E0\u1084\'EC \u1085\'ED\u1088\'F0\u1072\'E0\u1074\'E2\u1080\'E8\u1090\'F2\u1089\'F1\u1103\'FF \u1080\'E8 \u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1090\'F2\u1086\'EE, \u1095\'F7\u1090\'F2\u1086\'EE \u1087\'EF\u1086\'EE\u1084\'EC\u1086\'EE\u1078\'E6\u1077\'E5\u1090\'F2 \u1074\'E2\u1072\'E0\u1084\'EC \u1074\'E2 \u1078\'E6\u1080\'E8\u1079\'E7\u1085\'ED\u1080\'E8. \u1057\'D1\u1087\'EF\u1072\'E0\u1089\'F1\u1080\'E8\u1073\'E1\u1086\'EE \u1079\'E7\u1072\'E0 \u1074\'E2\u1085\'ED\u1080\'E8\u1084\'EC\u1072\'E0\u1085\'ED\u1080\'E8\u1077\'E5.}
