\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1042\'C2 \u1086\'EE\u1090\'F2\u1076\'E4\u1077\'E5\u1083\'EB \u1084\'EC\u1072\'E0\u1088\'F0\u1082\'EA\u1077\'E5\u1090\'F2\u1080\'E8\u1085\'ED\u1075\'E3\u1072\'E0 \u1040\'C0\u1088\'F0\u1080\'E8\u1085\'ED\u1072\'E0 \u1088\'F0\u1077\'E5\u1096\'F8\u1080\'E8\u1083\'EB\u1072\'E0 \u1087\'EF\u1088\'F0\u1086\'EE\u1074\'E2\u1077\'E5\u1089\'F1\u1090\'F2\u1080\'E8 \u8220\'93\u1089\'F1\u1086\'EE\u1080\'E8\u1089\'F1\u1082\'EA\u1072\'E0\u1090\'F2\u1077\'E5\u1083\'EB\u1103\'FF\u8221\'94 \u1095\'F7\u1077\'E5\u1088\'F0\u1077\'E5\u1079\'E7 \u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB\u1086\'EE\u1074\'E2\u1091\'F3\u1102\'FE \u1076\'E4\u1083\'EB\u1103\'FF \u1088\'F0\u1072\'E0\u1073\'E1\u1086\'EE\u1090\'F2\u1085\'ED\u1080\'E8\u1082\'EA\u1086\'EE\u1074\'E2. \u1047\'C7\u1076\'E4\u1077\'E5\u1089\'F1\u1100\'FC \u1073\'E1\u1099\'FB\u1083\'EB\u1080\'E8 \u1076\'E4\u1083\'EB\u1080\'E8\u1085\'ED\u1085\'ED\u1085\'ED\u1099\'FB\u1077\'E5 \u1076\'E4\u1077\'E5\u1088\'F0\u1077\'E5\u1074\'E2\u1103\'FF\u1085\'ED\u1085\'ED\u1099\'FB\u1077\'E5 \u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB\u1099\'FB \u1089\'F1 \u1083\'EB\u1072\'E0\u1074\'E2\u1086\'EE\u1095\'F7\u1082\'EA\u1072\'E0\u1084\'EC\u1080\'E8 \u1076\'E4\u1083\'EB\u1103\'FF \u1090\'F2\u1077\'E5\u1093\'F5, \u1082\'EA\u1090\'F2\u1086\'EE \u1093\'F5\u1086\'EE\u1095\'F7\u1077\'E5\u1090\'F2 \u1087\'EF\u1086\'EE\u1089\'F1\u1080\'E8\u1076\'E4\u1077\'E5\u1090\'F2\u1100\'FC \u1088\'F0\u1103\'FF\u1076\'E4\u1086\'EE\u1084\'EC \u1089\'F1 \u1088\'F0\u1072\'E0\u1073\'E1\u1086\'EE\u1090\'F2\u1085\'ED\u1080\'E8\u1082\'EA\u1072\'E0\u1084\'EC\u1080\'E8 \u1080\'E8\u1079\'E7 \u1089\'F1\u1074\'E2\u1086\'EE\u1077\'E5\u1075\'E3\u1086\'EE \u1086\'EE\u1090\'F2\u1076\'E4\u1077\'E5\u1083\'EB\u1072\'E0 \u1080\'E8 \u1085\'ED\u1077\'E5\u1073\'E1\u1086\'EE\u1083\'EB\u1100\'FC\u1096\'F8\u1080\'E8\u1077\'E5 \u1082\'EA\u1088\'F0\u1091\'F3\u1075\'E3\u1083\'EB\u1099\'FB\u1077\'E5 \u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB\u1099\'FB \u1076\'E4\u1083\'EB\u1103\'FF \u1076\'E4\u1088\'F0\u1091\'F3\u1079\'E7\u1077\'E5\u1081\'E9.}
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1055\'CF\u1080\'E8\u1090\'F2\u1072\'E0\u1085\'ED\u1080\'E8\u1077\'E5 \u1091\'F3 \u1085\'ED\u1072\'E0\u1089\'F1 \u1073\'E1\u1077\'E5\u1089\'F1\u1087\'EF\u1083\'EB\u1072\'E0\u1090\'F2\u1085\'ED\u1086\'EE\u1077\'E5, \hich\f1 \emdash \loch\f1  \u1074\'E2\u1077\'E5\u1097\'F9\u1072\'E0\u1083\'EB\u1072\'E0 \u1076\'E4\u1077\'E5\u1074\'E2\u1091\'F3\u1096\'F8\u1082\'EA\u1072\'E0, \u1087\'EF\u1088\'F0\u1086\'EE\u1074\'E2\u1086\'EE\u1076\'E4\u1103\'FF \u1044\'C4\u1077\'E5\u1085\'ED\u1072\'E0 \u1095\'F7\u1077\'E5\u1088\'F0\u1077\'E5\u1079\'E7 \u1087\'EF\u1091\'F3\u1089\'F1\u1090\'F2\u1091\'F3\u1102\'FE \u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB\u1086\'EE\u1074\'E2\u1091\'F3\u1102\'FE, \hich\f1 \emdash \loch\f1   \u1087\'EF\u1086\'EE \u1087\'EF\u1088\'F0\u1072\'E0\u1079\'E7\u1076\'E4\u1085\'ED\u1080\'E8\u1082\'EA\u1072\'E0\u1084\'EC \u1096\'F8\u1074\'E2\u1077\'E5\u1076\'E4\u1089\'F1\u1082\'EA\u1080\'E8\u1081\'E9 \u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB, \u1090\'F2\u1072\'E0\u1082\'EA \u1078\'E6\u1077\'E5 \u1077\'E5\u1089\'F1\u1090\'F2\u1100\'FC \u1086\'EE\u1090\'F2\u1076\'E4\u1077\'E5\u1083\'EB\u1100\'FC\u1085\'ED\u1086\'EE\u1077\'E5 \u1084\'EC\u1077\'E5\u1085\'ED\u1102\'FE \u1076\'E4\u1083\'EB\u1103\'FF \u1072\'E0\u1083\'EB\u1083\'EB\u1077\'E5\u1088\'F0\u1075\'E3\u1080\'E8\u1082\'EA\u1086\'EE\u1074\'E2. \u1057\'D1\u1077\'E5\u1081\'E9\u1095\'F7\u1072\'E0\u1089\'F1 \u1091\'F3 \u1085\'ED\u1072\'E0\u1089\'F1 \u1091\'F3\u1090\'F2\u1088\'F0\u1077\'E5\u1085\'ED\u1085\'ED\u1103\'FF\u1103\'FF \u1085\'ED\u1072\'E0\u1075\'E3\u1088\'F0\u1091\'F3\u1079\'E7\u1082\'EA\u1072\'E0, \u1087\'EF\u1086\'EE\u1101\'FD\u1090\'F2\u1086\'EE\u1084\'EC\u1091\'F3 \u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB\u1086\'EE\u1074\'E2\u1082\'EA\u1072\'E0 \u1087\'EF\u1091\'F3\u1089\'F1\u1090\'F2\u1091\'F3\u1077\'E5\u1090\'F2, \u1072\'E0 \u1090\'F2\u1072\'E0\u1082\'EA, \u1089\'F1\u1102\'FE\u1076\'E4\u1072\'E0 \u1084\'EC\u1086\'EE\u1078\'E