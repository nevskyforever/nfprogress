\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u1063\'D7\u1090\'F2\u1086\'EE\u1073\'E1\u1099\'FB \u1085\'ED\u1072\'E0\u1089\'F1 \u1090\'F2\u1086\'EE\u1095\'F7\u1085\'ED\u1086\'EE \u1085\'ED\u1080\'E8\u1082\'EA\u1090\'F2\u1086\'EE \u1085\'ED\u1077\'E5 \u1091\'F3\u1074\'E2\u1080\'E8\u1076\'E4\u1077\'E5\u1083\'EB, \u1084\'EC\u1099\'FB \u1091\'F3\u1096\'F8\u1083\'EB\u1080\'E8 \u1074\'E2 \u1089\'F1\u1086\'EE\u1089\'F1\u1077\'E5\u1076\'E4\u1085\'ED\u1080\'E8\u1081\'E9 \u1076\'E4\u1074\'E2\u1086\'EE\u1088\'F0. \u1048\'C8 \u1101\'FD\u1090\'F2\u1086\'EE \u1084\'EC\u1099\'FB \u1089\'F1\u1076\'E4\u1077\'E5\u1083\'EB\u1072\'E0\u1083\'EB\u1080\'E8 \u1079\'E7\u1088\'F0\u1103\'FF. \u1055\'CF\u1088\'F0\u1072\'E0\u1082\'EA\u1090\'F2\u1080\'E8\u1095\'F7\u1077\'E5\u1089\'F1\u1082\'EA\u1080\'E8 \u1089\'F1\u1088\'F0\u1072\'E0\u1079\'E7\u1091\'F3 \u1085\'ED\u1072\'E0\u1089\'F1 \u1080\'E8 \u1090\'F2\u1072\'E0\u1084\'EC \u1085\'ED\u1072\'E0\u1089\'F1\u1090\'F2\u1080\'E8\u1075\'E3 \u1087\'EF\u1072\'E0\u1090\'F2\u1088\'F0\u1091\'F3\u1083\'EB\u1100\'FC, \u1080\'E8 \u1084\'EC\u1099\'FB \u1089\'F1\u1085\'ED\u1086\'EE\u1074\'E2\u1072\'E0 \u1087\'EF\u1088\'F0\u1103\'FF\u1090\'F2\u1072\'E0\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1079\'E7\u1072\'E0 \u1084\'EC\u1072\'E0\u1096\'F8\u1080\'E8\u1085\'ED\u1072\'E0\u1084\'EC\u1080\'E8. \u1044\'C4\u1074\'E2\u1086\'EE\u1088\'F0 \u1073\'E1\u1099\'FB\u1083\'EB \u1076\'E4\u1086\'EE\u1074\'E2\u1086\'EE\u1083\'EB\u1100\'FC\u1085\'ED\u1086\'EE \u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1090\'F2\u1086\'EE\u1088\'F0\u1085\'ED\u1099\'FB\u1084\'EC \u1080\'E8 \u1092\'F4\u1072\'E0\u1088\'F0\u1099\'FB \u1083\'EB\u1077\'E5\u1075\'E3\u1082\'EA\u1086\'EE \u1084\'EC\u1086\'EE\u1075\'E3\u1083\'EB\u1080\'E8 \u1085\'ED\u1072\'E0\u1089\'F1 \u1079\'E7\u1072\'E0\u1094\'F6\u1077\'E5\u1087\'EF\u1080\'E8\u1090\'F2\u1100\'FC, \u1085\'ED\u1086\'EE \u1089\'F1\u1085\'ED\u1086\'EE\u1074\'E2\u1072\'E0 \u1087\'EF\u1088\'F0\u1086\'EE\u1085\'ED\u1077\'E5\u1089\'F1\u1083\'EB\u1086\'EE.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u1053\'CD\u1077\'E5 \u1079\'E7\u1085\'ED\u1072\'E0\u1103\'FF \u1091\'F3\u1078\'E6\u1077\'E5, \u1082\'EA\u1091\'F3\u1076\'E4\u1072\'E0 \u1076\'E4\u1077\'E5\u1074\'E2\u1072\'E0\u1090\'F2\u1100\'FC\u1089\'F1\u1103\'FF, \u1084\'EC\u1099\'FB \u1087\'EF\u1086\'EE\u1096\'F8\u1083\'EB\u1080\'E8 \u1074\'E2 \u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3\u1086\'EE\u1077\'E5 \u1084\'EC\u1077\'E5\u1089\'F1\u1090\'F2\u1086\'EE. \u1042\'C2 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1091\'F3\u1083\'EB\u1086\'EE\u1095\'F7\u1082\'EA\u1077\'E5 \u1084\'EC\u1077\'E5\u1078\'E6\u1076\'E4\u1091\'F3 \u1076\'E4\u1074\'E2\u1086\'EE\u1088\'F0\u1072\'E0\u1084\'EC\u1080\'E8 \u1084\'EC\u1099\'FB \u1086\'EE\u1089\'F1\u1090\'F2\u1072\'E0\u1085\'ED\u1086\'EE\u1074\'E2\u1080\'E8\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC, \u1095\'F7\u1090\'F2\u1086\'EE\u1073\'E1\u1099\'FB \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1076\'E4\u1086\'EE\u1093\'F5\u1085\'ED\u1091\'F3\u1090\'F2\u1100\'FC. \u1053\'CD\u1077\'E5 \u1087\'EF\u1088\'F0\u1086\'EE\u1096\'F8\u1083\'EB\u1086\'EE \u1080\'E8 \u1076\'E4\u1074\'E2\u1091\'F3\u1093\'F5 \u1084\'EC\u1080\'E8\u1085\'ED\u1091\'F3\u1090\'F2, \u1082\'EA\u1072\'E0\u1082\'EA \u1085\'ED\u1077\'E5\u1080\'E8\u1079\'E7\u1074\'E2\u1077\'E5\u1089\'F1\u1090\'F2\u1085\'ED\u1086\'EE \u1082\'EA\u1090\'F2\u1086\'EE \u1080\'E8 \u1085\'ED\u1077\'E5\u1080\'E8\u1079\'E7\u1074\'E2\u1077\'E5\u1089\'F1\u1090\'F2\u1085\'ED\u1086\'EE \u1086\'EE\u1090\'F2\u1082\'EA\u1091\'F3\u1076\'E4\u1072\'E0 \u1082\'EA\u1088\'F0\u1080\'E8\u1082\'EA\u1085\'ED\u1091\'F3\u1083\'EB \u1095\'F7\u1090\'F2\u1086\'EE-\u1090\'F2\u1086\'EE \u1085\'ED\u1077\'E5\u1087\'EF\u1086\'EE\u1085\'ED\u1103\'FF\u1090\'F2\u1085\'ED\u1086\'EE\u1077\'E5 \u1080\'E8 \u1074\'E2 \u1087\'EF\u1088\'F0\u1080\'E8\u1082\'EA\u1072\'E0\u1079\'E7\u1085\'ED\u1086\'EE\u1084\'EC \u1090\'F2\u1086\'EE\u1085\'ED\u1077\'E5. \u1052\'CC\u1099\'FB \u1089\'F1 \u1087\'EF\u1086\'EE\u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3\u1086\'EE\u1081\'E9 \u1088\'F0\u1077\'E5\u1096\'F8\u1080\'E8\u1083\'EB\u1080\'E8, \u1095\'F7\u1090\'F2\u1086\'EE \u1101\'FD\u1090\'F2\u1086\'EE \u1085\'ED\u1072\'E0\u1084\'EC \u1080\'E8 \u1076\'E4\u1088\'F0\u1072\'E0\u1087\'EF\u1072\'E0\u1085\'ED\u1091\'F3\u1083\'EB\u1080\'E8 \u1087\'EF\u1086\'EE \u1090\'F2\u1077\'E5\u1084\'EC\u1085\'ED\u1086\'EE\u1084\'EC\u1091\'F3 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1091\'F3\u1083\'EB\u1086\'EE\u1095\'F7\u1082\'EA\u1091\'F3.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u1045\'C5\u1089\'F1\u1083\'EB\u1080\'E8 \u1095\'F7\u1077\'E5\u1089\'F1\u1090\'F2\u1085\'ED\u1086\'EE, \u1103\'FF \u1085\'ED\u1077\'E5\u1085\'ED\u1072\'E0\u1074\'E2\u1080\'E8\u1078\'E6\u1091\'F3 \u1101\'FD\u1090\'F2\u1091\'F3 \u1075\'E3\u1088\'F0\u1103\'FF\u1079\'E7\u1085\'ED\u1091\'F3\u1102\'FE, \u1074\'E2\u1089\'F1\u1077\'E5\u1075\'E3\u1076\'E4\u1072\'E0 \u1088\'F0\u1072\'E0\u1079\'E7\u1084\'EC\u1099\'FB\u1074\'E2\u1072\'E0\u1077\'E5\u1084\'EC\u1091\'F3\u1102\'FE \u1090\'F2\u1088\'F0\u1086\'EE\u1087\'EF\u1080\'E8\u1085\'ED\u1086\'EE\u1095\'F7\u1082\'EA\u1091\'F3. \u1059\'D3\u1076\'E4\u1080\'E8\u1088\'F0\u1072\'E0\u1103\'FF \u1089\'F1\u1086\'EE \u1074\'E2\u1089\'F1\u1077\'E5\u1093\'F5 \u1085\'ED\u1086\'EE\u1075\'E3, \u1103\'FF \u1079\'E7\u1072\'E0\u1087\'EF\u1085\'ED\u1091\'F3\u1083\'EB\u1089\'F1\u1103\'FF, \u1091\'F3\u1087\'EF\u1072\'E0\u1083\'EB \u1085\'ED\u1072\'E0 \u1082\'EA\u1086\'EE\u1083\'EB\u1077\'E5\u1085\'ED\u1080\'E8 \u1080\'E8 \u1087\'EF\u1088\'F0\u1086\'EE\u1082\'EA\u1072\'E0\u1090\'F2\u1080\'E8\u1083\'EB\u1089\'F1\u1103\'FF \u1085\'ED\u1072\'E0 \u1085\'ED\u1080\'E8\u1093\'F5 \u1087\'EF\u1086\'EE \u1075\'E3\u1088\'F0\u1103\'FF\u1079\'E7\u1080\'E8, \u1082\'EA\u1072\'E0\u1082\'EA \u1084\'EC\u1091\'F3\u1079\'E7\u1099\'FB\u1082\'EA\u1072\'E0\u1085\'ED\u1090\'F2 \u1087\'EF\u1086\'EE \u1089\'F1\u1094\'F6\u1077\'E5\u1085\'ED\u1077\'E5 \u1089\'F1 \u1075\'E3\u1080\'E8\u1090\'F2\u1072\'E0\u1088\'F0\u1086\'EE\u1081\'E9 \u1074\'E2 \u1088\'F0\u1091\'F3\u1082\'EA\u1072\'E0\u1093\'F5. \u1052\'CC\u1086\'EE\u1080\'E8 \u1089\'F1\u1074\'E2\u1077\'E5\u1078\'E6\u1077\'E5\u1087\'EF\u1086\'EE\u1089\'F1\u1090\'F2\u1080\'E8\u1088\'F0\u1072\'E0\u1085\'ED\u1085\'ED\u1099\'FB\u1077\'E5 \u1076\'E4\u1078\'E6\u1080\'E8\u1085\'ED\u1089\'F1\u1099\'FB \u1087\'EF\u1086\'EE\u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1076\'E4\u1072\'E0\u1083\'EB\u1080\'E8. \u1059\'D3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1073\'E1\u1086\'EE\u1083\'EB\u1100\'FC\u1096\'F8\u1086\'EE\u1081\'E9 \u1087\'EF\u1091\'F3\u1085\'ED\u1082\'EA\u1090\'F2\u1080\'E8\u1082\'EA \u1085\'ED\u1072\'E0 \u1075\'E3\u1088\'F0\u1103\'FF\u1079\'E7\u1100\'FC, \u1085\'ED\u1086\'EE \u1090\'F2\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0 \u1073\'E1\u1099\'FB\u1083\'EB\u1086\'EE \u1085\'ED\u1072\'E0\u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1096\'F8\u1085\'ED\u1086\'EE, \u1095\'F7\u1090\'F2\u1086\'EE \u1103\'FF \u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1090\'F2\u1086\'EE \u1074\'E2\u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB \u1080\'E8 \u1087\'EF\u1086\'EE\u1073\'E1\u1077\'E5\u1078\'E6\u1072\'E0\u1083\'EB. \u1054\'CE\u1089\'F1\u1090\'F2\u1072\'E0\u1085\'ED\u1086\'EE\u1074\'E2\u1080\'E8\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1084\'EC\u1099\'FB \u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1090\'F2\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0, \u1082\'EA\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0 \u1087\'EF\u1086\'EE\u1085\'ED\u1103\'FF\u1083\'EB\u1080\'E8, \u1095\'F7\u1090\'F2\u1086\'EE \u1085\'ED\u1080\'E8\u1082\'EA\u1086\'EE\u1084\'EC\u1091\'F3 \u1085\'ED\u1077\'E5 \u1085\'ED\u1091\'F3\u1078\'E6\u1085\'ED\u1099\'FB.}
