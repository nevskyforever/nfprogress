\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1054\'CE, \u1057\'D1\u1072\'E0\u1085\'ED\u1103\'FF! \hich\f1 \emdash \loch\f1  \u1074\'E2\u1077\'E5\u1089\'F1\u1077\'E5\u1083\'EB\u1099\'FB\u1081\'E9 \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1 \u1075\'E3\u1088\'F0\u1077\'E5\u1084\'EC\u1080\'E8\u1090\'F2 \u1091\'F3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1085\'ED\u1072\'E0\u1076\'E4 \u1091\'F3\u1093\'F5\u1086\'EE\u1084\'EC.}
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1052\'CC\u1080\'E8\u1096\'F8\u1072\'E0 \hich\f1 \emdash \loch\f1  \u1087\'EF\u1077\'E5\u1088\'F0\u1074\'E2\u1072\'E0\u1103\'FF \u1076\'E4\u1077\'E5\u1090\'F2\u1072\'E0\u1083\'EB\u1100\'FC \u1087\'EF\u1086\'EE \u1089\'F1\u1086\'EE\u1089\'F1\u1077\'E5\u1076\'E4\u1089\'F1\u1090\'F2\u1074\'E2\u1091\'F3. \u1057\'D1\u1080\'E8\u1076\'E4\u1080\'E8\u1090\'F2 \u1089\'F1\u1087\'EF\u1088\'F0\u1072\'E0\u1074\'E2\u1072\'E0 \u1086\'EE\u1090\'F2 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1080\'E8 \u1085\'ED\u1080\'E8\u1082\'EA\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0 \u1085\'ED\u1077\'E5 \u1079\'E7\u1072\'E0\u1090\'F2\u1099\'FB\u1082\'EA\u1072\'E0\u1077\'E5\u1090\'F2\u1089\'F1\u1103\'FF. \u1044\'C4\u1072\'E0\u1078\'E6\u1077\'E5 \u1074\'E2 \u1087\'EF\u1088\'F0\u1086\'EE\u1094\'F6\u1077\'E5\u1089\'F1\u1089\'F1\u1077\'E5 \u1088\'F0\u1072\'E0\u1073\'E1\u1086\'EE\u1090\'F2\u1099\'FB \u1087\'EF\u1086\'EE\u1089\'F1\u1090\'F2\u1086\'EE\u1103\'FF\u1085\'ED\u1085\'ED\u1086\'EE \u1095\'F7\u1090\'F2\u1086\'EE-\u1090\'F2\u1086\'EE \u1073\'E1\u1091\'F3\u1088\'F0\u1095\'F7\u1080\'E8\u1090\'F2. \u1043\'C3\u1086\'EE\u1074\'E2\u1086\'EE\u1088\'F0\u1080\'E8\u1090\'F2, \u1095\'F7\u1090\'F2\u1086\'EE \u1101\'FD\u1090\'F2\u1086\'EE \u1077\'E5\u1084\'EC\u1091\'F3 \u1087\'EF\u1086\'EE\u1084\'EC\u1086\'EE\u1075\'E3\u1072\'E0\u1077\'E5\u1090\'F2 \u1088\'F0\u1072\'E0\u1073\'E1\u1086\'EE\u1090\'F2\u1072\'E0\u1090\'F2\u1100\'FC \u1101\'FD\u1092\'F4\u1092\'F4\u1077\'E5\u1082\'EA\u1090\'F2\u1080\'E8\u1074\'E2\u1085\'ED\u1077\'E5\u1077\'E5. \u1053\'CD\u1072\'E0\u1074\'E2\u1077\'E5\u1088\'F0\u1085\'ED\u1086\'EE\u1077\'E5, \u1086\'EE\u1085\'ED \u1074\'E2\u1079\'E7\u1086\'EE\u1088\'F0\u1074\'E2\u1077\'E5\u1090\'F2\u1089\'F1\u1103\'FF, \u1077\'E5\u1089\'F1\u1083\'EB\u1080\'E8 \u1079\'E7\u1072\'E0\u1096\'F8\u1080\'E8\u1090\'F2\u1100\'FC \u1077\'E5\u1084\'EC\u1091\'F3 \u1088\'F0\u1086\'EE\u1090\'F2. \u1054\'CE\u1090\'F2 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1075\'E3\u1088\'F0\u1091\'F3\u1079\'E7\u1082\'EA\u1080\'E8. \u1053\'CD\u1086\'EE \u1086\'EE\u1085\'ED \u1084\'EC\u1085\'ED\u1077\'E5 \u1085\'ED\u1088\'F0\u1072\'E0\u1074\'E2\u1080\'E8\u1090\'F2\u1089\'F1\u1103\'FF \hich\f1 \emdash \loch\f1  \u1074\'E2\u1089\'F1\u1077\'E5\u1075\'E3\u1076\'E4\u1072\'E0 \u1091\'F3\u1079\'E7\u1085\'ED\u1072\'E0\u1077\'E5\u1096\'F8\u1100\'FC \u1086\'EE\u1090\'F2 \u1085\'ED\u1077\'E5\u1075\'E3\u1086\'EE \u1095\'F7\u1090\'F2\u1086\'EE-\u1090\'F2\u1086\'EE \u1085\'ED\u1086\'EE\u1074\'E2\u1086\'EE\u1077\'E5, \u1085\'ED\u1080\'E8\u1082\'EA\u1072\'E0\u1082\'EA\u1080\'E8\u1093\'F5 \u1089\'F1\u1086\'EE\u1094\'F6\u1089\'F1\u1077\'E5\u1090\'F2\u1077\'E5\u1081\'E9 \u1085\'ED\u1077\'E5 \u1085\'ED\u1072\'E0\u1076\'E4\u1086\'EE, \u1052\'CC\u1080\'E8\u1096\'F8\u1072\'E0 \u1089\'F1\u1072\'E0\u1084\'EC \u1090\'F2\u1077\'E5\u1073\'E1\u1077\'E5 \u1074\'E2\u1089\'F1\u1077\'E5 \u1088\'F0\u1072\'E0\u1089\'F1\u1089\'F1\u1082\'EA\u1072\'E0\u1078\'E6\u1077\'E5\u1090\'F2 \u1080\'E8 \u1076\'E4\u1072\'E0\u1089\'F1\u1090\'F2 \u1089\'F1\u1074\'E2\u1086\'EE\u1081\'E9 \u1082\'EA\u1086\'EE\u1084\'EC\u1084\'EC\u1077\'E5\u1085\'ED\u1090\'F2\u1072\'E0\u1088\'F0\u1080\'E8\u1081\'E9. }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1072\'E0\u1082\'EA \u1090\'F2\u1072\'E0\u1084\'EC \u1090\'F2\u1074\'E2\u1086\'EE\u1081\'E9 \u1086\'EE\u1090\'F2\u1095\'F7\u1077\'E5\u1090\'F2? \hich\f1 \emdash \loch\f1  \u1089\'F1\u1087\'EF\u1088\'F0\u1072\'E0\u1096\'F8\u1080\'E8\u1074\'E2\u1072\'E0\u1077\'E5\u1090\'F2 \u1087\'EF\u1072\'E0\u1088\'F0\u1077\'E5\u1085\'ED\u1100\'FC, \u1091\'F3\u1078\'E6\u1077\'E5 \u1075\'E3\u1086\'EE\u1088\'F0\u1072\'E0\u1079\'E7\u1076\'E4\u1086\'EE \u1090\'F2\u1080\'E8\u1096\'F8\u1077\'E5, \u1074\'E2\u1089\'F1\u1077\'E5 \u1088\'F0\u1072\'E0\u1074\'E2\u1085\'ED\u1086\'EE \u1074\'E2\u1089\'F1\u1077\'E5 \u1086\'EE\u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB\u1100\'FC\u1085\'ED\u1099\'FB\u1077\'E5 \u1084\'EC\u1086\'EE\u1083\'EB\u1095\'F7\u1072\'E0\u1090\'F2. \u1057\'D1\u1083\'EB\u1091\'F3\u1096\'F8\u1072\'E0\u1102\'FE\u1090\'F2 \u1095\'F7\u1090\'F2\u1086\'EE-\u1090\'F2\u1086\'EE \u1074\'E2 \u1089\'F1\u1074\'E2\u1086\'EE\u1080\'E8\u1093\'F5 \u1085\'ED\u1072\'E0\u1091\'F3\u1096\'F8\u1085\'ED\u1080\'E8\u1082\'EA\u1072\'E0\u1093\'F5, \u1086\'EE\u1073\'E1 \u1101\'FD\u1090\'F2\u1086\'EE\u1084\'EC \u1075\'E3\u1086\'EE\u1074\'E2\u1086\'EE\u1088\'F0\u1080\'E8\u1090\'F2 \u1090\'F2\u1080\'E8\u1093\'F5\u1080\'E8\u1081\'E9 \u1096\'F8\u1077\'E5\u1083\'EB\u1077\'E5\u1089\'F1\u1090\'F2 \u1074\'E2\u1086\'EE\u1082\'EA\u1088\'F0\u1091\'F3\u1075\'E3. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1057\'D1\u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1080\'E8 \u1095\'F7\u1090\'F2\u1086\'EE \u1087\'EF\u1086\'EE\u1087\'EF\u1088\'F0\u1086\'EE\u1097\'F9\u1077\'E5, \hich\f1 \emdash \loch\f1  \u1087\'EF\u1088\'F0\u1086\'EE\u1073\'E1\u1086\'EE\u1088\'F0\u1084\'EC\u1086\'EE\u1090\'F2\u1072\'E0\u1083\'EB \u1103\'FF \u1074\'E2 \u1086\'EE\u1090\'F2\u1074\'E2\u1077\'E5\u1090\'F2. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1071\'DF-\u1072\'E0-\u1089\'F1\u1085\'ED\u1086\'EE\u8230\'85 \u1055\'CF\u1086\'EE \u1082\'EA\u1086\'EE\u1092\'F4\u1077\'E5? }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1047\'C7\u1072\'E0 \u1085\'ED\u1080\'E8\u1084\'EC \u1080\'E8 \u1089\'F1\u1087\'EF\u1091\'F3\u1089\'F1\u1082\'EA\u1072\'E0\u1102\'FE\u1089\'F1\u1100\'FC. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1057\'D1\u1077\'E5\u1075\'E3\u1086\'EE\u1076\'E4\u1085\'ED\u1103\'FF \u1085\'ED\u1072\'E0\u1096\'F8\u1080\'E8 \u1072\'E0\u1082\'EA\u1094\'F6\u1080\'E8\u1080\'E8 \u1091\'F3\u1087\'EF\u1072\'E0\u1083\'EB\u1080\'E8, \hich\f1 \emdash \loch\f1  \u1052\'CC\u1080\'E8\u1096\'F8\u1072\'E0 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1082\'EA\u1083\'EB\u1102\'FE\u1095\'F7\u1080\'E8\u1083\'EB\u1089\'F1\u1103\'FF \u1085\'ED\u1072\'E0 \u1089\'F1\u1074\'E2\u1086\'EE\u1076\'E4\u1082\'EA\u1091\'F3 \u1085\'ED\u1086\'EE\u1074\'E2\u1086\'EE\u1089\'F1\u1090\'F2\u1077\'E5\u1081\'E9. \u1069\'DD\u1090\'F2\u1086\'EE \u1091\'F3 \u1085\'ED\u1077\'E5\u1075\'E3\u1086\'EE \u1087\'EF\u1086\'EE\u1083\'EB\u1091\'F3\u1095\'F7\u1072\'E0\u1077\'E5\u1090\'F2\u1089\'F1\u1103\'FF \u1083\'EB\u1091\'F3\u1095\'F7\u1096\'F8\u1077\'E5, \u1095\'F7\u1077\'E5\u1084\'EC \u1073\'E1\u1086\'EE\u1083\'EB\u1090\'F2\u1086\'EE\u1074\'E2\u1085\'ED\u1103\'FF, \hich\f1 \emdash \loch\f1  \u1082\'EA\u1090\'F2\u1086\'EE-\u1090\'F2\u1086\'EE \u1089\'F1\u1073\'E1\u1088\'F0\u1086\'EE\u1089\'F1\u1080\'E8\u1083\'EB \u1089\'F1\u1074\'E2\u1086\'EE\u1081\'E9 \u1087\'EF\u1072\'E0\u1082\'EA\u1077\'E5\u1090\'F2. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1052\'CC\u1099\'FB \u1080\'E8\u1076\'E4\u1077\'E5\u1084\'EC \u1085\'ED\u1072\'E0 \u1076\'E4\u1085\'ED\u1086\'EE, \hich\f1 \emdash \loch\f1  \u1093\'F5\u1084\'EC\u1099\'FB\u1082\'EA\u1085\'ED\u1091\'F3\u1083\'EB \u1103\'FF, \u1093\'F5\u1086\'EE\u1090\'F2\u1103\'FF \u1085\'ED\u1077\'E5 \u1096\'F8\u1091\'F3\u1090\'F2\u1080\'E8\u1083\'EB. \u1061\'D5\u1086\'EE\u1076\'E4\u1080\'E8\u1083\'EB\u1080\'E8 \u1089\'F1\u1083\'EB\u1091\'F3\u1093\'F5\u1080\'E8 \u1086\'EE \u1087\'EF\u1086\'EE\u1075\'E3\u1083\'EB\u1086\'EE\u1097\'F9\u1077\'E5\u1085\'ED\u1080\'E8\u1080\'E8 \u1082\'EA\u1086\'EE\u1085\'ED\u1082\'EA\u1091\'F3\u1088\'F0\u1077\'E5\u1085\'ED\u1090\'F2\u1086\'EE\u1084\'EC, \u1084\'EC\u1099\'FB \u1091\'F3\u1078\'E6\u1077\'E5 \u1074\'E2\u1090\'F2\u1086\'EE\u1088\'F0\u1086\'EE\u1081\'E9 \u1075\'E3\u1086\'EE\u1076\'E4 \u1091\'F3\u1073\'E1\u1099\'FB\u1090\'F2\u1086\'EE\u1095\'F7\u1085\'ED\u1099\'FB, \u1087\'EF\u1086\'EE\u1075\'E3\u1088\'F0\u1103\'FF\u1079\'E7\u1083\'EB\u1080\'E8 \u1074\'E2 \u1076\'E4\u1086\'EE\u1083\'EB\u1075\'E3\u1072\'E0\u1093\'F5, \u1085\'ED\u1086\'EE \u1076\'E4\u1077\'E5\u1088\'F0\u1078\'E6\u1080\'E8\u1084\'EC\u1089\'F1\u1103\'FF. \u1040\'C0 \u1082\'EA\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0-\u1090\'F2\u1086\'EE \u1073\'E1\u1099\'FB\u1083\'EB\u1080\'E8 \u1083\'EB\u1080\'E8\u1076\'E4\u1077\'E5\u1088\'F0\u1072\'E0\u1084\'EC\u1080\'E8. \u1050\'CA\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0-\u1090\'F2\u1086\'EE\u8230\'85 }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1044\'C4\u1074\'E2\u1077\'E5\u1088\'F0\u1080\'E8 \u1083\'EB\u1080\'E8\u1092\'F4\u1090\'F2\u1072\'E0 \u1086\'EE\u1090\'F2\u1082\'EA\u1088\'F0\u1099\'FB\u1074\'E2\u1072\'E0\u1102\'FE\u1090\'F2\u1089\'F1\u1103\'FF. \u1051\'CB\u1102\'FE\u1076\'E4\u1080\'E8 \u1089\'F1\u1090\'F2\u1088\'F0\u1077\'E5\u1084\'EC\u1080\'E8\u1090\'F2\u1077\'E5\u1083\'EB\u1100\'FC\u1085\'ED\u1086\'EE \u1074\'E2\u1099\'FB\u1093\'F5\u1086\'EE\u1076\'E4\u1103\'FF\u1090\'F2, \u1103\'FF \u1073\'E1\u1099\'FB \u1076\'E4\u1072\'E0\u1078\'E6\u1077\'E5 \u1089\'F1\u1082\'EA\u1072\'E0\u1079\'E7\u1072\'E0\u1083\'EB, \u1074\'E2\u1099\'FB\u1083\'EB\u1077\'E5\u1090\'F2\u1072\'E0\u1102\'FE\u1090\'F2. \u1044\'C4\u1083\'EB\u1103\'FF \u1084\'EC\u1085\'ED\u1086\'EE\u1075\'E3\u1080\'E8\u1093\'F5 \u1080\'E8\u1079\'E7 \u1085\'ED\u1080\'E8\u1093\'F5 \u1082\'EA\u1072\'E0\u1078\'E6\u1076\'E4\u1072\'E0\u1103\'FF \u1089\'F1\u1077\'E5\u1082\'EA\u1091\'F3\u1085\'ED\u1076\'E4\u1072\'E0 \u1076\'E4\u1086\'EE\u1088\'F0\u1086\'EE\u1075\'E3\u1072\'E0. \u1052\'CC\u1099\'FB \u1089\'F1 \u1052\'CC\u1080\'E8\u1096\'F8\u1077\'E5\u1081\'E9 \u1074\'E2\u1099\'FB\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1084\'EC \u1080\'E8\u1079\'E7 \u1083\'EB\u1080\'E8\u1092\'F4\u1090\'F2\u1072\'E0 \u1087\'EF\u1086\'EE\u1089\'F1\u1083\'EB\u1077\'E5\u1076\'E4\u1085\'ED\u1080\'E8\u1084\'EC\u1080\'E8. \u1055\'CF\u1088\'F0\u1103\'FF\u1084\'EC\u1086\'EE, \u1087\'EF\u1086\'EE\u1090\'F2\u1086\'EE\u1084\'EC \u1085\'ED\u1072\'E0\u1087\'EF\u1088\'F0\u1072\'E0\u1074\'E2\u1086\'EE, \u1089\'F1\u1085\'ED\u1086\'EE\u1074\'E2\u1072\'E0 \u1087\'EF\u1088\'F0\u1103\'FF\u1084\'EC\u1086\'EE \u1080\'E8 \u1074\'E2\u1086\'EE\u1090\'F2 \hich\f1 \emdash \loch\f1  \u1082\'EA\u1072\'E0\u1092\'F4\u1077\'E5\u1090\'F2\u1077\'E5\u1088\'F0\u1080\'E8\u1081\'E9. \u1058\'D2\u1091\'F3\u1076\'E4\u1072\'E0 \u1074\'E2\u1084\'EC\u1077\'E5\u1089\'F1\u1090\'F2\u1077\'E5 \u1089\'F1 \u1085\'ED\u1072\'E0\u1084\'EC\u1080\'E8 \u1080\'E8\u1076\'E4\u1077\'E5\u1090\'F2 \u1077\'E5\u1097\'F9\u1077\'E5 \u1082\'EA\u1091\'F3\u1095\'F7\u1072\'E0 \u1085\'ED\u1072\'E0\u1088\'F0\u1086\'EE\u1076\'E4\u1091\'F3. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1057\'D1\u1083\'EB\u1099\'FB\u1096\'F8\u1072\'E0\u1083\'EB \u1087\'EF\u1088\'F0\u1086\'EE \u1079\'E7\u1077\'E5\u1084\'EC\u1083\'EB\u1077\'E5\u1090\'F2\u1088\'F0\u1103\'FF\u1089\'F1\u1077\'E5\u1085\'ED\u1080\'E8\u1077\'E5 \u1074\'E2 \u1071\'DF\u1087\'EF\u1086\'EE\u1085\'ED\u1080\'E8\u1080\'E8? \hich\f1 \emdash \loch\f1  \u1089\'F1\u1086\'EE\u1089\'F1\u1077\'E5\u1076\'E4 \u1089\'F1\u1085\'ED\u1086\'EE\u1074\'E2\u1072\'E0 \u1089\'F1\u1084\'EC\u1077\'E5\u1085\'ED\u1080\'E8\u1083\'EB \u1090\'F2\u1077\'E5\u1084\'EC\u1091\'F3. \u1055\'CF\u1103\'FF\u1083\'EB\u1103\'FF\u1089\'F1\u1100\'FC \u1074\'E2 \u1090\'F2\u1077\'E5\u1083\'EB\u1077\'E5\u1092\'F4\u1086\'EE\u1085\'ED, \u1086\'EE\u1085\'ED \u1096\'F8\u1077\'E5\u1083\'EB, \u1089\'F1\u1083\'EB\u1086\'EE\u1074\'E2\u1085\'ED\u1086\'EE \u1085\'ED\u1072\'E0 \u1072\'E0\u1074\'E2\u1090\'F2\u1086\'EE\u1084\'EC\u1072\'E0\u1090\'F2\u1077\'E5 \u1086\'EE\u1075\'E3\u1080\'E8\u1073\'E1\u1072\'E0\u1103\'FF \u1083\'EB\u1102\'FE\u1076\'E4\u1077\'E5\u1081\'E9, \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1099\'FB\u1077\'E5 \u1076\'E4\u1074\'E2\u1080\'E8\u1075\'E3\u1072\'E0\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1077\'E5\u1084\'EC\u1091\'F3 \u1085\'ED\u1072\'E0\u1074\'E2\u1089\'F1\u1090\'F2\u1088\'F0\u1077\'E5\u1095\'F7\u1091\'F3. \u1042\'C2\u1089\'F1\u1077\'E5\u1075\'E3\u1076\'E4\u1072\'E0 \u1087\'EF\u1086\'EE\u1088\'F0\u1072\'E0\u1078\'E6\u1072\'E0\u1083\'EB\u1089\'F1\u1103\'FF \u1101\'FD\u1090\'F2\u1086\'EE\u1081\'E9 \u1089\'F1\u1087\'EF\u1086\'EE\u1089\'F1\u1086\'EE\u1073\'E1\u1085\'ED\u1086\'EE\u1089\'F1\u1090\'F2\u1080\'E8 \u1082\'EA\u1086\'EE\u1085\'ED\u1090\'F2\u1077\'E5\u1085\'ED\u1090\'F2-\u1079\'E7\u1072\'E0\u1074\'E2\u1080\'E8\u1089\'F1\u1080\'E8\u1084\'EC\u1099\'FB\u1093\'F5. \hich\f1 \emdash \loch\f1  \u1058\'D2\u1072\'E0\u1084\'EC \u1087\'EF\u1088\'F0\u1103\'FF\u1084\'EC \u1078\'E6\u1077\'E5\u1089\'F1\u1090\'F2\u1100\'FC, \u1082\'EA\u1072\'E0\u1082\'EA \u1087\'EF\u1103\'FF\u1090\'F2\u1100\'FC \u1083\'EB\u1077\'E5\u1090\'F2 \u1085\'ED\u1072\'E0\u1079\'E7\u1072\'E0\u1076\'E4. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1059\'D3\u1078\'E6\u1072\'E0\u1089\'F1, \hich\f1 \emdash \loch\f1  \u1073\'E1\u1077\'E5\u1079\'E7\u1091\'F3\u1095\'F7\u1072\'E0\u1089\'F1\u1090\'F2\u1085\'ED\u1086\'EE \u1073\'E1\u1088\'F0\u1086\'EE\u1089\'F1\u1072\'E0\u1102\'FE \u1103\'FF, \u1074\'E2\u1089\'F1\u1087\'EF\u1086\'EE\u1084\'EC\u1080\'E8\u1085\'ED\u1072\'E0\u1103\'FF \u1082\'EA\u1072\'E0\u1076\'E4\u1088\'F0\u1099\'FB \u1076\'E4\u1086\'EE\u1082\'EA\u1091\'F3\u1084\'EC\u1077\'E5\u1085\'ED\u1090\'F2\u1072\'E0\u1083\'EB\u1082\'EA\u1080\'E8, \u1075\'E3\u1076\'E4\u1077\'E5 \u1074\'E2\u1086\'EE\u1076\'E4\u1072\'E0 \u1086\'EE\u1084\'EC\u1099\'FB\u1074\'E2\u1072\'E0\u1083\'EB\u1072\'E0 \u1086\'EE\u1075\'E3\u1088\'F0\u1086\'EE\u1084\'EC\u1085\'ED\u1099\'FB\u1077\'E5 \u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1085\'ED\u1089\'F1\u1090\'F2\u1074\'E2\u1072\'E0, \u1087\'EF\u1088\'F0\u1080\'E8\u1095\'F7\u1080\'E8\u1085\'ED\u1103\'FF\u1103\'FF \u1091\'F3\u1073\'E1\u1099\'FB\u1090\'F2\u1082\'EA\u1080\'E8 \u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1090\'F2\u1099\'FB\u1084\'EC \u1092\'F4\u1077\'E5\u1088\'F0\u1084\'EC\u1077\'E5\u1088\'F0\u1072\'E0\u1084\'EC. \hich\f1 \emdash \loch\f1  \u1057\'D1\u1082\'EA\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1073\'E1\u1072\'E0\u1083\'EB\u1083\'EB\u1086\'EE\u1074\'E2? }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1044\'C4\u1074\'E2\u1077\'E5\u1085\'ED\u1072\'E0\u1076\'E4\u1094\'F6\u1072\'E0\u1090\'F2\u1100\'FC! }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1052\'CC\u1099\'FB \u1087\'EF\u1088\'F0\u1080\'E8\u1073\'E1\u1083\'EB\u1080\'E8\u1078\'E6\u1072\'E0\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1082\'EA \u1082\'EA\u1072\'E0\u1092\'F4\u1077\'E5\u1090\'F2\u1077\'E5\u1088\'F0\u1080\'E8\u1102\'FE, \u1076\'E4\u1086\'EE \u1085\'ED\u1072\'E0\u1089\'F1 \u1091\'F3\u1078\'E6\u1077\'E5 \u1076\'E4\u1086\'EE\u1085\'ED\u1086\'EE\u1089\'F1\u1080\'E8\u1083\'EB\u1089\'F1\u1103\'FF \u1087\'EF\u1088\'F0\u1080\'E8\u1103\'FF\u1090\'F2\u1085\'ED\u1099\'FB\u1081\'E9 \u1079\'E7\u1072\'E0\u1087\'EF\u1072\'E0\u1093\'F5 \u1077\'E5\u1076\'E4\u1099\'FB \u1080\'E8 \u1096\'F8\u1091\'F3\u1084\'EC \u1083\'EB\u1102\'FE\u1076\'E4\u1077\'E5\u1081\'E9. }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1072\'E0\u1092\'F4\u1077\'E5\u1090\'F2\u1077\'E5\u1088\'F0\u1080\'E8\u1081\'E9. \u1069\'DD\u1090\'F2\u1086\'EE \u1083\'EB\u1091\'F3\u1095\'F7\u1096\'F8\u1077\'E5\u1077\'E5 \u1084\'EC\u1077\'E5\u1089\'F1\u1090\'F2\u1086\'EE \u1074\'E2 \u1079\'E7\u1076\'E4\u1072\'E0\u1085\'ED\u1080\'E8\u1080\'E8, \u1087\'EF\u1088\'F0\u1080\'E8\u1103\'FF\u1090\'F2\u1085\'ED\u1086\'EE \u1082\'EA\u1086\'EE\u1085\'ED\u1090\'F2\u1088\'F0\u1072\'E0\u1089\'F1\u1090\'F2\u1080\'E8\u1088\'F0\u1091\'F3\u1102\'FE\u1097\'F9\u1077\'E5\u1077\'E5 \u1089\'F1 \u1086\'EE\u1089\'F1\u1085\'ED\u1086\'EE\u1074\'E2\u1085\'ED\u1099\'FB\u1084\'EC \u1089\'F1\u1077\'E5\u1088\'F0\u1099\'FB\u1084\'EC \u1076\'E4\u1080\'E8\u1079\'E7\u1072\'E0\u1081\'E9\u1085\'ED\u1086\'EE\u1084\'EC. \u1047\'C7\u1076\'E4\u1077\'E5\u1089\'F1\u1100\'FC \u1074\'E2\u1089\'F1\u1077\'E5 \u1089\'F1\u1074\'E2\u1077\'E5\u1090\'F2\u1083\'EB\u1086\'EE\u1077\'E5 \u1080\'E8 \u1087\'EF\u1088\'F0\u1080\'E8\u1103\'FF\u1090\'F2\u1085\'ED\u1086\'EE\u1077\'E5, \u1083\'EB\u1102\'FE\u1076\'E4\u1080\'E8 \u1089\'F1\u1087\'EF\u1086\'EE\u1082\'EA\u1086\'EE\u1081\'E9\u1085\'ED\u1086\'EE \u1088\'F0\u1072\'E0\u1079\'E7\u1075\'E3\u1086\'EE\u1074\'E2\u1072\'E0\u1088\'F0\u1080\'E8\u1074\'E2\u1072\'E0\u1102\'FE\u1090\'F2 \u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3 \u1089\'F1 \u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3\u1086\'EE\u1084\'EC \u1080\'E8 \u1085\'ED\u1072\'E0\u1087\'EF\u1086\'EE\u1083\'EB\u1085\'ED\u1103\'FF\u1102\'FE\u1090\'F2 \u1089\'F1\u1074\'E2\u1086\'EE\u1080\'E8 \u1078\'E6\u1077\'E5\u1083\'EB\u1091\'F3\u1076\'E4\u1082\'EA\u1080\'E8 \u1090\'F2\u1077\'E5\u1084\'EC, \u1095\'F7\u1090\'F2\u1086\'EE \u1083\'EB\u1102\'FE\u1073\'E1\u1103\'FF\u1090\'F2. \u1040\'C0 \u1077\'E5\u1097\'F9\u1077\'E5, \u1090\'F2\u1091\'F3\u1090\'F2 \u1085\'ED\u1077\'E5\u1087\'EF\u1083\'EB\u1086\'EE\u1093\'F5\u1086\'EE\u1081\'E9 \u1082\'EA\u1086\'EE\u1092\'F4\u1077\'E5. \u1071\'DF \u1087\'EF\u1086\'EE\u1076\'E4\u1093\'F5\u1086\'EE\u1078\'E6\u1091\'F3 \u1082\'EA \u1086\'EE\u1076\'E4\u1085\'ED\u1086\'EE\u1084\'EC\u1091\'F3 \u1080\'E8\u1079\'E7 \u1072\'E0\u1074\'E2\u1090\'F2\u1086\'EE\u1084\'EC\u1072\'E0\u1090\'F2\u1086\'EE\u1074\'E2 \u1080\'E8 \u1074\'E2\u1099\'FB\u1078\'E6\u1080\'E8\u1076\'E4\u1072\'E0\u1102\'FE \u1086\'EE\u1095\'F7\u1077\'E5\u1088\'F0\u1077\'E5\u1076\'E4\u1100\'FC, \u1079\'E7\u1072\'E0 \u1084\'EC\u1085\'ED\u1086\'EE\u1081\'E9 \u1074\'E2\u1089\'F1\u1090\'F2\u1072\'E0\u1077\'E5\u1090\'F2 \u1052\'CC\u1080\'E8\u1096\'F8\u1072\'E0. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1053\'CD\u1072\'E0 \u1084\'EC\u1086\'EE\u1105\'B8 \u1087\'EF\u1088\'F0\u1072\'E0\u1074\'E2\u1086\'EE\u1077\'E5 \u1087\'EF\u1083\'EB\u1077\'E5\u1095\'F7\u1086\'EE \u1082\'EA\u1090\'F2\u1086\'EE-\u1090\'F2\u1086\'EE \u1082\'EA\u1083\'EB\u1072\'E0\u1076\'E4\u1077\'E5\u1090\'F2 \u1088\'F0\u1091\'F3\u1082\'EA\u1091\'F3, \u1103\'FF \u1086\'EE\u1073\'E1\u1086\'EE\u1088\'F0\u1072\'E0\u1095\'F7\u1080\'E8\u1074\'E2\u1072\'E0\u1102\'FE\u1089\'F1\u1100\'FC. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1055\'CF\u1086\'EE\u1083\'EB\u1080\'E8\u1085\'ED\u1072\'E0. \u1061\'D5\u1086\'EE\u1088\'F0\u1086\'EE\u1096\'F8\u1077\'E5\u1085\'ED\u1100\'FC\u1082\'EA\u1072\'E0\u1103\'FF \u1076\'E4\u1077\'E5\u1074\'E2\u1091\'F3\u1096\'F8\u1082\'EA\u1072\'E0 \u1089\'F1 \u1087\'EF\u1091\'F3\u1093\'F5\u1083\'EB\u1099\'FB\u1084\'EC\u1080\'E8 \u1097\'F9\u1077\'E5\u1095\'F7\u1082\'EA\u1072\'E0\u1084\'EC\u1080\'E8 \u1080\'E8 \u1085\'ED\u1080\'E8\u1079\'E7\u1082\'EA\u1080\'E8\u1084\'EC \u1088\'F0\u1086\'EE\u1089\'F1\u1090\'F2\u1086\'EE\u1084\'EC. \u1048\'C8\u1079\'E7-\u1079\'E7\u1072\'E0 \u1089\'F1\u1074\'E2\u1077\'E5\u1090\'F2\u1083\'EB\u1086\'EE-\u1088\'F0\u1091\'F3\u1089\'F1\u1099\'FB\u1093\'F5 \u1074\'E2\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1 \u1085\'ED\u1072\'E0\u1087\'EF\u1086\'EE\u1084\'EC\u1080\'E8\u1085\'ED\u1072\'E0\u1077\'E5\u1090\'F2 \u1084\'EC\u1085\'ED\u1077\'E5 \u1093\'F5\u1086\'EE\u1084\'EC\u1103\'FF\u1095\'F7\u1082\'EA\u1072\'E0, \u1074\'E2\u1089\'F1\u1077\'E5\u1075\'E3\u1076\'E4\u1072\'E0 \u1090\'F2\u1072\'E0\u1082\'EA\u1072\'E0\u1103\'FF \u1103\'FF\u1088\'F0\u1082\'EA\u1072\'E0\u1103\'FF \u1080\'E8 \u1076\'E4\u1086\'EE\u1073\'E1\u1088\'F0\u1072\'E0\u1103\'FF. \u1050\'CA\u1088\'F0\u1091\'F3\u1078\'E6\u1080\'E8\u1090\'F2 \u1074\'E2\u1086\'EE\u1082\'EA\u1088\'F0\u1091\'F3\u1075\'E3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF, \u1082\'EA\u1072\'E0\u1082\'EA \u1087\'EF\u1095\'F7\u1077\'E5\u1083\'EB\u1072\'E0 \u1074\'E2\u1086\'EE\u1082\'EA\u1088\'F0\u1091\'F3\u1075\'E3 \u1094\'F6\u1074\'E2\u1077\'E5\u1090\'F2\u1082\'EA\u1072\'E0. \u1057\'D1\u1080\'E8\u1076\'E4\u1080\'E8\u1090\'F2 \u1089\'F1\u1083\'EB\u1077\'E5\u1074\'E2\u1072\'E0, \u1074\'E2\u1089\'F1\u1077\'E5\u1075\'E3\u1076\'E4\u1072\'E0 \u1076\'E4\u1072\'E0\u1088\'F0\u1080\'E8\u1090\'F2 \u1086\'EE\u1090\'F2\u1082\'EA\u1088\'F0\u1099\'FB\u1090\'F2\u1082\'EA\u1080\'E8 \u1085\'ED\u1072\'E0 \u1087\'EF\u1088\'F0\u1072\'E0\u1079\'E7\u1076\'E4\u1085\'ED\u1080\'E8\u1082\'EA\u1080\'E8 \u1080\'E8 \u1089\'F1\u1074\'E2\u1103\'FF\u1079\'E7\u1072\'E0\u1083\'EB\u1072\'E0 \u1084\'EC\u1085\'ED\u1077\'E5 \u1089\'F1\u1074\'E2\u1080\'E8\u1090\'F2\u1077\'E5\u1088\'F0. \u1071\'DF \u1075\'E3\u1086\'EE\u1074\'E2\u1086\'EE\u1088\'F0\u1080\'E8\u1083\'EB \u1055\'CF\u1086\'EE\u1083\'EB\u1080\'E8\u1085\'ED\u1077\'E5, \u1095\'F7\u1090\'F2\u1086\'EE \u1086\'EE\u1085\'ED\u1072\'E0 \u1085\'ED\u1077\'E5 \u1074\'E2 \u1084\'EC\u1086\'EE\u1077\'E5\u1084\'EC \u1074\'E2\u1082\'EA\u1091\'F3\u1089\'F1\u1077\'E5, \u1085\'ED\u1086\'EE \u1086\'EE\u1085\'ED\u1072\'E0 \u1085\'ED\u1077\'E5 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB\u1072\'E0 \u1083\'EB\u1077\'E5\u1090\'F2\u1072\'E0\u1090\'F2\u1100\'FC \u1074\'E2\u1086\'EE\u1082\'EA\u1088\'F0\u1091\'F3\u1075\'E3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF. \u1071\'DF \u1076\'E4\u1072\'E0\u1078\'E6\u1077\'E5 \u1087\'EF\u1086\'EE\u1083\'EB\u1102\'FE\u1073\'E1\u1080\'E8\u1083\'EB \u1077\'E5\u1105\'B8, \u1085\'ED\u1086\'EE \u1082\'EA\u1072\'E0\u1082\'EA \u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3\u1072\'E0. \u1054\'CE\u1085\'ED\u1072\'E0 \u1086\'EE\u1076\'E4\u1085\'ED\u1072\'E0 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1075\'E3\u1088\'F0\u1077\'E5\u1077\'E5\u1090\'F2 \u1089\'F1\u1074\'E2\u1086\'EE\u1077\'E5\u1081\'E9 \u1079\'E7\u1072\'E0\u1073\'E1\u1086\'EE\u1090\'F2\u1086\'EE\u1081\'E9, \u1080\'E8\u1089\'F1\u1082\'EA\u1088\'F0\u1077\'E5\u1085\'ED\u1085\'ED\u1077\'E5 \u1080\'E8 \u1089\'F1\u1083\'EB\u1077\'E5\u1087\'EF\u1086\'EE, \u1093\'F5\u1086\'EE\u1090\'F2\u1103\'FF \u1084\'EC\u1086\'EE\u1078\'E6\u1077\'E5\u1090\'F2 \u1085\'ED\u1072\'E0\u1081\'E9\u1090\'F2\u1080\'E8 \u1089\'F1\u1077\'E5\u1073\'E1\u1077\'E5 \u1086\'EE\u1073\'E1\u1098\'FA\u1077\'E5\u1082\'EA\u1090\'F2 \u1087\'EF\u1086\'EE\u1083\'EB\u1091\'F3\u1095\'F7\u1096\'F8\u1077\'E5. \u1055\'CF\u1086\'EE\u1083\'EB\u1103\'FF \u1089\'F1\u1072\'E0\u1084\'EC\u1072\'E0 \u1089\'F1\u1076\'E4\u1077\'E5\u1083\'EB\u1072\'E0\u1083\'EB\u1072\'E0 \u1101\'FD\u1090\'F2\u1086\'EE\u1090\'F2 \u1074\'E2\u1099\'FB\u1073\'E1\u1086\'EE\u1088\'F0, \u1072\'E0 \u1103\'FF \u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1090\'F2\u1086\'EE \u1087\'EF\u1088\'F0\u1080\'E8\u1085\'ED\u1103\'FF\u1083\'EB \u1077\'E5\u1075\'E3\u1086\'EE.}
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1057\'D1\u1083\'EB\u1077\'E5\u1075\'E3\u1082\'EA\u1072\'E0 \u1091\'F3\u1083\'EB\u1099\'FB\u1073\'E1\u1072\'E0\u1102\'FE\u1089\'F1\u1100\'FC \u1080\'E8 \u1091\'F3\u1073\'E1\u1080\'E8\u1088\'F0\u1072\'E0\u1102\'FE \u1077\'E5\u1105\'B8 \u1088\'F0\u1091\'F3\u1082\'EA\u1091\'F3. }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1058\'D2\u1091\'F3\u1090\'F2 \u1085\'ED\u1077\'E5\u1076\'E4\u1072\'E0\u1083\'EB\u1077\'E5\u1082\'EA\u1086\'EE \u1088\'F0\u1072\'E0\u1079\'E7\u1074\'E2\u1083\'EB\u1077\'E5\u1082\'EA\u1072\'E0\u1090\'F2\u1077\'E5\u1083\'EB\u1100\'FC\u1085\'ED\u1099\'FB\u1081\'E9 \u1094\'F6\u1077\'E5\u1085\'ED\u1090\'F2\u1088\'F0 \u1086\'EE\u1090\'F2\u1082\'EA\u1088\'F0\u1099\'FB\u1083\'EB\u1089\'F1\u1103\'FF, \u1089\'F1\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1084\'EC \u1085\'ED\u1072\'E0 \u8220\'93\u1044\'C4\u1078\'E6\u1086\'EE\u1081\'E9\u8221\'94? \hich\f1 \emdash \loch\f1  \u1089\'F1\u1087\'EF\u1088\'F0\u1072\'E0\u1096\'F8\u1080\'E8\u1074\'E2\u1072\'E0\u1077\'E5\u1090\'F2 \u1055\'CF\u1086\'EE\u1083\'EB\u1080\'E8\u1085\'ED\u1072\'E0, \u1086\'EE\u1089\'F1\u1083\'EB\u1077\'E5\u1087\'EF\u1083\'EB\u1103\'FF\u1103\'FF \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1089\'F1\u1074\'E2\u1086\'EE\u1080\'E8\u1084\'EC\u1080\'E8 \u1073\'E1\u1077\'E5\u1083\'EB\u1099\'FB\u1084\'EC\u1080\'E8 \u1079\'E7\u1091\'F3\u1073\'E1\u1072\'E0\u1084\'EC\u1080\'E8. \u1053\'CD\u1077\'E5 \u1090\'F2\u1086\'EE, \u1095\'F7\u1090\'F2\u1086\'EE \u1084\'EC\u1086\'EE\u1080\'E8 \hich\f1 \emdash \loch\f1  \u1078\'E6\u1077\'E5\u1083\'EB\u1090\'F2\u1099\'FB\u1077\'E5 \u1086\'EE\u1090\'F2 \u1073\'E1\u1077\'E5\u1089\'F1\u1082\'EA\u1086\'EE\u1085\'ED\u1077\'E5\u1095\'F7\u1085\'ED\u1086\'EE\u1075\'E3\u1086\'EE \u1082\'EA\u1086\'EE\u1092\'F4\u1077\'E5. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1041\'C1\u1080\'E8\u1086\'EE\u1075\'E3\u1088\'F0\u1072\'E0\u1092\'F4\u1080\'E8\u1103\'FF, \hich\f1 \emdash \loch\f1  \u1086\'EE\u1090\'F2\u1082\'EA\u1083\'EB\u1080\'E8\u1082\'EA\u1072\'E0\u1077\'E5\u1090\'F2\u1089\'F1\u1103\'FF \u1052\'CC\u1080\'E8\u1096\'F8\u1072\'E0, \hich\f1 \emdash \loch\f1  \u1088\'F0\u1077\'E5\u1081\'E9\u1090\'F2\u1080\'E8\u1085\'ED\u1075\'E3 \u1087\'EF\u1086\'EE\u1082\'EA\u1072\'E0 \u1090\'F2\u1072\'E0\u1082\'EA\u1086\'EE\u1081\'E9 \u1089\'F1\u1077\'E5\u1073\'E1\u1077\'E5, \u1085\'ED\u1086\'EE \u1103\'FF \u1073\'E1 \u1089\'F1\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1083\'EB. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1044\'C4\u1072\'E0, \u1084\'EC\u1086\'EE\u1078\'E6\u1085\'ED\u1086\'EE \u1087\'EF\u1086\'EE\u1081\'E9\u1090\'F2\u1080\'E8 \u1074\'E2\u1090\'F2\u1088\'F0\u1086\'EE\u1077\'E5\u1084\'EC! \hich\f1 \emdash \loch\f1  \u1086\'EE\u1073\'E1\u1088\'F0\u1072\'E0\u1076\'E4\u1086\'EE\u1074\'E2\u1072\'E0\u1083\'EB\u1072\'E0\u1089\'F1\u1100\'FC \u1089\'F1\u1086\'EE\u1089\'F1\u1077\'E5\u1076\'E4\u1082\'EA\u1072\'E0. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1071\'DF \u1086\'EE\u1090\'F2\u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1085\'ED\u1103\'FF\u1102\'FE\u1089\'F1\u1100\'FC \u1086\'EE\u1090\'F2 \u1080\'E8\u1093\'F5 \u1088\'F0\u1072\'E0\u1079\'E7\u1075\'E3\u1086\'EE\u1074\'E2\u1086\'EE\u1088\'F0\u1072\'E0, \u1074\'E2\u1079\'E7\u1103\'FF\u1074\'E2 \u1074\'E2 \u1088\'F0\u1091\'F3\u1082\'EA\u1080\'E8 \u1090\'F2\u1077\'E5\u1083\'EB\u1077\'E5\u1092\'F4\u1086\'EE\u1085\'ED. \u1048\'C8\u1084\'EC \u1084\'EC\u1086\'EE\u1077\'E5 \u1091\'F3\u1095\'F7\u1072\'E0\u1089\'F1\u1090\'F2\u1080\'E8\u1077\'E5 \u1090\'F2\u1077\'E5\u1087\'EF\u1077\'E5\u1088\'F0\u1100\'FC \u1085\'ED\u1077\'E5 \u1085\'ED\u1091\'F3\u1078\'E6\u1085\'ED\u1086\'EE. \u1043\'C3\u1083\'EB\u1103\'FF\u1076\'E4\u1080\'E8\u1096\'F8\u1100\'FC, \u1055\'CF\u1086\'EE\u1083\'EB\u1103\'FF \u1079\'E7\u1072\'E0\u1080\'E8\u1085\'ED\u1090\'F2\u1077\'E5\u1088\'F0\u1077\'E5\u1089\'F1\u1091\'F3\u1077\'E5\u1090\'F2\u1089\'F1\u1103\'FF \u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3\u1080\'E8\u1084\'EC \u1094\'F6\u1074\'E2\u1077\'E5\u1090\'F2\u1082\'EA\u1086\'EE\u1084\'EC, \u1073\'E1\u1086\'EE\u1083\'EB\u1077\'E5\u1077\'E5 \u1073\'E1\u1083\'EB\u1072\'E0\u1075\'E3\u1086\'EE\u1091\'F3\u1093\'F5\u1072\'E0\u1102\'FE\u1097\'F9\u1080\'E8\u1084\'EC. \u1051\'CB\u1080\'E8\u1089\'F1\u1090\'F2\u1072\'E0\u1102\'FE \u1083\'EB\u1077\'E5\u1085\'ED\u1090\'F2\u1091\'F3, \u1080\'E8\u1084\'EC\u1080\'E8\u1090\'F2\u1080\'E8\u1088\'F0\u1091\'F3\u1102\'FE \u1076\'E4\u1077\'E5\u1103\'FF\u1090\'F2\u1077\'E5\u1083\'EB\u1100\'FC\u1085\'ED\u1086\'EE\u1089\'F1\u1090\'F2\u1100\'FC, \u1087\'EF\u1086\'EE\u1082\'EA\u1072\'E0 \u1085\'ED\u1077\'E5 \u1086\'EE\u1082\'EA\u1072\'E0\u1079\'E7\u1099\'FB\u1074\'E2\u1072\'E0\u1102\'FE\u1089\'F1\u1100\'FC \u1087\'EF\u1088\'F0\u1103\'FF\u1084\'EC\u1086\'EE \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1076\'E4 \u1072\'E0\u1074\'E2\u1090\'F2\u1086\'EE\u1084\'EC\u1072\'E0\u1090\'F2\u1086\'EE\u1084\'EC. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1047\'C7\u1072\'E0\u1087\'EF\u1088\'F0\u1072\'E0\u1096\'F8\u1080\'E8\u1074\'E2\u1072\'E0\u1102\'FE \u1091\'F3 \u1084\'EC\u1072\'E0\u1096\'F8\u1080\'E8\u1085\'ED\u1099\'FB \u1072\'E0\u1084\'EC\u1077\'E5\u1088\'F0\u1080\'E8\u1082\'EA\u1072\'E0\u1085\'ED\u1086\'EE \u1073\'E1\u1077\'E5\u1079\'E7 \u1089\'F1\u1072\'E0\u1093\'F5\u1072\'E0\u1088\'F0\u1072\'E0, \u1083\'EB\u1077\'E5\u1079\'E7\u1091\'F3 \u1074\'E2 \u1082\'EA\u1072\'E0\u1088\'F0\u1084\'EC\u1072\'E0\u1085\'ED. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1072\'E0\u1088\'F0\u1084\'EC\u1072\'E0\u1085\'ED \u1087\'EF\u1091\'F3\u1089\'F1\u1090\'F2, \u1082\'EA\u1086\'EE\u1096\'F8\u1077\'E5\u1083\'EB\u1100\'FC\u1082\'EA\u1072\'E0 \u1085\'ED\u1077\'E5\u1090\'F2. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1043\'C3\u1086\'EE\u1089\'F1\u1087\'EF\u1086\'EE\u1076\'E4\u1080\'E8, \hich\f1 \emdash \loch\f1  \u1103\'FF \u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1102\'FE \u1087\'EF\u1086\'EE\u1090\'F2\u1086\'EE\u1083\'EB\u1086\'EE\u1082\'EA \u1082\'EA\u1088\'F0\u1077\'E5\u1084\'EC\u1086\'EE\u1074\'E2\u1086\'EE\u1075\'E3\u1086\'EE \u1094\'F6\u1074\'E2\u1077\'E5\u1090\'F2\u1072\'E0 \u1089\'F1 \u1076\'E4\u1077\'E5\u1089\'F1\u1103\'FF\u1090\'F2\u1082\'EA\u1072\'E0\u1084\'EC\u1080\'E8 \u1089\'F1\u1074\'E2\u1077\'E5\u1090\'F2\u1086\'EE\u1076\'E4\u1080\'E8\u1086\'EE\u1076\'E4\u1086\'EE\u1074\'E2, \hich\f1 \emdash \loch\f1  \u1087\'EF\u1086\'EE\u1095\'F7\u1077\'E5\u1084\'EC\u1091\'F3 \u1091\'F3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1090\'F2\u1072\'E0\u1082\'EA\u1072\'E0\u1103\'FF \u1076\'E4\u1099\'FB\u1088\'F0\u1103\'FF\u1074\'E2\u1072\'E0\u1103\'FF \u1073\'E1\u1072\'E0\u1096\'F8\u1082\'EA\u1072\'E0? }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1054\'CE\u1087\'EF\u1103\'FF\u1090\'F2\u1100\'FC \u1079\'E7\u1072\'E0\u1073\'E1\u1099\'FB\u1083\'EB \u1076\'E4\u1077\'E5\u1085\'ED\u1100\'FC\u1075\'E3\u1080\'E8 \u1074\'E2 \u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB\u1077\'E5? \hich\f1 \emdash \loch\f1  \u1052\'CC\u1080\'E8\u1096\'F8\u1072\'E0 \u1091\'F3\u1078\'E6\u1077\'E5 \u1076\'E4\u1088\'F0\u1091\'F3\u1078\'E6\u1077\'E5\u1083\'EB\u1102\'FE\u1073\'E1\u1085\'ED\u1086\'EE \u1087\'EF\u1088\'F0\u1086\'EE\u1090\'F2\u1103\'FF\u1075\'E3\u1080\'E8\u1074\'E2\u1072\'E0\u1077\'E5\u1090\'F2 \u1084\'EC\u1085\'ED\u1077\'E5 \u1084\'EC\u1077\'E5\u1083\'EB\u1082\'EA\u1091\'F3\u1102\'FE \u1082\'EA\u1091\'F3\u1087\'EF\u1102\'FE\u1088\'F0\u1091\'F3. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1057\'D1\u1087\'EF\u1072\'E0\u1089\'F1\u1080\'E8\u1073\'E1\u1086\'EE, \hich\f1 \emdash \loch\f1  \u1103\'FF \u1080\'E8\u1089\'F1\u1082\'EA\u1088\'F0\u1077\'E5\u1085\'ED\u1085\'ED\u1077\'E5 \u1091\'F3\u1083\'EB\u1099\'FB\u1073\'E1\u1072\'E0\u1102\'FE\u1089\'F1\u1100\'FC, \hich\f1 \emdash \loch\f1  \u1087\'EF\u1086\'EE\u1076\'E4\u1085\'ED\u1080\'E8\u1084\'EC\u1077\'E5\u1084\'EC\u1089\'F1\u1103\'FF, \u1103\'FF \u1074\'E2\u1077\'E5\u1088\'F0\u1085\'ED\u1091\'F3. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1059\'D3\u1075\'E3\u1086\'EE\u1097\'F9\u1072\'E0\u1102\'FE! \hich\f1 \emdash \loch\f1  \u1082\'EA\u1086\'EE\u1083\'EB\u1083\'EB\u1077\'E5\u1075\'E3\u1072\'E0 \u1084\'EC\u1072\'E0\u1093\'F5\u1072\'E0\u1077\'E5\u1090\'F2 \u1088\'F0\u1091\'F3\u1082\'EA\u1086\'EE\u1081\'E9. }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1040\'C0\u1074\'E2\u1090\'F2\u1086\'EE\u1084\'EC\u1072\'E0\u1090\'F2 \u1078\'E6\u1072\'E0\u1076\'E4\u1085\'ED\u1086\'EE \u1086\'EE\u1090\'F2\u1085\'ED\u1080\'E8\u1084\'EC\u1072\'E0\u1077\'E5\u1090\'F2 \u1091\'F3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1073\'E1\u1072\'E0\u1085\'ED\u1082\'EA\u1085\'ED\u1086\'EE\u1090\'F2\u1091\'F3 \u1080\'E8 \u1085\'ED\u1072\'E0\u1095\'F7\u1080\'E8\u1085\'ED\u1072\'E0\u1077\'E5\u1090\'F2 \u1087\'EF\u1088\'F0\u1086\'EE\u1080\'E8\u1079\'E7\u1074\'E2\u1086\'EE\u1076\'E4\u1080\'E8\u1090\'F2\u1100\'FC \u1078\'E6\u1080\'E8\u1079\'E7\u1085\'ED\u1077\'E5\u1085\'ED\u1085\'ED\u1086\'EE \u1074\'E2\u1072\'E0\u1078\'E6\u1085\'ED\u1099\'FB\u1081\'E9 \u1076\'E4\u1083\'EB\u1103\'FF \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1085\'ED\u1072\'E0\u1087\'EF\u1080\'E8\u1090\'F2\u1086\'EE\u1082\'EA. \u1063\'D7\u1077\'E5\u1088\'F0\u1077\'E5\u1079\'E7 30 \u1089\'F1\u1077\'E5\u1082\'EA\u1091\'F3\u1085\'ED\u1076\'E4 \u1086\'EE\u1085\'ED \u1091\'F3\u1078\'E6\u1077\'E5 \u1091\'F3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1074\'E2 \u1088\'F0\u1091\'F3\u1082\'EA\u1072\'E0\u1093\'F5, \u1103\'FF \u1086\'EE\u1090\'F2\u1093\'F5\u1086\'EE\u1078\'E6\u1091\'F3 \u1074\'E2 \u1089\'F1\u1090\'F2\u1086\'EE\u1088\'F0\u1086\'EE\u1085\'ED\u1091\'F3. \u1044\'C4\u1077\'E5\u1083\'EB\u1072\'E0\u1102\'FE \u1085\'ED\u1077\'E5\u1073\'E1\u1086\'EE\u1083\'EB\u1100\'FC\u1096\'F8\u1086\'EE\u1081\'E9 \u1075\'E3\u1083\'EB\u1086\'EE\u1090\'F2\u1086\'EE\u1095\'F7\u1077\'E5\u1082\'EA. \u1043\'C3\u1086\'EE\u1088\'F0\u1103\'FF\u1095\'F7\u1086\'EE, \u1085\'ED\u1086\'EE \u1082\'EA\u1072\'E0\u1082\'EA \u1074\'E2\u1082\'EA\u1091\'F3\u1089\'F1\u1085\'ED\u1086\'EE! }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1043\'C3\u1086\'EE\u1089\'F1\u1087\'EF\u1086\'EE\u1076\'E4\u1080\'E8, \u1089\'F1\u1087\'EF\u1072\'E0\u1089\'F1\u1080\'E8\u1073\'E1\u1086\'EE \u1079\'E7\u1072\'E0 \u1101\'FD\u1090\'F2\u1086\'EE \u1073\'E1\u1083\'EB\u1072\'E0\u1078\'E6\u1077\'E5\u1085\'ED\u1089\'F1\u1090\'F2\u1074\'E2\u1086\'EE! \hich\f1 \emdash \loch\f1  \u1089\'F1 \u1085\'ED\u1072\'E0\u1089\'F1\u1083\'EB\u1072\'E0\u1078\'E6\u1076\'E4\u1077\'E5\u1085\'ED\u1080\'E8\u1077\'E5\u1084\'EC \u1074\'E2\u1086\'EE\u1089\'F1\u1082\'EA\u1083\'EB\u1080\'E8\u1094\'F6\u1072\'E0\u1102\'FE \u1103\'FF. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1043\'C3\u1072\'E0\u1076\'E4\u1086\'EE\u1089\'F1\u1090\'F2\u1100\'FC \u1085\'ED\u1077\'E5\u1074\'E2\u1077\'E5\u1088\'F0\u1086\'EE\u1103\'FF\u1090\'F2\u1085\'ED\u1072\'E0\u1103\'FF, \hich\f1 \emdash \loch\f1  \u1103\'FF \u1074\'E2\u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3 \u1089\'F1\u1083\'EB\u1099\'FB\u1096\'F8\u1091\'F3 \u1085\'ED\u1077\'E5\u1079\'E7\u1085\'ED\u1072\'E0\u1082\'EA\u1086\'EE\u1084\'EC\u1099\'FB\u1081\'E9 \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1, \hich\f1 \emdash \loch\f1  \u1050\'CA\u1072\'E0\u1082\'EA \u1090\'F2\u1099\'FB \u1101\'FD\u1090\'F2\u1086\'EE \u1087\'EF\u1100\'FC\u1077\'E5\u1096\'F8\u1100\'FC \u1074\'E2\u1086\'EE\u1086\'EE\u1073\'E1\u1097\'F9\u1077\'E5? }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1054\'CE\u1075\'E3\u1083\'EB\u1103\'FF\u1076\'E4\u1099\'FB\u1074\'E2\u1072\'E0\u1102\'FE\u1089\'F1\u1100\'FC. \u1056\'D0\u1103\'FF\u1076\'E4\u1086\'EE\u1084\'EC \u1089\'F1\u1086\'EE \u1084\'EC\u1085\'ED\u1086\'EE\u1081\'E9 \u1074\'E2\u1086\'EE\u1088\'F0\u1082\'EA\u1091\'F3\u1102\'FE\u1090\'F2 \u1055\'CF\u1086\'EE\u1083\'EB\u1080\'E8\u1085\'ED\u1072\'E0 \u1080\'E8 \u1052\'CC\u1080\'E8\u1096\'F8\u1072\'E0, \u1091\'F3\u1074\'E2\u1080\'E8\u1076\'E4\u1077\'E5\u1074\'E2 \u1084\'EC\u1086\'EE\u1081\'E9 \u1074\'E2\u1079\'E7\u1075\'E3\u1083\'EB\u1103\'FF\u1076\'E4, \u1086\'EE\u1085\'ED\u1080\'E8 \u1079\'E7\u1072\'E0\u1084\'EC\u1086\'EE\u1083\'EB\u1082\'EA\u1072\'E0\u1102\'FE\u1090\'F2. \u1040\'C0 \u1074\'E2\u1086\'EE\u1090\'F2 \u1085\'ED\u1077\'E5\u1079\'E7\u1085\'ED\u1072\'E0\u1082\'EA\u1086\'EE\u1084\'EC\u1099\'FB\u1081\'E9 \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1 \u1085\'ED\u1077\'E5\u1090\'F2. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1090\'F2\u1086\'EE \u1074\'E2\u1086\'EE\u1086\'EE\u1073\'E1\u1097\'F9\u1077\'E5 \u1087\'EF\u1088\'F0\u1080\'E8\u1076\'E4\u1091\'F3\u1084\'EC\u1072\'E0\u1083\'EB \u1101\'FD\u1090\'F2\u1091\'F3 \u1086\'EE\u1090\'F2\u1088\'F0\u1072\'E0\u1074\'E2\u1091\'F3? \hich\f1 \emdash \loch\f1  \u1086\'EE\u1085\'ED \u1079\'E7\u1074\'E2\u1091\'F3\u1095\'F7\u1072\'E0\u1083\'EB \u1085\'ED\u1077\'E5\u1076\'E4\u1086\'EE\u1074\'E2\u1086\'EE\u1083\'EB\u1100\'FC\u1085\'ED\u1086\'EE, \u1089\'F1\u1083\'EB\u1086\'EE\u1074\'E2\u1085\'ED\u1086\'EE \u1086\'EE\u1090\'F2\u1095\'F7\u1080\'E8\u1090\'F2\u1099\'FB\u1074\'E2\u1072\'E0\u1083\'EB \u1082\'EA\u1086\'EE\u1075\'E3\u1086\'EE-\u1090\'F2\u1086\'EE. \u1071\'DF \u1087\'EF\u1088\'F0\u1086\'EE\u1076\'E4\u1086\'EE\u1083\'EB\u1078\'E6\u1080\'E8\u1083\'EB \u1074\'E2\u1099\'FB\u1089\'F1\u1084\'EC\u1072\'E0\u1090\'F2\u1088\'F0\u1080\'E8\u1074\'E2\u1072\'E0\u1090\'F2\u1100\'FC \u1080\'E8\u1089\'F1\u1090\'F2\u1086\'EE\u1095\'F7\u1085\'ED\u1080\'E8\u1082\'EA. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1086\'EE\u1075\'E3\u1086\'EE \u1090\'F2\u1099\'FB \u1080\'E8\u1097\'F9\u1077\'E5\u1096\'F8\u1100\'FC? \hich\f1 \emdash \loch\f1  \u1055\'CF\u1086\'EE\u1083\'EB\u1080\'E8\u1085\'ED\u1072\'E0 \u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1077\'E5\u1083\'EB\u1072\'E0 \u1085\'ED\u1072\'E0 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1079\'E7\u1072\'E0\u1080\'E8\u1085\'ED\u1090\'F2\u1077\'E5\u1088\'F0\u1077\'E5\u1089\'F1\u1086\'EE\u1074\'E2\u1072\'E0\u1085\'ED\u1085\'ED\u1086\'EE, \u1052\'CC\u1080\'E8\u1096\'F8\u1072\'E0 \u1079\'E7\u1072\'E0\u1074\'E2\u1080\'E8\u1089\'F1\u1072\'E0\u1083\'EB \u1074\'E2 \u1090\'F2\u1077\'E5\u1083\'EB\u1077\'E5\u1092\'F4\u1086\'EE\u1085\'ED\u1077\'E5, \u1073\'E1\u1086\'EE\u1088\'F0\u1084\'EC\u1086\'EE\u1095\'F7\u1072\'E0 \u1095\'F7\u1090\'F2\u1086\'EE-\u1090\'F2\u1086\'EE \u1089\'F1\u1077\'E5\u1073\'E1\u1077\'E5 \u1087\'EF\u1086\'EE\u1076\'E4 \u1085\'ED\u1086\'EE\u1089\'F1 \u1080\'E8 \u1086\'EE\u1090\'F2\u1087\'EF\u1080\'E8\u1074\'E2\u1072\'E0\u1103\'FF \u1082\'EA\u1086\'EE\u1092\'F4\u1077\'E5. \u1058\'D2\u1086\'EE\u1078\'E6\u1077\'E5 \u1083\'EB\u1102\'FE\u1073\'E1\u1080\'E8\u1090\'F2 \u1075\'E3\u1086\'EE\u1088\'F0\u1103\'FF\u1095\'F7\u1080\'E8\u1081\'E9. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1042\'C2\u1086\'EE\u1090\'F2 \u1087\'EF\u1086\'EE\u1087\'EF\u1088\'F0\u1086\'EE\u1073\'E1\u1086\'EE\u1074\'E2\u1072\'E0\u1083\'EB \u1073\'E1\u1099\'FB \u1085\'ED\u1072\'E0\u1089\'F1\u1090\'F2\u1086\'EE\u1103\'FF\u1097\'F9\u1091\'F3\u1102\'FE \u1072\'E0\u1088\'F0\u1072\'E0\u1073\'E1\u1080\'E8\u1082\'EA\u1091\'F3, \u1087\'EF\u1083\'EB\u1077\'E5\u1074\'E2\u1072\'E0\u1083\'EB\u1089\'F1\u1103\'FF \u1073\'E1\u1099\'FB \u1089\'F1\u1077\'E5\u1081\'E9\u1095\'F7\u1072\'E0\u1089\'F1! \hich\f1 \emdash \loch\f1  \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1 \u1073\'E1\u1099\'FB\u1083\'EB \u1089\'F1\u1083\'EB\u1080\'E8\u1096\'F8\u1082\'EA\u1086\'EE\u1084\'EC \u1095\'F7\u1077\'E5\u1090\'F2\u1082\'EA\u1080\'E8\u1084\'EC \u1076\'E4\u1083\'EB\u1103\'FF \u1096\'F8\u1091\'F3\u1084\'EC\u1072\'E0 \u1082\'EA\u1072\'E0\u1092\'F4\u1077\'E5\u1090\'F2\u1077\'E5\u1088\'F0\u1080\'E8\u1103\'FF. \u1054\'CE\u1085\'ED \u1073\'E1\u1099\'FB\u1083\'EB \u1074\'E2 \u1084\'EC\u1086\'EE\u1077\'E5\u1081\'E9 \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1074\'E2\u1077\'E5. \hich\f1 \emdash \loch\f1  \u1061\'D5\u1074\'E2\u1072\'E0\u1090\'F2\u1080\'E8\u1090\'F2 \u1074\'E2\u1088\'F0\u1072\'E0\u1097\'F9\u1072\'E0\u1090\'F2\u1100\'FC \u1095\'F7\u1077\'E5\u1088\'F0\u1077\'E5\u1087\'EF\u1091\'F3\u1096\'F8\u1082\'EA\u1086\'EE\u1081\'E9 \u1090\'F2\u1091\'F3\u1076\'E4\u1072\'E0-\u1089\'F1\u1102\'FE\u1076\'E4\u1072\'E0, \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1079\'E7\u1076\'E4\u1077\'E5\u1089\'F1\u1100\'FC \u1085\'ED\u1077\'E5\u1090\'F2, \hich\f1 \emdash \loch\f1  \u1075\'E3\u1086\'EE\u1074\'E2\u1086\'EE\u1088\'F0\u1080\'E8\u1074\'E2\u1096\'F8\u1080\'E8\u1081\'E9 \u1085\'ED\u1072\'E0\u1082\'EA\u1086\'EE\u1085\'ED\u1077\'E5\u1094\'F6-\u1090\'F2\u1086\'EE \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB \u1087\'EF\u1086\'EE\u1085\'ED\u1086\'EE\u1089\'F1\u1080\'E8\u1090\'F2\u1100\'FC \u1084\'EC\u1086\'EE\u1081\'E9 \u1083\'EB\u1102\'FE\u1073\'E1\u1080\'E8\u1084\'EC\u1099\'FB\u1081\'E9 \u1085\'ED\u1072\'E0\u1087\'EF\u1080\'E8\u1090\'F2\u1086\'EE\u1082\'EA. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1071\'DF \u1089\'F1\u1086\'EE\u1096\'F8\u1077\'E5\u1083\'EB \u1089\'F1 \u1091\'F3\u1084\'EC\u1072\'E0? \hich\f1 \emdash \loch\f1  \u1079\'E7\u1072\'E0\u1076\'E4\u1072\'E0\u1083\'EB \u1103\'FF \u1074\'E2\u1086\'EE\u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1 \u1089\'F1\u1072\'E0\u1084\'EC\u1086\'EE\u1084\'EC\u1091\'F3 \u1089\'F1\u1077\'E5\u1073\'E1\u1077\'E5. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1044\'C4\u1072\'E0, \u1088\'F0\u1072\'E0\u1079\'E7 \u1091\'F3\u1078\'E6 \u1090\'F2\u1099\'FB \u1087\'EF\u1100\'FC\u1077\'E5\u1096\'F8\u1100\'FC \u1101\'FD\u1090\'F2\u1086\'EE \u1078\'E6\u1080\'E8\u1076\'E4\u1082\'EA\u1086\'EE\u1077\'E5 \u1076\'E4\u1077\'E5\u1088\'F0\u1100\'FC\u1084\'EC\u1086\'EE! \hich\f1 \emdash \loch\f1  \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1 \u1086\'EE\u1090\'F2\u1074\'E2\u1077\'E5\u1090\'F2\u1080\'E8\u1083\'EB \u1084\'EC\u1085\'ED\u1077\'E5 \u1080\'E8 \u1090\'F2\u1091\'F3\u1090\'F2 \u1078\'E6\u1077\'E5 \u1087\'EF\u1088\'F0\u1080\'E8\u1082\'EA\u1072\'E0\u1079\'E7\u1072\'E0\u1083\'EB: \hich\f1 \emdash \loch\f1  \u1041\'C1\u1088\'F0\u1086\'EE\u1089\'F1\u1100\'FC \u1089\'F1\u1077\'E5\u1081\'E9\u1095\'F7\u1072\'E0\u1089\'F1 \u1078\'E6\u1077\'E5! }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1055\'CF\u1072\'E0\u1083\'EB\u1100\'FC\u1094\'F6\u1099\'FB \u1088\'F0\u1072\'E0\u1079\'E7\u1078\'E6\u1072\'E0\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1086\'EE\u1090\'F2 \u1080\'E8\u1089\'F1\u1087\'EF\u1091\'F3\u1075\'E3\u1072\'E0, \u1087\'EF\u1083\'EB\u1072\'E0\u1089\'F1\u1090\'F2\u1080\'E8\u1082\'EA\u1086\'EE\u1074\'E2\u1099\'FB\u1081\'E9 \u1089\'F1\u1090\'F2\u1072\'E0\u1082\'EA\u1072\'E0\u1085\'ED\u1095\'F7\u1080\'E8\u1082\'EA \u1087\'EF\u1086\'EE\u1083\'EB\u1077\'E5\u1090\'F2\u1077\'E5\u1083\'EB \u1085\'ED\u1072\'E0 \u1087\'EF\u1086\'EE\u1083\'EB. \u1071\'DF \u1074\'E2 \u1096\'F8\u1086\'EE\u1082\'EA\u1077\'E5 \u1085\'ED\u1072\'E0\u1073\'E1\u1083\'EB\u1102\'FE\u1076\'E4\u1072\'E0\u1083\'EB \u1079\'E7\u1072\'E0 \u1077\'E5\u1075\'E3\u1086\'EE \u1087\'EF\u1072\'E0\u1076\'E4\u1077\'E5\u1085\'ED\u1080\'E8\u1077\'E5\u1084\'EC, \u1087\'EF\u1086\'EE\u1082\'EA\u1072\'E0 \u1086\'EE\u1085\'ED \u1085\'ED\u1077\'E5 \u1091\'F3\u1076\'E4\u1072\'E0\u1088\'F0\u1080\'E8\u1083\'EB\u1089\'F1\u1103\'FF \u1086\'EE \u1078\'E6\u1077\'E5\u1089\'F1\u1090\'F2\u1082\'EA\u1080\'E8\u1081\'E9 \u1080\'E8 \u1073\'E1\u1077\'E5\u1089\'F1\u1087\'EF\u1086\'EE\u1097\'F9\u1072\'E0\u1076\'E4\u1085\'ED\u1099\'FB\u1081\'E9 \u1073\'E1\u1077\'E5\u1090\'F2\u1086\'EE\u1085\'ED. \u1046\'C6\u1080\'E8\u1076\'E4\u1082\'EA\u1086\'EE\u1089\'F1\u1090\'F2\u1100\'FC \u1088\'F0\u1072\'E0\u1089\'F1\u1087\'EF\u1083\'EB\u1077\'E5\u1089\'F1\u1082\'EA\u1072\'E0\u1083\'EB\u1072\'E0\u1089\'F1\u1100\'FC, \u1085\'ED\u1086\'EE \u1103\'FF \u1086\'EE\u1090\'F2\u1089\'F1\u1082\'EA\u1086\'EE\u1095\'F7\u1080\'E8\u1083\'EB, \u1085\'ED\u1072\'E0\u1090\'F2\u1086\'EE\u1083\'EB\u1082\'EA\u1085\'ED\u1091\'F3\u1074\'E2\u1096\'F8\u1080\'E8\u1089\'F1\u1100\'FC \u1085\'ED\u1072\'E0 \u1052\'CC\u1080\'E8\u1096\'F8\u1091\'F3. \u1052\'CC\u1072\'E0\u1083\'EB\u1086\'EE \u1090\'F2\u1086\'EE\u1075\'E3\u1086\'EE, \u1095\'F7\u1090\'F2\u1086\'EE \u1103\'FF \u1086\'EE\u1090\'F2\u1076\'E4\u1072\'E0\u1074\'E2\u1080\'E8\u1083\'EB \u1077\'E5\u1084\'EC\u1091\'F3 \u1085\'ED\u1086\'EE\u1075\'E3\u1091\'F3, \u1090\'F2\u1072\'E0\u1082\'EA \u1077\'E5\u1097\'F9\u1077\'E5 \u1080\'E8 \u1086\'EE\u1073\'E1\u1083\'EB\u1080\'E8\u1083\'EB \u1082\'EA\u1086\'EE\u1092\'F4\u1077\'E5. }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1072\'E0\u1082\'EA\u1086\'EE\u1075\'E3\u1086\'EE \u1095\'F7\u1077\'E5\u1088\'F0\u1090\'F2\u1072\'E0?! \hich\f1 \emdash \loch\f1  \u1074\'E2\u1089\'F1\u1082\'EA\u1088\'F0\u1080\'E8\u1095\'F7\u1072\'E0\u1083\'EB \u1089\'F1\u1086\'EE\u1089\'F1\u1077\'E5\u1076\'E4, \u1086\'EE\u1073\'E1\u1078\'E6\u1080\'E8\u1075\'E3\u1072\'E0\u1103\'FF\u1089\'F1\u1100\'FC \u1082\'EA\u1080\'E8\u1087\'EF\u1103\'FF\u1090\'F2\u1082\'EA\u1086\'EE\u1084\'EC. \u1044\'C4\u1074\'E2\u1072\'E0 \u1089\'F1\u1090\'F2\u1091\'F3\u1082\'EA\u1072\'E0 \u1086\'EE\u1073\'E1 \u1087\'EF\u1086\'EE\u1083\'EB, \u1086\'EE\u1076\'E4\u1080\'E8\u1085\'ED \u1084\'EC\u1103\'FF\u1075\'E3\u1095\'F7\u1077\'E5, \u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3\u1086\'EE\u1081\'E9 \u1078\'E6\u1077\'E5\u1089\'F1\u1090\'F2\u1095\'F7\u1077\'E5. \u1071\'DF \u1086\'EE\u1073\'E1\u1077\'E5\u1088\'F0\u1085\'ED\u1091\'F3\u1083\'EB\u1089\'F1\u1103\'FF \u1080\'E8 \u1091\'F3\u1074\'E2\u1080\'E8\u1076\'E4\u1077\'E5\u1083\'EB, \u1082\'EA\u1072\'E0\u1082\'EA \u1077\'E5\u1075\'E3\u1086\'EE \u1090\'F2\u1077\'E5\u1083\'EB\u1077\'E5\u1092\'F4\u1086\'EE\u1085\'ED \u1082\'EA\u1091\'F3\u1087\'EF\u1072\'E0\u1077\'E5\u1090\'F2\u1089\'F1\u1103\'FF \u1074\'E2 \u1087\'EF\u1088\'F0\u1086\'EE\u1083\'EB\u1080\'E8\u1090\'F2\u1086\'EE\u1084\'EC \u1082\'EA\u1086\'EE\u1092\'F4\u1077\'E5. \u1052\'CC\u1080\'E8\u1096\'F8\u1072\'E0 \u1073\'E1\u1099\'FB\u1083\'EB \u1075\'E3\u1086\'EE\u1090\'F2\u1086\'EE\u1074\'E2 \u1079\'E7\u1072\'E0\u1087\'EF\u1083\'EB\u1072\'E0\u1082\'EA\u1072\'E0\u1090\'F2\u1100\'FC, \u1055\'CF\u1086\'EE\u1083\'EB\u1080\'E8\u1085\'ED\u1072\'E0 \u1087\'EF\u1086\'EE\u1090\'F2\u1077\'E5\u1088\'F0\u1103\'FF\u1083\'EB\u1072\'E0 \u1076\'E4\u1072\'E0\u1088\'F0 \u1088\'F0\u1077\'E5\u1095\'F7\u1080\'E8, \u1086\'EE\u1085\'ED\u1072\'E0 \u1086\'EE\u1096\'F8\u1072\'E0\u1088\'F0\u1072\'E0\u1096\'F8\u1077\'E5\u1085\'ED\u1085\'ED\u1086\'EE \u1089\'F1\u1105\'B8\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1077\'E5\u1083\'EB\u1072\'E0 \u1090\'F2\u1086\'EE \u1085\'ED\u1072\'E0 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF, \u1090\'F2\u1086\'EE \u1085\'ED\u1072\'E0 \u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1076\'E4\u1072\'E0\u1102\'FE\u1097\'F9\u1077\'E5\u1075\'E3\u1086\'EE \u1052\'CC\u1080\'E8\u1096\'F8\u1091\'F3. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1057\'D1\u1077\'E5\u1088\'F0\u1074\'E2\u1080\'E8\u1089\'F1 \u1082\'EA\u1091\'F3\u1095\'F7\u1091\'F3 \u1073\'E1\u1072\'E0\u1073\'E1\u1086\'EE\u1082\'EA \u1089\'F1\u1090\'F2\u1086\'EE\u1080\'E8-\u1080\'E8-\u1080\'E8\u1090\'F2! \hich\f1 \emdash \loch\f1  \u1087\'EF\u1088\'F0\u1086\'EE\u1090\'F2\u1103\'FF\u1085\'ED\u1091\'F3\u1083\'EB \u1087\'EF\u1072\'E0\u1088\'F0\u1077\'E5\u1085\'ED\u1100\'FC. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1058\'D2\u1077\'E5\u1073\'E1\u1077\'E5 \u1087\'EF\u1088\'F0\u1080\'E8\u1076\'E4\u1077\'E5\u1090\'F2\u1089\'F1\u1103\'FF \u1077\'E5\u1075\'E3\u1086\'EE \u1086\'EE\u1087\'EF\u1083\'EB\u1072\'E0\u1090\'F2\u1080\'E8\u1090\'F2\u1100\'FC, \hich\f1 \emdash \loch\f1  \u1087\'EF\u1086\'EE\u1076\'E4\u1076\'E4\u1072\'E0\u1082\'EA\u1085\'ED\u1091\'F3\u1083\'EB \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1071\'DF\u8230\'85 \u1103\'FF \u1079\'E7\u1072\'E0\u1087\'EF\u1083\'EB\u1072\'E0\u1095\'F7\u1091\'F3, \hich\f1 \emdash \loch\f1  \u1074\'E2\u1089\'F1\u1083\'EB\u1091\'F3\u1093\'F5 \u1088\'F0\u1072\'E0\u1089\'F1\u1090\'F2\u1077\'E5\u1088\'F0\u1103\'FF\u1085\'ED\u1085\'ED\u1086\'EE \u1087\'EF\u1088\'F0\u1086\'EE\u1073\'E1\u1086\'EE\u1088\'F0\u1084\'EC\u1086\'EE\u1090\'F2\u1072\'E0\u1083\'EB \u1103\'FF \u1080\'E8 \u1087\'EF\u1086\'EE\u1089\'F1\u1087\'EF\u1077\'E5\u1096\'F8\u1080\'E8\u1083\'EB \u1089\'F1\u1073\'E1\u1077\'E5\u1078\'E6\u1072\'E0\u1090\'F2\u1100\'FC, \u1079\'E7\u1072\'E0\u1073\'E1\u1099\'FB\u1074\'E2 \u1087\'EF\u1088\'F0\u1086\'EE \u1083\'EB\u1091\'F3\u1078\'E6\u1091\'F3 \u1080\'E8\u1079\'E7 \u1082\'EA\u1086\'EE\u1092\'F4\u1077\'E5 \u1080\'E8 \u1052\'CC\u1080\'E8\u1096\'F8\u1091\'F3 \u1089\'F1 \u1055\'CF\u1086\'EE\u1083\'EB\u1080\'E8\u1085\'ED\u1086\'EE\u1081\'E9. \u1052\'CC\u1085\'ED\u1077\'E5 \u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB\u1086\'EE \u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1096\'F8\u1085\'ED\u1086\'EE. }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1090\'F2\u1086\'EE \u1090\'F2\u1099\'FB \u1090\'F2\u1072\'E0\u1082\'EA\u1086\'EE\u1081\'E9? \hich\f1 \emdash \loch\f1  \u1103\'FF \u1089\'F1\u1085\'ED\u1086\'EE\u1074\'E2\u1072\'E0 \u1079\'E7\u1072\'E0\u1076\'E4\u1072\'E0\u1083\'EB \u1074\'E2\u1086\'EE\u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1, \u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1091\'F3\u1078\'E6\u1077\'E5 \u1090\'F2\u1086\'EE\u1084\'EC\u1091\'F3, \u1082\'EA\u1090\'F2\u1086\'EE \u1080\'E8\u1089\'F1\u1087\'EF\u1086\'EE\u1088\'F0\u1090\'F2\u1080\'E8\u1083\'EB \u1084\'EC\u1086\'EE\u1081\'E9 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1088\'F0\u1099\'FB\u1074\'E2. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1071\'DF? \hich\f1 \emdash \loch\f1  \u1074\'E2\u1088\'F0\u1077\'E5\u1076\'E4\u1080\'E8\u1090\'F2\u1077\'E5\u1083\'EB\u1100\'FC \u1089\'F1\u1088\'F0\u1072\'E0\u1079\'E7\u1091\'F3 \u1078\'E6\u1077\'E5 \u1086\'EE\u1090\'F2\u1082\'EA\u1083\'EB\u1080\'E8\u1082\'EA\u1085\'ED\u1091\'F3\u1083\'EB\u1089\'F1\u1103\'FF, \hich\f1 \emdash \loch\f1  \u1053\'CD\u1091\'F3\u8230\'85 \u1050\'CA\u1072\'E0\u1082\'EA \u1073\'E1\u1099\'FB \u1101\'FD\u1090\'F2\u1086\'EE \u1089\'F1\u1082\'EA\u1072\'E0\u1079\'E7\u1072\'E0\u1090\'F2\u1100\'FC\u8230\'85 \u1071\'DF \hich\f1 \emdash \loch\f1  \u1041\'C1\u1086\'EE\u1075\'E3. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1058\'D2\u1086\'EE\u1095\'F7\u1085\'ED\u1086\'EE \u1089\'F1\u1086\'EE\u1096\'F8\u1077\'E5\u1083\'EB \u1089\'F1 \u1091\'F3\u1084\'EC\u1072\'E0! \hich\f1 \emdash \loch\f1  \u1089\'F1\u1093\'F5\u1074\'E2\u1072\'E0\u1090\'F2\u1080\'E8\u1074\'E2\u1096\'F8\u1080\'E8\u1089\'F1\u1100\'FC \u1079\'E7\u1072\'E0 \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1074\'E2\u1091\'F3, \u1103\'FF \u1074\'E2\u1086\'EE\u1096\'F8\u1077\'E5\u1083\'EB \u1074\'E2 \u1083\'EB\u1080\'E8\u1092\'F4\u1090\'F2, \hich\f1 \emdash \loch\f1  \u1069\'DD\u1090\'F2\u1086\'EE \u1074\'E2\u1089\'F1\u1077\'E5 \u1089\'F1\u1090\'F2\u1088\'F0\u1077\'E5\u1089\'F1\u1089\'F1, \u1057\'D1\u1072\'E0\u1096\'F8\u1072\'E0, \u1101\'FD\u1090\'F2\u1086\'EE \u1074\'E2\u1089\'F1\u1077\'E5 \u1089\'F1\u1090\'F2\u1088\'F0\u1077\'E5\u1089\'F1\u1089\'F1\u8230\'85 \hich\f1 \emdash \loch\f1  \u1096\'F8\u1077\'E5\u1087\'EF\u1090\'F2\u1072\'E0\u1083\'EB \u1103\'FF, \u1084\'EC\u1072\'E0\u1089\'F1\u1089\'F1\u1080\'E8\u1088\'F0\u1091\'F3\u1103\'FF \u1074\'E2\u1080\'E8\u1089\'F1\u1082\'EA\u1080\'E8, \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1074\'E2\u1072\'E0 \u1074\'E2\u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3 \u1088\'F0\u1072\'E0\u1079\'E7\u1073\'E1\u1086\'EE\u1083\'EB\u1077\'E5\u1083\'EB\u1072\'E0\u1089\'F1\u1100\'FC }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1086\'EE\u1092\'F4\u1077\'E5 \u1084\'EC\u1077\'E5\u1085\'ED\u1100\'FC\u1096\'F8\u1077\'E5 \u1093\'F5\u1083\'EB\u1077\'E5\u1089\'F1\u1090\'F2\u1072\'E0\u1090\'F2\u1100\'FC \u1085\'ED\u1072\'E0\u1076\'E4\u1086\'EE, \u1086\'EE\u1085\'ED \u1087\'EF\u1086\'EE\u1076\'E4\u1085\'ED\u1080\'E8\u1084\'EC\u1072\'E0\u1077\'E5\u1090\'F2 \u1076\'E4\u1072\'E0\u1074\'E2\u1083\'EB\u1077\'E5\u1085\'ED\u1080\'E8\u1077\'E5. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1063\'D7\u1077\'E5\u1075\'E3\u1086\'EE \u1090\'F2\u1077\'E5\u1073\'E1\u1077\'E5 \u1085\'ED\u1072\'E0\u1076\'E4\u1086\'EE \u1086\'EE\u1090\'F2 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF, \u8220\'93\u1041\'C1\u1086\'EE\u1075\'E3\u8221\'94? \hich\f1 \emdash \loch\f1  \u1103\'FF \u1075\'E3\u1076\'E4\u1077\'E5-\u1090\'F2\u1086\'EE \u1089\'F1\u1083\'EB\u1099\'FB\u1096\'F8\u1072\'E0\u1083\'EB, \u1095\'F7\u1090\'F2\u1086\'EE \u1089\'F1\u1086\'EE \u1089\'F1\u1074\'E2\u1086\'EE\u1080\'E8\u1084\'EC\u1080\'E8 \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1\u1072\'E0\u1084\'EC\u1080\'E8 \u1085\'ED\u1072\'E0\u1076\'E4\u1086\'EE \u1076\'E4\u1088\'F0\u1091\'F3\u1078\'E6\u1080\'E8\u1090\'F2\u1100\'FC. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1059\'D3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1077\'E5\u1089\'F1\u1090\'F2\u1100\'FC \u1076\'E4\u1083\'EB\u1103\'FF \u1090\'F2\u1077\'E5\u1073\'E1\u1103\'FF \u1087\'EF\u1088\'F0\u1077\'E5\u1076\'E4\u1083\'EB\u1086\'EE\u1078\'E6\u1077\'E5\u1085\'ED\u1080\'E8\u1077\'E5, \u1086\'EE\u1090\'F2 \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1086\'EE\u1075\'E3\u1086\'EE \u1090\'F2\u1099\'FB \u1090\'F2\u1086\'EE\u1095\'F7\u1085\'ED\u1086\'EE \u1085\'ED\u1077\'E5 \u1086\'EE\u1090\'F2\u1082\'EA\u1072\'E0\u1078\'E6\u1077\'E5\u1096\'F8\u1100\'FC\u1089\'F1\u1103\'FF! \hich\f1 \emdash \loch\f1  \u1073\'E1\u1086\'EE\u1078\'E6\u1077\'E5\u1089\'F1\u1090\'F2\u1074\'E2\u1077\'E5\u1085\'ED\u1085\'ED\u1099\'FB\u1081\'E9 \u1075\'E3\u1083\'EB\u1072\'E0\u1089\'F1 \u1074\'E2\u1085\'ED\u1077\'E5\u1079\'E7\u1072\'E0\u1087\'EF\u1085\'ED\u1086\'EE \u1087\'EF\u1088\'F0\u1080\'E8\u1086\'EE\u1073\'E1\u1088\'F0\u1077\'E5\u1083\'EB \u1080\'E8\u1085\'ED\u1090\'F2\u1086\'EE\u1085\'ED\u1072\'E0\u1094\'F6\u1080\'E8\u1102\'FE \u1087\'EF\u1088\'F0\u1086\'EE\u1076\'E4\u1072\'E0\u1074\'E2\u1094\'F6\u1072\'E0 \u1080\'E8\u1079\'E7 \u1090\'F2\u1077\'E5\u1083\'EB\u1077\'E5\u1084\'EC\u1072\'E0\u1075\'E3\u1072\'E0\u1079\'E7\u1080\'E8\u1085\'ED\u1072\'E0. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1044\'C4\u1072\'E0\u1081\'E9 \u1091\'F3\u1075\'E3\u1072\'E0\u1076\'E4\u1072\'E0\u1102\'FE, \hich\f1 \emdash \loch\f1  \u1086\'EE\u1090\'F2\u1074\'E2\u1077\'E5\u1090\'F2\u1080\'E8\u1083\'EB \u1103\'FF, \hich\f1 \emdash \loch\f1  \u1087\'EF\u1088\'F0\u1077\'E5\u1076\'E4\u1083\'EB\u1086\'EE\u1078\'E6\u1080\'E8\u1096\'F8\u1100\'FC \u1084\'EC\u1085\'ED\u1077\'E5 \u1090\'F2\u1074\'E2\u1086\'EE\u1088\'F0\u1080\'E8\u1090\'F2\u1100\'FC \u1095\'F7\u1091\'F3\u1076\'E4\u1077\'E5\u1089\'F1\u1072\'E0? \u1048\'C8\u1083\'EB\u1080\'E8 \u1079\'E7\u1072\'E0\u1076\'E4\u1091\'F3\u1096\'F8\u1080\'E8\u1090\'F2\u1100\'FC \u1082\'EA\u1086\'EE\u1075\'E3\u1086\'EE-\u1085\'ED\u1080\'E8\u1073\'E1\u1091\'F3\u1076\'E4\u1100\'FC, \u1087\'EF\u1086\'EE\u1090\'F2\u1086\'EE\u1084\'EC\u1091\'F3 \u1095\'F7\u1090\'F2\u1086\'EE \u1086\'EE\u1085\'ED \u1090\'F2\u1077\'E5\u1073\'E1\u1077\'E5 \u1085\'ED\u1077\'E5 \u1085\'ED\u1088\'F0\u1072\'E0\u1074\'E2\u1080\'E8\u1090\'F2\u1089\'F1\u1103\'FF? }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1040\'C0 \u1086\'EE\u1085\'ED \u1076\'E4\u1088\'F0\u1091\'F3\u1078\'E6\u1080\'E8\u1090\'F2\u1100\'FC \u1077\'E5\u1097\'F9\u1077\'E5 \u1093\'F5\u1086\'EE\u1090\'F2\u1077\'E5\u1083\'EB\u8230\'85 \hich\f1 \emdash \loch\f1  \u8220\'93\u1041\'C1\u1086\'EE\u1075\'E3\u8221\'94 \u1086\'EE\u1073\'E1\u1080\'E8\u1076\'E4\u1077\'E5\u1083\'EB\u1089\'F1\u1103\'FF, \u1085\'ED\u1086\'EE \u1085\'ED\u1077\'E5\u1085\'ED\u1072\'E0\u1076\'E4\u1086\'EE\u1083\'EB\u1075\'E3\u1086\'EE: \hich\f1 \emdash \loch\f1  \u1071\'DF \u1076\'E4\u1072\'E0\u1084\'EC \u1090\'F2\u1077\'E5\u1073\'E1\u1077\'E5 \u1074\'E2\u1086\'EE\u1079\'E7\u1084\'EC\u1086\'EE\u1078\'E6\u1085\'ED\u1086\'EE\u1089\'F1\u1090\'F2\u1100\'FC \u1080\'E8\u1079\'E7\u1084\'EC\u1077\'E5\u1085\'ED\u1080\'E8\u1090\'F2\u1100\'FC \u1084\'EC\u1080\'E8\u1088\'F0! }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1086\'EE\u1077\'E5-\u1082\'EA\u1090\'F2\u1086\'EE \u1091\'F3\u1078\'E6\u1077\'E5 \u1087\'EF\u1099\'FB\u1090\'F2\u1072\'E0\u1083\'EB\u1089\'F1\u1103\'FF \u1101\'FD\u1090\'F2\u1086\'EE \u1089\'F1\u1076\'E4\u1077\'E5\u1083\'EB\u1072\'E0\u1090\'F2\u1100\'FC \u1076\'E4\u1074\'E2\u1077\'E5 \u1090\'F2\u1099\'FB\u1089\'F1\u1103\'FF\u1095\'F7\u1080\'E8 \u1083\'EB\u1077\'E5\u1090\'F2 \u1085\'ED\u1072\'E0\u1079\'E7\u1072\'E0\u1076\'E4, \u1077\'E5\u1075\'E3\u1086\'EE \u1087\'EF\u1088\'F0\u1080\'E8\u1073\'E1\u1080\'E8\u1083\'EB\u1080\'E8 \u1082\'EA \u1082\'EA\u1088\'F0\u1077\'E5\u1089\'F1\u1090\'F2\u1091\'F3. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1069\'DD\u1090\'F2\u1086\'EE \u1074\'E2\u1089\'F1\u1077\'E5 \u1089\'F1\u1082\'EA\u1072\'E0\u1079\'E7\u1082\'EA\u1080\'E8! \hich\f1 \emdash \loch\f1  \u1092\'F4\u1099\'FB\u1088\'F0\u1082\'EA\u1085\'ED\u1091\'F3\u1083\'EB \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1 \u1074\'E2 \u1086\'EE\u1090\'F2\u1074\'E2\u1077\'E5\u1090\'F2, \hich\f1 \emdash \loch\f1  \u1059\'D3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1074\'E2\u1089\'F1\u1077\'E5 \u1075\'E3\u1086\'EE\u1088\'F0\u1072\'E0\u1079\'E7\u1076\'E4\u1086\'EE \u1082\'EA\u1088\'F0\u1091\'F3\u1095\'F7\u1077\'E5. \u1048\'C8 \u1103\'FF \u1090\'F2\u1077\'E5\u1073\'E1\u1077\'E5 \u1076\'E4\u1086\'EE\u1082\'EA\u1072\'E0\u1078\'E6\u1091\'F3. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1072\'E0\u1082\'EA? }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1059\'D3\u1074\'E2\u1080\'E8\u1076\'E4\u1080\'E8\u1096\'F8\u1100\'FC. }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1044\'C4\u1074\'E2\u1077\'E5\u1088\'F0\u1080\'E8 \u1083\'EB\u1080\'E8\u1092\'F4\u1090\'F2\u1072\'E0 \u1086\'EE\u1090\'F2\u1082\'EA\u1088\'F0\u1099\'FB\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1080\'E8 \u1103\'FF \u1074\'E2\u1099\'FB\u1096\'F8\u1077\'E5\u1083\'EB. \u1054\'CE\u1095\'F7\u1077\'E5\u1085\'ED\u1100\'FC \u1073\'E1\u1099\'FB\u1089\'F1\u1090\'F2\u1088\'F0\u1086\'EE \u1087\'EF\u1088\'F0\u1086\'EE\u1096\'F8\u1072\'E0\u1075\'E3\u1072\'E0\u1074\'E2 \u1074\'E2 \u1089\'F1\u1074\'E2\u1086\'EE\u1081\'E9 \u1086\'EE\u1090\'F2\u1089\'F1\u1077\'E5\u1082\'EA, \u1103\'FF \u1089\'F1\u1077\'E5\u1083\'EB \u1079\'E7\u1072\'E0 \u1089\'F1\u1090\'F2\u1086\'EE\u1083\'EB. \u1051\'CB\u1091\'F3\u1095\'F7\u1096\'F8\u1077\'E5 \u1086\'EE\u1090\'F2\u1074\'E2\u1083\'EB\u1077\'E5\u1095\'F7\u1100\'FC\u1089\'F1\u1103\'FF, \u1084\'EC\u1086\'EE\u1078\'E6\u1077\'E5\u1090\'F2, \u1087\'EF\u1088\'F0\u1086\'EE\u1081\'E9\u1076\'E4\u1077\'E5\u1090\'F2. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1059\'D3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1075\'E3\u1083\'EB\u1072\'E0\u1079\'E7\u1072\'E0 \u1080\'E8\u1079\'E7 \u1086\'EE\u1088\'F0\u1073\'E1\u1080\'E8\u1090\'F2 \u1087\'EF\u1086\'EE\u1083\'EB\u1077\'E5\u1079\'E7\u1083\'EB\u1080\'E8, \u1082\'EA\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0 \u1103\'FF \u1091\'F3\u1074\'E2\u1080\'E8\u1076\'E4\u1077\'E5\u1083\'EB \u1085\'ED\u1072\'E0 \u1101\'FD\u1082\'EA\u1088\'F0\u1072\'E0\u1085\'ED\u1077\'E5 \u1086\'EE\u1090\'F2\u1082\'EA\u1088\'F0\u1099\'FB\u1090\'F2\u1099\'FB\u1081\'E9 \u1076\'E4\u1086\'EE\u1082\'EA\u1091\'F3\u1084\'EC\u1077\'E5\u1085\'ED\u1090\'F2 \u1089\'F1 \u1079\'E7\u1072\'E0\u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1074\'E2\u1082\'EA\u1086\'EE\u1084\'EC: \u8220\'93\u1054\'CE\u1090\'F2\u1095\'F7\u1077\'E5\u1090\'F2 \u1087\'EF\u1086\'EE \u1094\'F6\u1077\'E5\u1083\'EB\u1077\'E5\u1074\'E2\u1086\'EE\u1084\'EC\u1091\'F3 \u1084\'EC\u1072\'E0\u1088\'F0\u1082\'EA\u1077\'E5\u1090\'F2\u1080\'E8\u1085\'ED\u1075\'E3\u1091\'F3 \u1079\'E7\u1072\'E0 1-\u1099\'FB\u1081\'E9 \u1082\'EA\u1074\'E2\u1072\'E0\u1088\'F0\u1090\'F2\u1072\'E0\u1083\'EB\u8221\'94. \u1042\'C2 \u1092\'F4\u1072\'E0\u1081\'E9\u1083\'EB\u1077\'E5 \u1089\'F1\u1086\'EE\u1076\'E4\u1077\'E5\u1088\'F0\u1078\'E6\u1072\'E0\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1074\'E2\u1089\'F1\u1077\'E5 \u1085\'ED\u1077\'E5\u1086\'EE\u1073\'E1\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1084\'EC\u1099\'FB\u1077\'E5 \u1076\'E4\u1080\'E8\u1072\'E0\u1075\'E3\u1088\'F0\u1072\'E0\u1084\'EC\u1084\'EC\u1099\'FB, \u1072\'E0\u1085\'ED\u1072\'E0\u1083\'EB\u1080\'E8\u1090\'F2\u1080\'E8\u1082\'EA\u1072\'E0, \u1082\'EA\u1086\'EE\u1090\'F2\u1086\'EE\u1088\'F0\u1091\'F3\u1102\'FE \u1103\'FF \u1077\'E5\u1097\'F9\u1077\'E5 \u1085\'ED\u1077\'E5 \u1085\'ED\u1072\'E0\u1095\'F7\u1080\'E8\u1085\'ED\u1072\'E0\u1083\'EB. \u1059\'D3\u1076\'E4\u1086\'EE\u1073\'E1\u1085\'ED\u1086\'EE\u1077\'E5 \u1086\'EE\u1075\'E3\u1083\'EB\u1072\'E0\u1074\'E2\u1083\'EB\u1077\'E5\u1085\'ED\u1080\'E8\u1077\'E5, \u1080\'E8\u1085\'ED\u1092\'F4\u1086\'EE\u1088\'F0\u1084\'EC\u1072\'E0\u1090\'F2\u1080\'E8\u1074\'E2\u1085\'ED\u1099\'FB\u1077\'E5 \u1079\'E7\u1072\'E0\u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1074\'E2\u1082\'EA\u1080\'E8 \u1080\'E8 \u1084\'EC\u1086\'EE\u1077\'E5 \u1072\'E0\u1074\'E2\u1090\'F2\u1086\'EE\u1088\'F0\u1089\'F1\u1090\'F2\u1074\'E2\u1086\'EE \u1074\'E2 \u1089\'F1\u1072\'E0\u1084\'EC\u1086\'EE\u1084\'EC \u1082\'EA\u1086\'EE\u1085\'ED\u1094\'F6\u1077\'E5. \u1054\'CE\u1090\'F2\u1082\'EA\u1088\'F0\u1099\'FB\u1083\'EB\u1086\'EE\u1089\'F1\u1100\'FC \u1086\'EE\u1082\'EA\u1085\'ED\u1086\'EE \u1087\'EF\u1077\'E5\u1095\'F7\u1072\'E0\u1090\'F2\u1080\'E8, \u1087\'EF\u1088\'F0\u1080\'E8\u1085\'ED\u1090\'F2\u1077\'E5\u1088\'F0 \u1084\'EC\u1077\'E5\u1088\'F0\u1085\'ED\u1086\'EE \u1079\'E7\u1072\'E0\u1096\'F8\u1091\'F3\u1084\'EC\u1077\'E5\u1083\'EB, \u1087\'EF\u1086\'EE\u1075\'E3\u1083\'EB\u1086\'EE\u1097\'F9\u1072\'E0\u1103\'FF \u1083\'EB\u1080\'E8\u1089\'F1\u1090\'F2\u1099\'FB \u1073\'E1\u1091\'F3\u1084\'EC\u1072\'E0\u1075\'E3\u1080\'E8 \u1080\'E8 \u1074\'E2\u1099\'FB\u1087\'EF\u1091\'F3\u1089\'F1\u1082\'EA\u1072\'E0\u1103\'FF \u1080\'E8\u1093\'F5 \u1089\'F1 }{\f2\fs24\b0\i1 \loch\af2\hich\af2\dbch\af2\uc1\u1084\'EC\u1086\'EE\u1080\'E8\u1084\'EC}{\f1\fs24\b0\i0  \loch\af1\hich\af1\dbch\af1\uc1\u1086\'EE\u1090\'F2\u1095\'F7\u1077\'E5\u1090\'F2\u1086\'EE\u1084\'EC. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1040\'C0? \hich\f1 \emdash \loch\f1  \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1 \u1074\'E2\u1077\'E5\u1088\'F0\u1085\'ED\u1091\'F3\u1083\'EB\u1089\'F1\u1103\'FF, \hich\f1 \emdash \loch\f1  \u1058\'D2\u1077\'E5\u1087\'EF\u1077\'E5\u1088\'F0\u1100\'FC \u1090\'F2\u1099\'FB \u1089\'F1\u1095\'F7\u1080\'E8\u1090\'F2\u1072\'E0\u1077\'E5\u1096\'F8\u1100\'FC \u1089\'F1\u1077\'E5\u1073\'E1\u1103\'FF \u1089\'F1\u1091\'F3\u1084\'EC\u1072\'E0\u1089\'F1\u1096\'F8\u1077\'E5\u1076\'E4\u1096\'F8\u1080\'E8\u1084\'EC? }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1069\'DD\u1090\'F2\u1086\'EE \u1085\'ED\u1077\'E5\u1074\'E2\u1086\'EE\u1079\'E7\u1084\'EC\u1086\'EE\u1078\'E6\u1085\'ED\u1086\'EE! }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1071\'DF \u1079\'E7\u1085\'ED\u1072\'E0\u1102\'FE \u1090\'F2\u1086\'EE\u1095\'F7\u1085\'ED\u1086\'EE \endash  \u1085\'ED\u1077\'E5\u1074\'E2\u1086\'EE\u1079\'E7\u1084\'EC\u1086\'EE\u1078\'E6\u1085\'ED\u1086\'EE\u1077\'E5 \u1074\'E2\u1086\'EE\u1079\'E7\u1084\'EC\u1086\'EE\u1078\'E6\u1085\'ED\u1086\'EE! \hich\f1 \emdash \loch\f1  \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1 \u1041\'C1\u1080\'E8\u1083\'EB\u1072\'E0\u1085\'ED\u1072\'E0 \u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB \u1086\'EE\u1090\'F2\u1074\'E2\u1077\'E5\u1090\'F2\u1086\'EE\u1084\'EC. \u1053\'CD\u1072\'E0\u1076\'E4\u1086\'EE \u1084\'EC\u1085\'ED\u1086\'EE\u1081\'E9 \u1089\'F1\u1085\'ED\u1086\'EE\u1074\'E2\u1072\'E0 \u1080\'E8\u1079\'E7\u1076\'E4\u1077\'E5\u1074\'E2\u1072\'E0\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC. \hich\f1 \emdash \loch\f1  \u1052\'CC\u1085\'ED\u1077\'E5 \u1085\'ED\u1091\'F3\u1078\'E6\u1077\'E5\u1085\'ED \u1087\'EF\u1088\'F0\u1086\'EE\u1088\'F0\u1086\'EE\u1082\'EA. \u1071\'DF \u1088\'F0\u1072\'E0\u1089\'F1\u1089\'F1\u1082\'EA\u1072\'E0\u1078\'E6\u1091\'F3 \u1090\'F2\u1077\'E5\u1073\'E1\u1077\'E5 \u1084\'EC\u1085\'ED\u1086\'EE\u1075\'E3\u1086\'EE \u1088\'F0\u1072\'E0\u1079\'E7\u1085\'ED\u1099\'FB\u1093\'F5 \u1090\'F2\u1072\'E0\u1081\'E9\u1085\'ED, \u1090\'F2\u1099\'FB \u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1076\'E4\u1086\'EE\u1085\'ED\u1077\'E5\u1089\'F1\u1080\'E8 \u1080\'E8\u1093\'F5 \u1076\'E4\u1086\'EE \u1085\'ED\u1091\'F3\u1078\'E6\u1085\'ED\u1099\'FB\u1093\'F5 \u1083\'EB\u1102\'FE\u1076\'E4\u1077\'E5\u1081\'E9, \u1057\'D1\u1072\'E0\u1085\'ED\u1103\'FF, \hich\f1 \emdash \loch\f1  \u1090\'F2\u1077\'E5\u1087\'EF\u1077\'E5\u1088\'F0\u1100\'FC \u8220\'93\u1041\'C1\u1086\'EE\u1075\'E3\u8221\'94 \u1075\'E3\u1086\'EE\u1074\'E2\u1086\'EE\u1088\'F0\u1080\'E8\u1083\'EB \u1074\'E2\u1082\'EA\u1088\'F0\u1072\'E0\u1076\'E4\u1095\'F7\u1080\'E8\u1074\'E2\u1086\'EE, \hich\f1 \emdash \loch\f1  \u1076\'E4\u1072\'E0\u1083\'EB\u1100\'FC\u1096\'F8\u1077\'E5 \u1074\'E2\u1089\'F1\u1077\'E5 \u1089\'F1\u1072\'E0\u1084\'EC\u1086\'EE \u1087\'EF\u1086\'EE\u1081\'E9\u1076\'E4\u1077\'E5\u1090\'F2, \u1073\'E1\u1086\'EE\u1083\'EB\u1100\'FC\u1096\'F8\u1077\'E5 \u1085\'ED\u1080\'E8\u1095\'F7\u1077\'E5\u1075\'E3\u1086\'EE \u1085\'ED\u1077\'E5 \u1085\'ED\u1072\'E0\u1076\'E4\u1086\'EE. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1040\'C0 \u1089\'F1\u1072\'E0\u1084\'EC \u1090\'F2\u1099\'FB \u1087\'EF\u1086\'EE\u1095\'F7\u1077\'E5\u1084\'EC\u1091\'F3 \u1085\'ED\u1077\'E5 \u1088\'F0\u1072\'E0\u1089\'F1\u1089\'F1\u1082\'EA\u1072\'E0\u1078\'E6\u1077\'E5\u1096\'F8\u1100\'FC \u8220\'93\u1085\'ED\u1091\'F3\u1078\'E6\u1085\'ED\u1099\'FB\u1084\'EC \u1083\'EB\u1102\'FE\u1076\'E4\u1103\'FF\u1084\'EC\u8221\'94 \u1101\'FD\u1090\'F2\u1080\'E8 \u1090\'F2\u1072\'E0\u1081\'E9\u1085\'ED\u1099\'FB? \hich\f1 \emdash \loch\f1  \u1089\'F1\u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1080\'E8\u1083\'EB \u1103\'FF \u1074\'E2 \u1086\'EE\u1090\'F2\u1074\'E2\u1077\'E5\u1090\'F2. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1071\'DF \u1085\'ED\u1077\'E5 \u1093\'F5\u1086\'EE\u1095\'F7\u1091\'F3 \u1089\'F1\u1074\'E2\u1086\'EE\u1076\'E4\u1080\'E8\u1090\'F2\u1100\'FC \u1089\'F1 \u1091\'F3\u1084\'EC\u1072\'E0 \u1083\'EB\u1091\'F3\u1095\'F7\u1096\'F8\u1080\'E8\u1077\'E5 \u1091\'F3\u1084\'EC\u1099\'FB \u1087\'EF\u1083\'EB\u1072\'E0\u1085\'ED\u1077\'E5\u1090\'F2\u1099\'FB, \u1072\'E0 \u1087\'EF\u1086\'EE\u1082\'EA\u1072\'E0 \u1074\'E2\u1099\'FB\u1088\'F0\u1072\'E0\u1089\'F1\u1090\'F2\u1091\'F3\u1090\'F2 \u1085\'ED\u1086\'EE\u1074\'E2\u1099\'FB\u1077\'E5 \hich\f1 \emdash \loch\f1  \u1083\'EB\u1102\'FE\u1076\'E4\u1080\'E8 \u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1076\'E4\u1086\'EE\u1093\'F5\u1085\'ED\u1091\'F3\u1090\'F2. \u1053\'CD\u1072\'E0\u1076\'E4\u1086\'EE \u1086\'EE\u1089\'F1\u1090\'F2\u1072\'E0\u1085\'ED\u1086\'EE\u1074\'E2\u1080\'E8\u1090\'F2\u1100\'FC \u1101\'FD\u1090\'F2\u1086\'EE. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1055\'CF\u1086\'EE\u1095\'F7\u1077\'E5\u1084\'EC\u1091\'F3 \u1103\'FF? }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1053\'CD\u1077\'E5 \u1079\'E7\u1085\'ED\u1072\'E0\u1102\'FE, \hich\f1 \emdash \loch\f1  \u1086\'EE\u1090\'F2\u1074\'E2\u1077\'E5\u1090\'F2\u1080\'E8\u1083\'EB \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1, \hich\f1 \emdash \loch\f1  \u1087\'EF\u1086\'EE\u1085\'ED\u1088\'F0\u1072\'E0\u1074\'E2\u1080\'E8\u1083\'EB\u1089\'F1\u1103\'FF \u1090\'F2\u1099\'FB \u1084\'EC\u1085\'ED\u1077\'E5. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1071\'DF \u1085\'ED\u1077\'E5 \u1093\'F5\u1086\'EE\u1095\'F7\u1091\'F3 \u1073\'E1\u1099\'FB\u1090\'F2\u1100\'FC \u1090\'F2\u1074\'E2\u1086\'EE\u1080\'E8\u1084\'EC \u1087\'EF\u1088\'F0\u1086\'EE\u1088\'F0\u1086\'EE\u1082\'EA\u1086\'EE\u1084\'EC! \hich\f1 \emdash \loch\f1  \u1074\'E2\u1086\'EE\u1089\'F1\u1082\'EA\u1083\'EB\u1080\'E8\u1082\'EA\u1085\'ED\u1091\'F3\u1083\'EB \u1103\'FF. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1086\'EE \u1084\'EC\u1085\'ED\u1077\'E5 \u1087\'EF\u1086\'EE\u1076\'E4\u1086\'EE\u1096\'F8\u1105\'B8\u1083\'EB \u1073\'E1\u1086\'EE\u1089\'F1\u1089\'F1, \u1103\'FF \u1084\'EC\u1072\'E0\u1096\'F8\u1080\'E8\u1085\'ED\u1072\'E0\u1083\'EB\u1100\'FC\u1085\'ED\u1086\'EE \u1091\'F3\u1082\'EA\u1072\'E0\u1079\'E7\u1072\'E0\u1083\'EB \u1085\'ED\u1072\'E0 \u1087\'EF\u1088\'F0\u1080\'E8\u1085\'ED\u1090\'F2\u1077\'E5\u1088\'F0. \u1054\'CE\u1085\'ED \u1091\'F3\u1076\'E4\u1080\'E8\u1074\'E2\u1083\'EB\u1077\'E5\u1085\'ED\u1085\'ED\u1086\'EE \u1074\'E2\u1089\'F1\u1082\'EA\u1080\'E8\u1085\'ED\u1091\'F3\u1083\'EB \u1073\'E1\u1088\'F0\u1086\'EE\u1074\'E2\u1080\'E8, \u1074\'E2\u1079\'E7\u1103\'FF\u1083\'EB \u1083\'EB\u1080\'E8\u1089\'F1\u1090\'F2\u1099\'FB \u1080\'E8 \u1091\'F3\u1096\'F8\u1077\'E5\u1083\'EB. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1071\'DF \u1074\'E2\u1077\'E5\u1076\'E4\u1100\'FC \u1086\'EE\u1090\'F2 \u1090\'F2\u1077\'E5\u1073\'E1\u1103\'FF \u1085\'ED\u1077\'E5 \u1086\'EE\u1090\'F2\u1089\'F1\u1090\'F2\u1072\'E0\u1085\'ED\u1091\'F3! \hich\f1 \emdash \loch\f1  \u1090\'F2\u1077\'E5\u1087\'EF\u1077\'E5\u1088\'F0\u1100\'FC \u8220\'93\u1041\'C1\u1086\'EE\u1075\'E3\u8221\'94 \u1084\'EC\u1085\'ED\u1077\'E5 \u1091\'F3\u1075\'E3\u1088\'F0\u1086\'EE\u1078\'E6\u1072\'E0\u1083\'EB. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1055\'CF\u1083\'EB\u1077\'E5\u1074\'E2\u1072\'E0\u1090\'F2\u1100\'FC! \hich\f1 \emdash \loch\f1  \u1086\'EE\u1090\'F2\u1084\'EC\u1072\'E0\u1093\'F5\u1085\'ED\u1091\'F3\u1083\'EB\u1089\'F1\u1103\'FF \u1103\'FF. }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1071\'DF \u1086\'EE\u1096\'F8\u1080\'E8\u1073\'E1\u1072\'E0\u1083\'EB\u1089\'F1\u1103\'FF. \u1054\'CE\u1085\'ED \u1073\'E1\u1077\'E5\u1089\'F1\u1082\'EA\u1086\'EE\u1085\'ED\u1077\'E5\u1095\'F7\u1085\'ED\u1086\'EE \u1090\'F2\u1072\'E0\u1088\'F0\u1072\'E0\u1090\'F2\u1086\'EE\u1088\'F0\u1080\'E8\u1083\'EB, \u1088\'F0\u1072\'E0\u1089\'F1\u1089\'F1\u1082\'EA\u1072\'E0\u1079\'E7\u1099\'FB\u1074\'E2\u1072\'E0\u1103\'FF \u1084\'EC\u1085\'ED\u1077\'E5 \u1086\'EE \u1085\'ED\u1086\'EE\u1074\'E2\u1099\'FB\u1093\'F5 \u1090\'F2\u1077\'E5\u1093\'F5\u1085\'ED\u1086\'EE\u1083\'EB\u1086\'EE\u1075\'E3\u1080\'E8\u1103\'FF\u1093\'F5, \u1085\'ED\u1072\'E0\u1091\'F3\u1082\'EA\u1072\'E0\u1093\'F5, \u1073\'E1\u1091\'F3\u1076\'E4\u1091\'F3\u1097\'F9\u1080\'E8\u1093\'F5 \u1086\'EE\u1090\'F2\u1082\'EA\u1088\'F0\u1099\'FB\u1090\'F2\u1080\'E8\u1103\'FF\u1093\'F5\u8230\'85 }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1063\'D7\u1077\'E5\u1088\'F0\u1077\'E5\u1079\'E7 \u1085\'ED\u1077\'E5\u1089\'F1\u1082\'EA\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1089\'F1\u1091\'F3\u1090\'F2\u1086\'EE\u1082\'EA \u1103\'FF \u1085\'ED\u1077\'E5 \u1074\'E2\u1099\'FB\u1076\'E4\u1077\'E5\u1088\'F0\u1078\'E6\u1072\'E0\u1083\'EB. \u1054\'CE\u1085\'ED \u1085\'ED\u1077\'E5 \u1089\'F1\u1083\'EB\u1091\'F3\u1096\'F8\u1072\'E0\u1083\'EB \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF, \u1087\'EF\u1086\'EE\u1089\'F1\u1090\'F2\u1086\'EE\u1103\'FF\u1085\'ED\u1085\'ED\u1086\'EE \u1091\'F3\u1075\'E3\u1086\'EE\u1074\'E2\u1072\'E0\u1088\'F0\u1080\'E8\u1074\'E2\u1072\'E0\u1103\'FF \u1080\'E8 \u1085\'ED\u1077\'E5 \u1074\'E2\u1086\'EE\u1089\'F1\u1087\'EF\u1088\'F0\u1080\'E8\u1085\'ED\u1080\'E8\u1084\'EC\u1072\'E0\u1103\'FF \u1084\'EC\u1086\'EE\u1080\'E8\u1093\'F5 \u1086\'EE\u1090\'F2\u1082\'EA\u1072\'E0\u1079\'E7\u1086\'EE\u1074\'E2. \u1071\'DF \u1085\'ED\u1072\'E0\u1095\'F7\u1072\'E0\u1083\'EB \u1082\'EA\u1088\'F0\u1080\'E8\u1095\'F7\u1072\'E0\u1090\'F2\u1100\'FC \u1074\'E2\u1089\'F1\u1083\'EB\u1091\'F3\u1093\'F5, \u1085\'ED\u1072\'E0\u1087\'EF\u1091\'F3\u1075\'E3\u1072\'E0\u1083\'EB \u1089\'F1\u1086\'EE\u1089\'F1\u1077\'E5\u1076\'E4\u1077\'E5\u1081\'E9, \u1090\'F2\u1077\'E5 \u1074\'E2\u1099\'FB\u1079\'E7\u1074\'E2\u1072\'E0\u1083\'EB\u1080\'E8 \u1087\'EF\u1086\'EE\u1083\'EB\u1080\'E8\u1094\'F6\u1080\'E8\u1102\'FE, \u1072\'E0 \u1086\'EE\u1085\'ED\u1080\'E8 \u1074\'E2\u1099\'FB\u1073\'E1\u1080\'E8\u1083\'EB\u1080\'E8 \u1076\'E4\u1074\'E2\u1077\'E5\u1088\'F0\u1100\'FC \u1080\'E8 \u1087\'EF\u1086\'EE\u1079\'E7\u1074\'E2\u1072\'E0\u1083\'EB\u1080\'E8 \u1085\'ED\u1072\'E0 \u1087\'EF\u1086\'EE\u1084\'EC\u1086\'EE\u1097\'F9\u1100\'FC \u1084\'EC\u1077\'E5\u1076\'E4\u1080\'E8\u1082\'EA\u1086\'EE\u1074\'E2. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1057\'D1\u1077\'E5\u1081\'E9\u1095\'F7\u1072\'E0\u1089\'F1 \u1074\'E2\u1099\'FB \u1091\'F3\u1089\'F1\u1087\'EF\u1086\'EE\u1082\'EA\u1086\'EE\u1080\'E8\u1090\'F2\u1077\'E5\u1089\'F1\u1100\'FC\u8230\'85 \hich\f1 \emdash \loch\f1  \u1089\'F1\u1087\'EF\u1086\'EE\u1082\'EA\u1086\'EE\u1081\'E9\u1085\'ED\u1086\'EE \u1089\'F1\u1082\'EA\u1072\'E0\u1079\'E7\u1072\'E0\u1083\'EB \u1084\'EC\u1085\'ED\u1077\'E5 \u1084\'EC\u1077\'E5\u1076\'E4\u1073\'E1\u1088\'F0\u1072\'E0\u1090\'F2, \u1087\'EF\u1086\'EE\u1082\'EA\u1072\'E0 \u1086\'EE\u1088\'F0\u1091\'F3\u1097\'F9\u1077\'E5\u1075\'E3\u1086\'EE \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1089\'F1\u1082\'EA\u1088\'F0\u1091\'F3\u1095\'F7\u1080\'E8\u1074\'E2\u1072\'E0\u1083\'EB\u1080\'E8, \u1095\'F7\u1090\'F2\u1086\'EE\u1073\'E1\u1099\'FB \u1087\'EF\u1086\'EE\u1089\'F1\u1090\'F2\u1072\'E0\u1074\'E2\u1080\'E8\u1090\'F2\u1100\'FC \u1091\'F3\u1082\'EA\u1086\'EE\u1083\'EB. \u1071\'DF \u1089\'F1\u1086\'EE\u1087\'EF\u1088\'F0\u1086\'EE\u1090\'F2\u1080\'E8\u1074\'E2\u1083\'EB\u1103\'FF\u1083\'EB\u1089\'F1\u1103\'FF. }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1054\'CE\u1090\'F2\u1074\'E2\u1072\'E0\u1083\'EB\u1080\'E8 \u1086\'EE\u1090\'F2 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF! \hich\f1 \emdash \loch\f1  \u1082\'EA\u1088\'F0\u1080\'E8\u1095\'F7\u1072\'E0\u1083\'EB \u1103\'FF \u1074\'E2 \u1087\'EF\u1091\'F3\u1089\'F1\u1090\'F2\u1086\'EE\u1090\'F2\u1091\'F3, \u1072\'E0 "\u1041\'C1\u1086\'EE\u1075\'E3" \u1074\'E2\u1089\'F1\u1077\'E5 \u1090\'F2\u1072\'E0\u1088\'F0\u1072\'E0\u1090\'F2\u1086\'EE\u1088\'F0\u1080\'E8\u1083\'EB \u1086\'EE \u1076\'E4\u1072\'E0\u1083\'EB\u1077\'E5\u1082\'EA\u1080\'E8\u1093\'F5 \u1075\'E3\u1072\'E0\u1083\'EB\u1072\'E0\u1082\'EA\u1090\'F2\u1080\'E8\u1082\'EA\u1072\'E0\u1093\'F5, \u1085\'ED\u1086\'EE\u1074\'E2\u1099\'FB\u1093\'F5 \u1101\'FD\u1083\'EB\u1077\'E5\u1084\'EC\u1077\'E5\u1085\'ED\u1090\'F2\u1072\'E0\u1093\'F5 \u1074\'E2 \u1090\'F2\u1072\'E0\u1073\'E1\u1083\'EB\u1080\'E8\u1094\'F6\u1077\'E5 \u1052\'CC\u1077\'E5\u1085\'ED\u1076\'E4\u1077\'E5\u1083\'EB\u1077\'E5\u1077\'E5\u1074\'E2\u1072\'E0, \u1089\'F1\u1090\'F2\u1072\'E0\u1074\'E2\u1082\'EA\u1072\'E0\u1093\'F5 \u1085\'ED\u1072\'E0 \u1089\'F1\u1087\'EF\u1086\'EE\u1088\'F0\u1090\'F2, \u1076\'E4\u1078\'E6\u1077\'E5\u1082\'EA\u1087\'EF\u1086\'EE\u1090\'F2\u1072\'E0\u1093\'F5 \u1083\'EB\u1086\'EE\u1090\'F2\u1077\'E5\u1088\'F0\u1077\'E5\u1081\'E9 \hich\f1 \emdash \loch\f1  \u1074\'E2\u1089\'F1\u1077\'E5 \u1101\'FD\u1090\'F2\u1086\'EE \u1073\'E1\u1077\'E5\u1079\'E7 \u1082\'EA\u1086\'EE\u1085\'ED\u1094\'F6\u1072\'E0. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1052\'CC\u1077\'E5\u1085\'ED\u1103\'FF \u1091\'F3\u1082\'EA\u1086\'EE\u1083\'EB\u1086\'EE\u1083\'EB\u1080\'E8 \u1080\'E8 \u1077\'E5\u1075\'E3\u1086\'EE \u1075\'E3\u1086\'EE\u1083\'EB\u1086\'EE\u1089\'F1 \u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB \u1079\'E7\u1072\'E0\u1090\'F2\u1080\'E8\u1093\'F5\u1072\'E0\u1090\'F2\u1100\'FC, \u1077\'E5\u1075\'E3\u1086\'EE \u1087\'EF\u1086\'EE\u1089\'F1\u1083\'EB\u1077\'E5\u1076\'E4\u1085\'ED\u1077\'E5\u1081\'E9 \u1092\'F4\u1088\'F0\u1072\'E0\u1079\'E7\u1086\'EE\u1081\'E9 \u1073\'E1\u1099\'FB\u1083\'EB\u1086\'EE \u1075\'E3\u1088\'F0\u1091\'F3\u1089\'F1\u1090\'F2\u1085\'ED\u1086\'EE\u1077\'E5: }
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1052\'CC\u1080\'E8\u1085\'ED\u1091\'F3\u1089\'F1 \u1086\'EE\u1076\'E4\u1080\'E8\u1085\'ED\u8230\'85}}