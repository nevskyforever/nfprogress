\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u1044\'C4\u1086\'EE\u1084\'EC\u1072\'E0 \u1074\'E2\u1089\'F1\u1077\'E5 \u1073\'E1\u1099\'FB\u1083\'EB\u1086\'EE \u1090\'F2\u1072\'E0\u1082\'EA \u1078\'E6\u1077\'E5 \u1090\'F2\u1080\'E8\u1093\'F5\u1086\'EE \u1080\'E8 \u1089\'F1\u1087\'EF\u1086\'EE\u1082\'EA\u1086\'EE\u1081\'E9\u1085\'ED\u1086\'EE, \u1084\'EC\u1086\'EE\u1077\'E5\u1075\'E3\u1086\'EE \u1080\'E8\u1089\'F1\u1095\'F7\u1077\'E5\u1079\'E7\u1085\'ED\u1086\'EE\u1074\'E2\u1077\'E5\u1085\'ED\u1080\'E8\u1103\'FF \u1085\'ED\u1080\'E8\u1082\'EA\u1090\'F2\u1086\'EE \u1085\'ED\u1077\'E5 \u1079\'E7\u1072\'E0\u1084\'EC\u1077\'E5\u1090\'F2\u1080\'E8\u1083\'EB. \u1050\'CA\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0 \u1074\'E2\u1099\'FB\u1093\'F5\u1086\'EE\u1076\'E4\u1082\'EA\u1072\'E0 \u1073\'E1\u1099\'FB\u1083\'EB\u1072\'E0 \u1089\'F1\u1086\'EE\u1074\'E2\u1077\'E5\u1088\'F0\u1096\'F8\u1077\'E5\u1085\'ED\u1072\'E0, \u1084\'EC\u1085\'ED\u1077\'E5 \u1091\'F3\u1078\'E6\u1077\'E5 \u1073\'E1\u1099\'FB\u1083\'EB\u1086\'EE \u1074\'E2\u1089\'F1\u1077\'E5 \u1088\'F0\u1072\'E0\u1074\'E2\u1085\'ED\u1086\'EE. \u1040\'C0\u1073\'E1\u1089\'F1\u1086\'EE\u1083\'EB\u1102\'FE\u1090\'F2\u1085\'ED\u1086\'EE \u1089\'F1\u1087\'EF\u1086\'EE\u1082\'EA\u1086\'EE\u1081\'E9\u1085\'ED\u1086\'EE \u1103\'FF \u1079\'E7\u1072\'E0\u1089\'F1\u1090\'F2\u1080\'E8\u1088\'F0\u1072\'E0\u1083\'EB \u1076\'E4\u1078\'E6\u1080\'E8\u1085\'ED\u1089\'F1\u1099\'FB \u1085\'ED\u1072\'E0 \u1088\'F0\u1091\'F3\u1082\'EA\u1072\'E0\u1093\'F5 \u1080\'E8 \u1086\'EE\u1087\'EF\u1088\'F0\u1072\'E0\u1074\'E2\u1080\'E8\u1083\'EB\u1089\'F1\u1103\'FF \u1089\'F1\u1087\'EF\u1072\'E0\u1090\'F2\u1100\'FC, \u1087\'EF\u1088\'F0\u1080\'E8\u1076\'E4\u1091\'F3\u1084\'EC\u1072\'E0\u1074\'E2 \u1080\'E8\u1089\'F1\u1090\'F2\u1086\'EE\u1088\'F0\u1080\'E8\u1102\'FE \u1086\'EE \u1090\'F2\u1086\'EE\u1084\'EC, \u1095\'F7\u1090\'F2\u1086\'EE \u1074\'E2\u1099\'FB\u1096\'F8\u1077\'E5\u1083\'EB \u1085\'ED\u1086\'EE\u1095\'F7\u1100\'FC\u1102\'FE \u1074\'E2 \u1090\'F2\u1091\'F3\u1072\'E0\u1083\'EB\u1077\'E5\u1090\'F2 \u1085\'ED\u1072\'E0 \u1091\'F3\u1083\'EB\u1080\'E8\u1094\'F6\u1091\'F3, \u1089\'F1\u1087\'EF\u1086\'EE\u1090\'F2\u1082\'EA\u1085\'ED\u1091\'F3\u1083\'EB\u1089\'F1\u1103\'FF \u1080\'E8 \u1091\'F3\u1087\'EF\u1072\'E0\u1083\'EB. \u1058\'D2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1089\'F1\u1077\'E5\u1081\'E9\u1095\'F7\u1072\'E0\u1089\'F1 \u1084\'EC\u1085\'ED\u1077\'E5 \u1082\'EA\u1072\'E0\u1078\'E6\u1077\'E5\u1090\'F2\u1089\'F1\u1103\'FF \u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1085\'ED\u1085\'ED\u1099\'FB\u1084\'EC \u1088\'F0\u1072\'E0\u1076\'E4\u1080\'E8 \u1101\'FD\u1090\'F2\u1086\'EE\u1075\'E3\u1086\'EE \u1085\'ED\u1072\'E0\u1087\'EF\u1103\'FF\u1083\'EB\u1080\'E8\u1074\'E2\u1072\'E0\u1090\'F2\u1100\'FC \u1085\'ED\u1072\'E0 \u1089\'F1\u1077\'E5\u1073\'E1\u1103\'FF \u1076\'E4\u1078\'E6\u1080\'E8\u1085\'ED\u1089\'F1\u1099\'FB, \u1085\'ED\u1086\'EE \u1090\'F2\u1086\'EE\u1075\'E3\u1076\'E4\u1072\'E0 \u1083\'EB\u1080\'E8\u1096\'F8\u1085\'ED\u1080\'E8\u1093\'F5 \u1074\'E2\u1086\'EE\u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1086\'EE\u1074\'E2 \u1085\'ED\u1077\'E5 \u1074\'E2\u1086\'EE\u1079\'E7\u1085\'ED\u1080\'E8\u1082\'EA\u1083\'EB\u1086\'EE\u8230\'85}
