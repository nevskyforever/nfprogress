\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1053\'CD\u1072\'E0 \u1084\'EC\u1086\'EE\u1085\'ED\u1080\'E8\u1090\'F2\u1086\'EE\u1088\'F0\u1077\'E5 \hich\f1 \emdash \loch\f1  \u1090\'F2\u1099\'FB\u1089\'F1\u1103\'FF\u1095\'F7\u1080\'E8 \u1089\'F1\u1090\'F2\u1088\'F0\u1086\'EE\u1082\'EA \u1085\'ED\u1077\'E5\u1086\'EE\u1073\'E1\u1088\'F0\u1072\'E0\u1073\'E1\u1086\'EE\u1090\'F2\u1072\'E0\u1085\'ED\u1085\'ED\u1086\'EE\u1081\'E9 \u1080\'E8\u1085\'ED\u1092\'F4\u1086\'EE\u1088\'F0\u1084\'EC\u1072\'E0\u1094\'F6\u1080\'E8\u1080\'E8. \u1063\'D7\u1077\'E5\u1088\'F0\u1077\'E5\u1079\'E7 \u1090\'F2\u1088\'F0\u1080\'E8 \u1076\'E4\u1085\'ED\u1103\'FF \u1089\'F1\u1076\'E4\u1072\'E0\u1074\'E2\u1072\'E0\u1090\'F2\u1100\'FC \u1082\'EA\u1074\'E2\u1072\'E0\u1088\'F0\u1090\'F2\u1072\'E0\u1083\'EB\u1100\'FC\u1085\'ED\u1099\'FB\u1081\'E9 \u1086\'EE\u1090\'F2\u1095\'F7\u1077\'E5\u1090\'F2, \u1072\'E0 \u1103\'FF\u8230\'85 \u1086\'EE\u1087\'EF\u1103\'FF\u1090\'F2\u1100\'FC \u1086\'EE\u1073\'E1\u1083\'EB\u1072\'E0\u1078\'E6\u1072\'E0\u1083\'EB\u1089\'F1\u1103\'FF. \u1042\'C2\u1089\'F1\u1077\'E5 \u1101\'FD\u1090\'F2\u1080\'E8 \u1095\'F7\u1077\'E5\u1088\'F0\u1090\'F2\u1086\'EE\u1074\'E2\u1099\'FB \u8220\'93\u1044\'C4\u1072\'E0\u1074\'E2\u1072\'E0\u1081\'E9 \u1077\'E5\u1097\'F9\u1077\'E5 \u1086\'EE\u1076\'E4\u1085\'ED\u1091\'F3 \u1089\'F1\u1077\'E5\u1088\'F0\u1080\'E8\u1102\'FE\u8221\'94, \u8220\'93\u1054\'CE, \u1091\'F3 \u1051\'CB\u1091\'F3\u1082\'EA\u1100\'FC\u1103\'FF\u1085\'ED\u1077\'E5\u1085\'ED\u1082\'EA\u1086\'EE \u1085\'ED\u1086\'EE\u1074\'E2\u1072\'E0\u1103\'FF \u1082\'EA\u1085\'ED\u1080\'E8\u1075\'E3\u1072\'E0!\u8221\'94 \u1080\'E8 \u8220\'93\u1061\'D5\u1086\'EE\u1088\'F0\u1086\'EE\u1096\'F8\u1080\'E8\u1081\'E9 \u1073\'E1\u1099\'FB\u1083\'EB \u1076\'E4\u1077\'E5\u1085\'ED\u1100\'FC, \u1085\'ED\u1072\'E0\u1076\'E4\u1086\'EE \u1088\'F0\u1072\'E0\u1089\'F1\u1089\'F1\u1083\'EB\u1072\'E0\u1073\'E1\u1080\'E8\u1090\'F2\u1100\'FC\u1089\'F1\u1103\'FF\u8221\'94 \hich\f1 \emdash \loch\f1  \u1089\'F1\u1086\'EE\u1078\'E6\u1088\'F0\u1072\'E0\u1083\'EB\u1080\'E8 \u1084\'EC\u1086\'EE\u1077\'E5 \u1074\'E2\u1088\'F0\u1077\'E5\u1084\'EC\u1103\'FF. \u1044\'C4\u1085\'ED\u1080\'E8 \u1091\'F3\u1076\'E4\u1086\'EE\u1074\'E2\u1086\'EE\u1083\'EB\u1100\'FC\u1089\'F1\u1090\'F2\u1074\'E2\u1080\'E8\u1103\'FF \u1074\'E2 \u1086\'EE\u1073\'E1\u1084\'EC\u1077\'E5\u1085\'ED \u1085\'ED\u1072\'E0 72 \u1095\'F7\u1072\'E0\u1089\'F1\u1072\'E0 \u1085\'ED\u1077\'E5\u1089\'F1\u1095\'F7\u1072\'E0\u1089\'F1\u1090\'F2\u1100\'FC\u1103\'FF. \u1053\'CD\u1077\'E5\u1085\'ED\u1072\'E0\u1074\'E2\u1080\'E8\u1078\'E6\u1091\'F3. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1040\'C0 \u1074\'E2\u1077\'E5\u1076\'E4\u1100\'FC \u1074\'E2\u1089\'F1\u1077\'E5 \u1084\'EC\u1086\'EE\u1075\'E3\u1083\'EB\u1086\'EE \u1073\'E1\u1099\'FB\u1090\'F2\u1100\'FC \u1080\'E8\u1085\'ED\u1072\'E0\u1095\'F7\u1077\'E5, \u1085\'ED\u1077\'E5 \u1079\'E7\u1072\'E0\u1073\'E1\u1080\'E8\u1074\'E2\u1072\'E0\u1081\'E9 \u1103\'FF \u1085\'ED\u1072\'E0 \u1101\'FD\u1082\'EA\u1079\'E7\u1072\'E0\u1084\'EC\u1077\'E5\u1085\'ED\u1099\'FB. \u1057\'D1\u1090\'F2\u1072\'E0\u1083\'EB \u1073\'E1\u1099\'FB \u1088\'F0\u1077\'E5\u1078\'E6\u1080\'E8\u1089\'F1\u1089\'F1\u1077\'E5\u1088\'F0\u1086\'EE\u1084\'EC, \u1072\'E0 \u1085\'ED\u1077\'E5 \u1084\'EC\u1072\'E0\u1088\'F0\u1082\'EA\u1077\'E5\u1090\'F2\u1086\'EE\u1083\'EB\u1086\'EE\u1075\'E3\u1086\'EE\u1084\'EC. \u1040\'C0 \u1077\'E5\u1089\'F1\u1083\'EB\u1080\'E8 \u1073\'E1\u1099\'FB \u1080\'E8 \u1087\'EF\u1080\'E8\u1089\'F1\u1072\'E0\u1090\'F2\u1100\'FC \u1085\'ED\u1077\'E5 \u1073\'E1\u1088\'F0\u1086\'EE\u1089\'F1\u1080\'E8\u1083\'EB, \u1090\'F2\u1072\'E0\u1082\'EA \u1074\'E2\u1086\'EE\u1086\'EE\u1073\'E1\u1097\'F9\u1077\'E5\u8230\'85 }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1050\'CA\u1086\'EE\u1092\'F4\u1077\'E5 \hich\f1 \emdash \loch\f1  \u1074\'E2\u1086\'EE\u1090\'F2, \u1095\'F7\u1090\'F2\u1086\'EE \u1084\'EC\u1085\'ED\u1077\'E5 \u1085\'ED\u1091\'F3\u1078\'E6\u1085\'ED\u1086\'EE. }
\par\plain \f1\fs24\b0\i0
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1044\'C4\u1074\'E2\u1080\'E8\u1075\'E3\u1072\'E0\u1102\'FE\u1089\'F1\u1100\'FC \u1074\'E2 \u1082\'EA\u1072\'E0\u1092\'F4\u1077\'E5\u1090\'F2\u1077\'E5\u1088\'F0\u1080\'E8\u1081\'E9. \u1057\'D1\u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1079\'E7\u1085\'ED\u1072\'E0\u1082\'EA\u1086\'EE\u1084\'EC\u1099\'FB\u1093\'F5 \u1083\'EB\u1080\'E8\u1094\'F6, \u1085\'ED\u1086\'EE \u1095\'F7\u1090\'F2\u1086\'EE \u1079\'E7\u1072\'E0 \u1085\'ED\u1080\'E8\u1084\'EC\u1080\'E8 \hich\f1 \emdash \loch\f1  \u1103\'FF \u1085\'ED\u1077\'E5 \u1079\'E7\u1085\'ED\u1072\'E0\u1102\'FE. \u1071\'DF \u1079\'E7\u1076\'E4\u1077\'E5\u1089\'F1\u1100\'FC \u1083\'EB\u1080\'E8\u1096\'F8\u1100\'FC \u1096\'F8\u1077\'E5\u1089\'F1\u1090\'F2\u1077\'E5\u1088\'F0\u1077\'E5\u1085\'ED\u1082\'EA\u1072\'E0 \u1080\'E8 \u1084\'EC\u1085\'ED\u1077\'E5 \u1079\'E7\u1085\'ED\u1072\'E0\u1082\'EA\u1086\'EE\u1084\'EC\u1099\'FB \u1090\'F2\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1086\'EE \u1073\'E1\u1083\'EB\u1080\'E8\u1079\'E7\u1082\'EA\u1080\'E8\u1077\'E5 \u1082\'EA\u1086\'EE \u1084\'EC\u1085\'ED\u1077\'E5 \u1082\'EA\u1086\'EE\u1084\'EC\u1087\'EF\u1086\'EE\u1085\'ED\u1077\'E5\u1085\'ED\u1090\'F2\u1099\'FB. \u1052\'CC\u1077\'E5\u1085\'ED\u1103\'FF \u1086\'EE\u1073\'E1\u1075\'E3\u1086\'EE\u1085\'ED\u1103\'FF\u1077\'E5\u1090\'F2 \u1079\'E7\u1085\'ED\u1072\'E0\u1082\'EA\u1086\'EE\u1084\'EC\u1072\'E0\u1103\'FF \u1092\'F4\u1080\'E8\u1075\'E3\u1091\'F3\u1088\'F0\u1072\'E0. \u1057\'D1\u1082\'EA\u1086\'EE\u1088\'F0\u1086\'EE \u1091\'F3\u1074\'E2\u1080\'E8\u1076\'E4\u1080\'E8\u1084\'EC\u1089\'F1\u1103\'FF. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1058\'D2\u1077\'E5\u1083\'EB\u1077\'E5\u1092\'F4\u1086\'EE\u1085\'ED \u1079\'E7\u1074\'E2\u1077\'E5\u1085\'ED\u1080\'E8\u1090\'F2 \u1089\'F1\u1077\'E5\u1082\'EA\u1091\'F3\u1085\'ED\u1076\'E4 \u1087\'EF\u1103\'FF\u1090\'F2\u1100\'FC, \u1085\'ED\u1077\'E5 \u1087\'EF\u1088\'F0\u1077\'E5\u1082\'EA\u1088\'F0\u1072\'E0\u1097\'F9\u1072\'E0\u1103\'FF \hich\f1 \emdash \loch\f1  \u1090\'F2\u1080\'E8\u1093\'F5\u1080\'E8\u1081\'E9 \u1088\'F0\u1077\'E5\u1078\'E6\u1080\'E8\u1084\'EC \u1086\'EE\u1090\'F2\u1082\'EA\u1083\'EB\u1102\'FE\u1095\'F7\u1080\'E8\u1083\'EB\u1089\'F1\u1103\'FF. \u1042\'C2\u1085\'ED\u1091\'F3\u1090\'F2\u1088\'F0\u1077\'E5\u1085\'ED\u1085\'ED\u1080\'E8\u1081\'E9 \u1088\'F0\u1072\'E0\u1089\'F1\u1087\'EF\u1086\'EE\u1088\'F0\u1103\'FF\u1076\'E4\u1086\'EE\u1082\'EA \u1079\'E7\u1072\'E0\u1087\'EF\u1088\'F0\u1077\'E5\u1097\'F9\u1072\'E0\u1077\'E5\u1090\'F2 \u1088\'F0\u1072\'E0\u1073\'E1\u1086\'EE\u1090\'F2\u1091\'F3 \u1089\'F1 \u1083\'EB\u1080\'E8\u1095\'F7\u1085\'ED\u1099\'FB\u1084\'EC\u1080\'E8 \u1091\'F3\u1089\'F1\u1090\'F2\u1088\'F0\u1086\'EE\u1081\'E9\u1089\'F1\u1090\'F2\u1074\'E2\u1072\'E0\u1084\'EC\u1080\'E8 \u1074\'E2 \u1088\'F0\u1072\'E0\u1073\'E1\u1086\'EE\u1095\'F7\u1077\'E5\u1077\'E5 \u1074\'E2\u1088\'F0\u1077\'E5\u1084\'EC\u1103\'FF, \u1087\'EF\u1088\'F0\u1080\'E8\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1090\'F2\u1089\'F1\u1103\'FF \u1079\'E7\u1072\'E0\u1089\'F1\u1090\'F2\u1072\'E0\u1074\'E2\u1083\'EB\u1103\'FF\u1090\'F2\u1100\'FC \u1080\'E8\u1093\'F5 \u1084\'EC\u1086\'EE\u1083\'EB\u1095\'F7\u1072\'E0\u1090\'F2\u1100\'FC. \u1048\'C8\u1085\'ED\u1072\'E0\u1095\'F7\u1077\'E5 \u1074\'E2\u1077\'E5\u1089\'F1\u1100\'FC \u1086\'EE\u1090\'F2\u1076\'E4\u1077\'E5\u1083\'EB \u1079\'E7\u1074\'E2\u1077\'E5\u1085\'ED\u1077\'E5\u1083\'EB \u1073\'E1\u1099\'FB \u1073\'E1\u1077\'E5\u1079\'E7 \u1086\'EE\u1089\'F1\u1090\'F2\u1072\'E0\u1085\'ED\u1086\'EE\u1074\'E2\u1082\'EA\u1080\'E8. \u1054\'CE\u1076\'E4\u1080\'E8\u1085\'ED \u1086\'EE\u1075\'E3\u1088\'F0\u1086\'EE\u1084\'EC\u1085\'ED\u1099\'FB\u1081\'E9 \u1082\'EA\u1086\'EE\u1083\'EB\u1086\'EE\u1082\'EA\u1086\'EE\u1083\'EB, \u1072\'E0 \u1074\'E2 \u1077\'E5\u1075\'E3\u1086\'EE \u1079\'E7\u1074\'E2\u1086\'EE\u1085\'ED\u1077\'E5 \u1090\'F2\u1099\'FB\u1089\'F1\u1103\'FF\u1095\'F7\u1080\'E8 \u1087\'EF\u1091\'F3\u1089\'F1\u1090\'F2\u1099\'FB\u1093\'F5 \u1091\'F3\u1074\'E2\u1077\'E5\u1076\'E4\u1086\'EE\u1084\'EC\u1083\'EB\u1077\'E5\u1085\'ED\u1080\'E8\u1081\'E9. \u1048\'C8\u1083\'EB\u1083\'EB\u1102\'FE\u1079\'E7\u1080\'E8\u1103\'FF \u1079\'E7\u1085\'ED\u1072\'E0\u1095\'F7\u1080\'E8\u1084\'EC\u1086\'EE\u1089\'F1\u1090\'F2\u1080\'E8. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1044\'C4\u1086\'EE\u1089\'F1\u1090\'F2\u1072\'E0\u1102\'FE \u1090\'F2\u1077\'E5\u1083\'EB\u1077\'E5\u1092\'F4\u1086\'EE\u1085\'ED \u1080\'E8\u1079\'E7 \u1082\'EA\u1072\'E0\u1088\'F0\u1084\'EC\u1072\'E0\u1085\'ED\u1072\'E0. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1051\'CB\u1072\'E0\u1081\'E9\u1082\'EA \u1079\'E7\u1076\'E4\u1077\'E5\u1089\'F1\u1100\'FC. \u1050\'CA\u1086\'EE\u1084\'EC\u1084\'EC\u1077\'E5\u1085\'ED\u1090\'F2\u1072\'E0\u1088\'F0\u1080\'E8\u1081\'E9 \u1090\'F2\u1072\'E0\u1084\'EC. \u1056\'D0\u1077\'E5\u1082\'EA\u1083\'EB\u1072\'E0\u1084\'EC\u1072\'E0. \u1048\'C8 \u1077\'E5\u1097\'F9\u1077\'E5 \u1088\'F0\u1077\'E5\u1082\'EA\u1083\'EB\u1072\'E0\u1084\'EC\u1072\'E0. \u1048\'C8 \u1077\'E5\u1097\'F9\u1077\'E5. \u1054\'CE\u1090\'F2 \u1085\'ED\u1077\'E5\u1105\'B8 \u1085\'ED\u1077\'E5 \u1080\'E8\u1079\'E7\u1073\'E1\u1072\'E0\u1074\'E2\u1080\'E8\u1096\'F8\u1100\'FC\u1089\'F1\u1103\'FF \hich\f1 \emdash \loch\f1  \u1073\'E1\u1088\'F0\u1077\'E5\u1085\'ED\u1076\'E4\u1099\'FB \u1087\'EF\u1088\'F0\u1086\'EE\u1076\'E4\u1072\'E0\u1102\'FE\u1090\'F2 \u1091\'F3\u1089\'F1\u1090\'F2\u1088\'F0\u1086\'EE\u1081\'E9\u1089\'F1\u1090\'F2\u1074\'E2\u1072\'E0 \u1079\'E7\u1072\'E0 \u1073\'E1\u1077\'E5\u1089\'F1\u1094\'F6\u1077\'E5\u1085\'ED\u1086\'EE\u1082\'EA, \u1072\'E0 \u1090\'F2\u1099\'FB \u1087\'EF\u1086\'EE\u1083\'EB\u1100\'FC\u1079\'E7\u1091\'F3\u1077\'E5\u1096\'F8\u1100\'FC\u1089\'F1\u1103\'FF \u1080\'E8\u1093\'F5 \u1089\'F1\u1077\'E5\u1088\'F0\u1074\'E2\u1080\'E8\u1089\'F1\u1072\'E0\u1084\'EC\u1080\'E8\u8230\'85 \u1080\'E8 \u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1080\'E8\u1096\'F8\u1100\'FC \u1080\'E8\u1093\'F5 \u1088\'F0\u1077\'E5\u1082\'EA\u1083\'EB\u1072\'E0\u1084\'EC\u1091\'F3. }
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1059\'D3\u1089\'F1\u1087\'EF\u1077\'E5\u1074\'E2\u1072\'E0\u1102\'FE \u1079\'E7\u1072\'E0\u1089\'F1\u1082\'EA\u1086\'EE\u1095\'F7\u1080\'E8\u1090\'F2\u1100\'FC \u1074\'E2 \u1083\'EB\u1080\'E8\u1092\'F4\u1090\'F2. \u1053\'CD\u1077\'E5\u1085\'ED\u1072\'E0\u1074\'E2\u1080\'E8\u1078\'E6\u1091\'F3 \u1101\'FD\u1090\'F2\u1086\'EE \u1086\'EE\u1097\'F9\u1091\'F3\u1097\'F9\u1077\'E5\u1085\'ED\u1080\'E8\u1077\'E5 \hich\f1 \emdash \loch\f1  \u1074\'E2\u1089\'F1\u1077\'E5 \u1089\'F1\u1086\'EE\u1087\'EF\u1088\'F0\u1080\'E8\u1082\'EA\u1072\'E0\u1089\'F1\u1072\'E0\u1102\'FE\u1090\'F2\u1089\'F1\u1103\'FF \u1089\'F1\u1086\'EE \u1084\'EC\u1085\'ED\u1086\'EE\u1081\'E9. }
