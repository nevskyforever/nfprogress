\pard\plain \li357\fi360\ri-2699\ql\ltrch\loch {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u1047\'C7\u1072\'E0\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1090\'F2 \u1089\'F1\u1086\'EE\u1083\'EB\u1085\'ED\u1094\'F6\u1077\'E5, \u1074\'E2\u1086\'EE\u1076\'E4\u1086\'EE\u1077\'E5\u1084\'EC \u1086\'EE\u1089\'F1\u1090\'F2\u1099\'FB\u1083\'EB. \u1051\'CB\u1077\'E5\u1075\'E3\u1082\'EA\u1080\'E8\u1081\'E9 \u1074\'E2\u1077\'E5\u1090\'F2\u1077\'E5\u1088\'F0 \u1096\'F8\u1077\'E5\u1083\'EB\u1077\'E5\u1089\'F1\u1090\'F2\u1080\'E8\u1090\'F2 \u1082\'EA\u1088\'F0\u1086\'EE\u1085\'ED\u1072\'E0\u1084\'EC\u1080\'E8 \u1076\'E4\u1077\'E5\u1088\'F0\u1077\'E5\u1074\'E2\u1100\'FC\u1077\'E5\u1074\'E2.}
\par\plain {\f0\fs24\b0\i0 \hich\f0 \emdash \loch\f0  \loch\af0\hich\af0\dbch\af0\uc1\u1055\'CF\u1086\'EE\u1088\'F0\u1072\'E0 \u1076\'E4\u1086\'EE\u1084\'EC\u1086\'EE\u1081\'E9, \hich\f0 \emdash \loch\f0  \u1075\'E3\u1086\'EE\u1074\'E2\u1086\'EE\u1088\'F0\u1080\'E8\u1090\'F2 \u1050\'CA\u1072\'E0\u1090\'F2\u1103\'FF, \u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1103\'FF \u1074\'E2\u1076\'E4\u1072\'E0\u1083\'EB\u1100\'FC. \u1047\'C7\u1072\'E0 \u1074\'E2\u1077\'E5\u1089\'F1\u1100\'FC \u1076\'E4\u1077\'E5\u1085\'ED\u1100\'FC \u1086\'EE\u1085\'ED\u1072\'E0 \u1085\'ED\u1080\'E8 \u1088\'F0\u1072\'E0\u1079\'E7\u1091\'F3 \u1085\'ED\u1077\'E5 \u1087\'EF\u1086\'EE\u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1077\'E5\u1083\'EB\u1072\'E0 \u1085\'ED\u1072\'E0 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF.}
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u1048\'C8\u1102\'FE\u1085\'ED\u1100\'FC \u1087\'EF\u1088\'F0\u1086\'EE\u1083\'EB\u1077\'E5\u1090\'F2\u1077\'E5\u1083\'EB \u1090\'F2\u1072\'E0\u1082\'EA \u1073\'E1\u1099\'FB\u1089\'F1\u1090\'F2\u1088\'F0\u1086\'EE, \u1095\'F7\u1090\'F2\u1086\'EE \u1103\'FF \u1085\'ED\u1072\'E0\u1095\'F7\u1072\'E0\u1083\'EB \u1073\'E1\u1086\'EE\u1103\'FF\u1090\'F2\u1100\'FC\u1089\'F1\u1103\'FF \u1079\'E7\u1072\'E0 \u1074\'E2\u1089\'F1\u1077\'E5 \u1086\'EE\u1089\'F1\u1090\'F2\u1072\'E0\u1083\'EB\u1100\'FC\u1085\'ED\u1086\'EE\u1077\'E5 \u1083\'EB\u1077\'E5\u1090\'F2\u1086\'EE. \u1053\'CD\u1080\'E8 \u1076\'E4\u1085\'ED\u1103\'FF \u1074\'E2 \u1095\'F7\u1077\'E5\u1090\'F2\u1099\'FB\u1088\'F0\u1077\'E5\u1093\'F5 \u1089\'F1\u1090\'F2\u1077\'E5\u1085\'ED\u1072\'E0\u1093\'F5, \u1076\'E4\u1072\'E0\u1078\'E6\u1077\'E5 \u1074\'E2 \u1076\'E4\u1086\'EE\u1078\'E6\u1076\'E4\u1080\'E8 \u1084\'EC\u1099\'FB \u1076\'E4\u1088\'F0\u1091\'F3\u1078\'E6\u1085\'ED\u1086\'EE \u1074\'E2\u1099\'FB\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1083\'EB\u1080\'E8 \u1085\'ED\u1072\'E0 \u1091\'F3\u1083\'EB\u1080\'E8\u1094\'F6\u1091\'F3 \u1080\'E8 \u1074\'E2 \u1073\'E1\u1086\'EE\u1083\'EB\u1090\'F2\u1086\'EE\u1074\'E2\u1085\'ED\u1077\'E5 \u1086\'EE\u1073\'E1\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1083\'EB\u1080\'E8 \u1075\'E3\u1086\'EE\u1088\'F0\u1086\'EE\u1076\'E4 \u1087\'EF\u1086\'EE \u1085\'ED\u1077\'E5\u1089\'F1\u1082\'EA\u1086\'EE\u1083\'EB\u1100\'FC\u1082\'EA\u1091\'F3 \u1088\'F0\u1072\'E0\u1079\'E7 \u1079\'E7\u1072\'E0 \u1076\'E4\u1077\'E5\u1085\'ED\u1100\'FC. \u1042\'C2\u1086\'EE \u1076\'E4\u1074\'E2\u1086\'EE\u1088\'F0\u1072\'E0\u1093\'F5 \u1084\'EC\u1077\'E5\u1083\'EB\u1100\'FC\u1082\'EA\u1072\'E0\u1083\'EB\u1080\'E8 \u1076\'E4\u1077\'E5\u1090\'F2\u1080\'E8 \u1080\'E8 \u1089\'F1\u1074\'E2\u1077\'E5\u1088\'F0\u1089\'F1\u1090\'F2\u1085\'ED\u1080\'E8\u1082\'EA\u1080\'E8, \u1086\'EE\u1089\'F1\u1074\'E2\u1086\'EE\u1073\'E1\u1086\'EE\u1078\'E6\u1076\'E4\u1077\'E5\u1085\'ED\u1085\'ED\u1099\'FB\u1077\'E5 \u1086\'EE\u1090\'F2 \u1096\'F8\u1082\'EA\u1086\'EE\u1083\'EB\u1100\'FC\u1085\'ED\u1086\'EE\u1081\'E9 \u1082\'EA\u1072\'E0\u1090\'F2\u1086\'EE\u1088\'F0\u1075\'E3\u1080\'E8, \u1082\'EA\u1072\'E0\u1082\'EA \u1084\'EC\u1099\'FB.}
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u1055\'CF\u1086\'EE\u1089\'F1\u1083\'EB\u1077\'E5 \u1086\'EE\u1095\'F7\u1077\'E5\u1088\'F0\u1077\'E5\u1076\'E4\u1085\'ED\u1086\'EE\u1075\'E3\u1086\'EE \u1096\'F8\u1091\'F3\u1084\'EC\u1085\'ED\u1086\'EE\u1075\'E3\u1086\'EE \u1080\'E8 \u1074\'E2\u1077\'E5\u1089\'F1\u1077\'E5\u1083\'EB\u1086\'EE\u1075\'E3\u1086\'EE \u1076\'E4\u1085\'ED\u1103\'FF \u1042\'C2\u1072\'E0\u1085\'ED\u1103\'FF \u1080\'E8 \u1050\'CA\u1086\'EE\u1089\'F1\u1090\'F2\u1103\'FF \hich\f0 \emdash \loch\f0  \u1085\'ED\u1072\'E0\u1096\'F8\u1080\'E8 \u1076\'E4\u1088\'F0\u1091\'F3\u1079\'E7\u1100\'FC\u1103\'FF \hich\f0 \emdash \loch\f0  \u1091\'F3\u1096\'F8\u1083\'EB\u1080\'E8 \u1076\'E4\u1086\'EE\u1084\'EC\u1086\'EE\u1081\'E9, \u1072\'E0 \u1084\'EC\u1099\'FB \u1089\'F1 \u1050\'CA\u1072\'E0\u1090\'F2\u1077\'E5\u1081\'E9 \u1086\'EE\u1090\'F2\u1087\'EF\u1088\'F0\u1072\'E0\u1074\'E2\u1080\'E8\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1085\'ED\u1072\'E0 \u1074\'E2\u1086\'EE\u1076\'E4\u1086\'EE\u1077\'E5\u1084\'EC. \u1057\'D1\u1085\'ED\u1086\'EE\u1074\'E2\u1072\'E0. \u1059\'D3\u1089\'F1\u1090\'F2\u1072\'E0\u1074\'E2\u1096\'F8\u1080\'E8\u1077\'E5 \u1086\'EE\u1090\'F2 \u1087\'EF\u1086\'EE\u1077\'E5\u1079\'E7\u1076\'E4\u1086\'EE\u1082\'EA \u1085\'ED\u1072\'E0 \u1074\'E2\u1077\'E5\u1083\'EB\u1086\'EE\u1089\'F1\u1080\'E8\u1087\'EF\u1077\'E5\u1076\'E4\u1077\'E5 \u1080\'E8 \u1085\'ED\u1072\'E0\u1083\'EB\u1077\'E5\u1090\'F2\u1086\'EE\u1074\'E2 \u1085\'ED\u1072\'E0 \u1089\'F1\u1091\'F3\u1087\'EF\u1077\'E5\u1088\'F0\u1084\'EC\u1072\'E0\u1088\'F0\u1082\'EA\u1077\'E5\u1090\'F2\u1099\'FB, \u1084\'EC\u1099\'FB \u1087\'EF\u1088\'F0\u1080\'E8\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1083\'EB\u1080\'E8 \u1089\'F1\u1102\'FE\u1076\'E4\u1072\'E0 \hich\f0 \emdash \loch\f0  \u1082\'EA \u1074\'E2\u1086\'EE\u1076\'E4\u1077\'E5. \u1055\'CF\u1088\'F0\u1080\'E8\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1083\'EB\u1080\'E8 \u1080\'E8 \u1084\'EC\u1086\'EE\u1083\'EB\u1095\'F7\u1072\'E0\u1083\'EB\u1080\'E8, \u1082\'EA\u1072\'E0\u1082\'EA \u1073\'E1\u1091\'F3\u1076\'E4\u1090\'F2\u1086\'EE \u1074\'E2\u1089\'F1\u1077\'E5 \u1089\'F1\u1083\'EB\u1086\'EE\u1074\'E2\u1072\'E0 \u1073\'E1\u1099\'FB\u1083\'EB\u1080\'E8 \u1089\'F1\u1082\'EA\u1072\'E0\u1079\'E7\u1072\'E0\u1085\'ED\u1099\'FB.}
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u1042\'C2 \u1082\'EA\u1072\'E0\u1082\'EA\u1086\'EE\u1081\'E9-\u1090\'F2\u1086\'EE \u1084\'EC\u1086\'EE\u1084\'EC\u1077\'E5\u1085\'ED\u1090\'F2 \u1050\'CA\u1072\'E0\u1090\'F2\u1077\'E5 \u1079\'E7\u1074\'E2\u1086\'EE\u1085\'ED\u1080\'E8\u1083\'EB\u1072\'E0 \u1084\'EC\u1072\'E0\u1084\'EC\u1072\'E0 \u1089\'F1 \u1074\'E2\u1086\'EE\u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1086\'EE\u1084\'EC: \u8220\'93\u1058\'D2\u1099\'FB \u1075\'E3\u1076\'E4\u1077\'E5?\u8221\'94 \hich\f0 \emdash \loch\f0  \u1080\'E8 \u1079\'E7\u1074\'E2\u1072\'E0\u1083\'EB\u1072\'E0 \u1076\'E4\u1086\'EE\u1084\'EC\u1086\'EE\u1081\'E9. \u1057\'D1\u1088\'F0\u1072\'E0\u1079\'E7\u1091\'F3 \u1087\'EF\u1086\'EE\u1089\'F1\u1083\'EB\u1077\'E5 \u1079\'E7\u1074\'E2\u1086\'EE\u1085\'ED\u1082\'EA\u1072\'E0 \u1084\'EC\u1099\'FB \u1089\'F1\u1072\'E0\u1076\'E4\u1080\'E8\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1085\'ED\u1072\'E0 \u1074\'E2\u1077\'E5\u1083\'EB\u1080\'E8\u1082\'EA\u1080\'E8 \u1080\'E8 \u1086\'EE\u1090\'F2\u1087\'EF\u1088\'F0\u1072\'E0\u1074\'E2\u1083\'EB\u1103\'FF\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1074\'E2 \u1087\'EF\u1091\'F3\u1090\'F2\u1100\'FC, \u1074\'E2\u1085\'ED\u1080\'E8\u1084\'EC\u1072\'E0\u1090\'F2\u1077\'E5\u1083\'EB\u1100\'FC\u1085\'ED\u1086\'EE \u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1103\'FF \u1074\'E2\u1087\'EF\u1077\'E5\u1088\'F0\u1077\'E5\u1076\'E4.}
\par\plain \f0\fs24\b0\i0
\par\plain {\f0\fs24\b0\i0 \loch\af0\hich\af0\dbch\af0\uc1\u1043\'C3\u1086\'EE\u1088\'F0\u1086\'EE\u1076\'E4 \u1079\'E7\u1072\'E0\u1089\'F1\u1099\'FB\u1087\'EF\u1072\'E0\u1083\'EB, \u1072\'E0 \u1084\'EC\u1072\'E0\u1092\'F4\u1080\'E8\u1103\'FF \u1085\'ED\u1077\'E5 \u1087\'EF\u1088\'F0\u1086\'EE\u1089\'F1\u1099\'FB\u1087\'EF\u1072\'E0\u1083\'EB\u1072\'E0\u1089\'F1\u1100\'FC. \u1045\'C5\u1097\'F9\u1077\'E5 \u1087\'EF\u1072\'E0\u1088\'F0\u1091\'F3 \u1095\'F7\u1072\'E0\u1089\'F1\u1086\'EE\u1074\'E2 \hich\f0 \emdash \loch\f0  \u1080\'E8 \u1089\'F1\u1074\'E2\u1077\'E5\u1090\'F2\u1086\'EE\u1092\'F4\u1086\'EE\u1088\'F0\u1099\'FB \u1073\'E1\u1091\'F3\u1076\'E4\u1091\'F3\u1090\'F2 \u1090\'F2\u1086\'EE\u1089\'F1\u1082\'EA\u1083\'EB\u1080\'E8\u1074\'E2\u1086\'EE \u1084\'EC\u1080\'E8\u1075\'E3\u1072\'E0\u1090\'F2\u1100\'FC \u1078\'E6\u1077\'E5\u1083\'EB\u1090\'F2\u1099\'FB\u1084\'EC \u1074\'E2 \u1086\'EE\u1078\'E6\u1080\'E8\u1076\'E4\u1072\'E0\u1085\'ED\u1080\'E8\u1080\'E8 \u1088\'F0\u1072\'E0\u1089\'F1\u1089\'F1\u1074\'E2\u1077\'E5\u1090\'F2\u1072\'E0. \u1040\'C0 \u1087\'EF\u1086\'EE\u1082\'EA\u1072\'E0, \u1088\'F0\u1072\'E0\u1073\'E1\u1086\'EE\u1095\'F7\u1080\'E8\u1077\'E5 \u1074\'E2\u1086\'EE\u1079\'E7\u1074\'E2\u1088\'F0\u1072\'E0\u1097\'F9\u1072\'E0\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1089\'F1 \u1083\'EB\u1072\'E0\u1084\'EC\u1087\'EF\u1086\'EE\u1074\'E2\u1086\'EE\u1075\'E3\u1086\'EE \u1079\'E7\u1072\'E0\u1074\'E2\u1086\'EE\u1076\'E4\u1072\'E0 \u1080\'E8 \u1076\'E4\u1086\'EE \u1085\'ED\u1072\'E0\u1089\'F1 \u1089\'F1 \u1050\'CA\u1072\'E0\u1090\'F2\u1077\'E5\u1081\'E9 \u1076\'E4\u1086\'EE\u1085\'ED\u1086\'EE\u1089\'F1\u1080\'E8\u1083\'EB\u1080\'E8\u1089\'F1\u1100\'FC \u1086\'EE\u1090\'F2\u1088\'F0\u1099\'FB\u1074\'E2\u1082\'EA\u1080\'E8 \u1080\'E8\u1093\'F5 \u1076\'E4\u1080\'E8\u1072\'E0\u1083\'EB\u1086\'EE\u1075\'E3\u1086\'EE\u1074\'E2:}
\par\plain {\f0\fs24\b0\i0 \hich\f0 \emdash \loch\f0  \loch\af0\hich\af0\dbch\af0\uc1\u8230\'85\u1047\'C7\u1072\'E0\u1088\'F0\u1087\'EF\u1083\'EB\u1072\'E0\u1090\'F2\u1091\'F3 \u1086\'EE\u1087\'EF\u1103\'FF\u1090\'F2\u1100\'FC \u1079\'E7\u1072\'E0\u1076\'E4\u1077\'E5\u1088\'F0\u1078\'E6\u1072\'E0\u1083\'EB\u1080\'E8, \u1089\'F1\u1091\'F3\u1082\'EA\u1080\'E8!}
\par\plain {\f0\fs24\b0\i0 \hich\f0 \emdash \loch\f0  \loch\af0\hich\af0\dbch\af0\uc1\u8230\'85\u1054\'CE\u1090\'F2\u1076\'E4\u1086\'EE\u1093\'F5\u1085\'ED\u1091\'F3\u1090\'F2\u1100\'FC \u1085\'ED\u1072\'E0\u1076\'E4\u1086\'EE, \u1076\'E4\u1074\'E2\u1077\'E5\u1085\'ED\u1072\'E0\u1076\'E4\u1094\'F6\u1072\'E0\u1090\'F2\u1100\'FC \u1095\'F7\u1072\'E0\u1089\'F1\u1086\'EE\u1074\'E2 \u1089\'F1\u1084\'EC\u1077\'E5\u1085\'ED\u1072\'E0!}
