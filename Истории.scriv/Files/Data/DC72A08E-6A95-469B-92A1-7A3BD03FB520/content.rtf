\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1052\'CC\u1077\'E5\u1085\'ED\u1102\'FE \u1076\'E4\u1083\'EB\u1103\'FF \u1072\'E0\u1083\'EB\u1083\'EB\u1077\'E5\u1088\'F0\u1075\'E3\u1080\'E8\u1082\'EA\u1086\'EE\u1074\'E2, \u1101\'FD\u1090\'F2\u1086\'EE, \u1082\'EA\u1086\'EE\u1085\'ED\u1077\'E5\u1095\'F7\u1085\'ED\u1086\'EE \u1082\'EA\u1088\'F0\u1091\'F3\u1090\'F2\u1086\'EE, \hich\f1 \emdash \loch\f1  \u1086\'EE\u1090\'F2\u1074\'E2\u1077\'E5\u1090\'F2\u1080\'E8\u1083\'EB \u1044\'C4\u1077\'E5\u1085\'ED, \hich\f1 \emdash \loch\f1  \u1085\'ED\u1086\'EE \u1087\'EF\u1086\'EE\u1076\'E4\u1086\'EE\u1073\'E1\u1085\'ED\u1099\'FB\u1077\'E5 \u1087\'EF\u1083\'EB\u1102\'FE\u1096\'F8\u1082\'EA\u1080\'E8 \u1077\'E5\u1089\'F1\u1090\'F2\u1100\'FC \u1080\'E8 \u1091\'F3 \u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3\u1080\'E8\u1093\'F5 \u1082\'EA\u1086\'EE\u1084\'EC\u1087\'EF\u1072\'E0\u1085\'ED\u1080\'E8\u1081\'E9\u8230\'85}
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1042\'C2 \u1076\'E4\u1088\'F0\u1091\'F3\u1075\'E3\u1080\'E8\u1077\'E5 \u1082\'EA\u1086\'EE\u1084\'EC\u1087\'EF\u1072\'E0\u1085\'ED\u1080\'E8\u1080\'E8 \u1074\'E2\u1072\'E0\u1089\'F1 \u1085\'ED\u1077\'E5 \u1087\'EF\u1088\'F0\u1080\'E8\u1075\'E3\u1083\'EB\u1072\'E0\u1089\'F1\u1080\'E8\u1083\'EB\u1080\'E8. \hich\f1 \emdash \loch\f1  \u1088\'F0\u1077\'E5\u1079\'E7\u1082\'EA\u1086\'EE \u1086\'EE\u1090\'F2\u1088\'F0\u1077\'E5\u1079\'E7\u1072\'E0\u1083\'EB\u1072\'E0 \u1076\'E4\u1077\'E5\u1074\'E2\u1091\'F3\u1096\'F8\u1082\'EA\u1072\'E0. \hich\f1 \emdash \loch\f1  \u1042\'C2\u1072\'E0\u1084\'EC \u1087\'EF\u1080\'E8\u1089\'F1\u1100\'FC\u1084\'EC\u1086\'EE \u1080\'E8\u1079\'E7 \u1070\'DE\u1085\'ED\u1080\'E8\u1090\'F2\u1072\'E0 \u1087\'EF\u1088\'F0\u1080\'E8\u1093\'F5\u1086\'EE\u1076\'E4\u1080\'E8\u1083\'EB\u1086\'EE? \u1040\'C0 \u1080\'E8\u1079\'E7 Zet Mind?}
