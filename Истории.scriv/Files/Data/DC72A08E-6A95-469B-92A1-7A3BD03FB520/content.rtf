\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1069\'DD\u1081\'E9! \hich\f1 \emdash \loch\f1  \u1082\'EA\u1088\'F0\u1080\'E8\u1082\'EA\u1085\'ED\u1091\'F3\u1083\'EB\u1072\'E0 \u1082\'EA\u1072\'E0\u1082\'EA\u1072\'E0\u1103\'FF-\u1090\'F2\u1086\'EE \u1076\'E4\u1077\'E5\u1074\'E2\u1091\'F3\u1096\'F8\u1082\'EA\u1072\'E0 \u1074\'E2 \u1073\'E1\u1080\'E8\u1088\'F0\u1102\'FE\u1079\'E7\u1086\'EE\u1074\'E2\u1086\'EE\u1081\'E9 \u1091\'F3\u1085\'ED\u1080\'E8\u1092\'F4\u1086\'EE\u1088\'F0\u1084\'EC\u1077\'E5, \u1087\'EF\u1086\'EE\u1076\'E4\u1085\'ED\u1080\'E8\u1084\'EC\u1072\'E0\u1103\'FF \u1087\'EF\u1072\'E0\u1087\'EF\u1082\'EA\u1091\'F3 \u1074\'E2 \u1088\'F0\u1091\'F3\u1082\'EA\u1077\'E5, \hich\f1 \emdash \loch\f1  \u1059\'D3 \u1084\'EC\u1077\'E5\u1085\'ED\u1103\'FF \u1090\'F2\u1091\'F3\u1090\'F2 \u1080\'E8\u1076\'E4\u1077\'E5\u1081\'E9\u1082\'EA\u1072\'E0 \u1085\'ED\u1072\'E0\u1082\'EA\u1083\'EB\u1102\'FE\u1085\'ED\u1091\'F3\u1083\'EB\u1072\'E0\u1089\'F1\u1100\'FC!}
\par\plain {\f1\fs24\b0\i0 \loch\af1\hich\af1\dbch\af1\uc1\u1063\'D7\u1077\'E5\u1088\'F0\u1077\'E5\u1079\'E7 \u1089\'F1\u1077\'E5\u1082\'EA\u1091\'F3\u1085\'ED\u1076\'E4\u1091\'F3 \u1085\'ED\u1072\'E0 \u1076\'E4\u1077\'E5\u1074\'E2\u1091\'F3\u1096\'F8\u1082\'EA\u1091\'F3 \u1089\'F1\u1084\'EC\u1086\'EE\u1090\'F2\u1088\'F0\u1077\'E5\u1083\'EB \u1074\'E2\u1077\'E5\u1089\'F1\u1100\'FC \u1086\'EE\u1090\'F2\u1076\'E4\u1077\'E5\u1083\'EB.}
\par\plain {\f1\fs24\b0\i0 \hich\f1 \emdash \loch\f1  \loch\af1\hich\af1\dbch\af1\uc1\u1054\'CE\u1076\'E4\u1085\'ED\u1072\'E0 \u1080\'E8\u1079\'E7 \u1089\'F1\u1090\'F2\u1072\'E0\u1088\'F0\u1099\'FB\u1093\'F5 \u1089\'F1\u1090\'F2\u1088\'F0\u1072\'E0\u1090\'F2\u1077\'E5\u1075\'E3\u1080\'E8\u1081\'E9 \u1089\'F1\u1085\'ED\u1086\'EE\u1074\'E2\u1072\'E0 \u1072\'E0\u1082\'EA\u1090\'F2\u1091\'F3\u1072\'E0\u1083\'EB\u1100\'FC\u1085\'ED\u1072\'E0. \hich\f1 \emdash \loch\f1  \u1076\'E4\u1086\'EE\u1087\'EF\u1086\'EE\u1083\'EB\u1085\'ED\u1080\'E8\u1083\'EB\u1072\'E0 \u1086\'EE\u1085\'ED\u1072\'E0.}
